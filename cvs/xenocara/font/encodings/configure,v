# mkfontscale

	# Extract the first word of "mkfontscale", so it can be a program name with args.
set dummy mkfontscale; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_MKFONTSCALE+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $MKFONTSCALE in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_MKFONTSCALE="$MKFONTSCALE" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_MKFONTSCALE="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
MKFONTSCALE=$ac_cv_path_MKFONTSCALE
if test -n "$MKFONTSCALE"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MKFONTSCALE" >&5
$as_echo "$MKFONTSCALE" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


	if test x"$MKFONTSCALE" = x; then
		as_fn_error $? "mkfontscale is required to build $PACKAGE_NAME." "$LINENO" 5
	fi


