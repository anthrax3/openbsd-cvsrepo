		{ $as_echo "$as_me:$LINENO: checking if $CC supports-Werror=main" >&5
$as_echo_n "checking if $CC supports-Werror=main... " >&6; }
		cacheid=xorg_cv_cc_flag__Werror_main
		if { as_var=$cacheid; eval "test \"\${$as_var+set}\" = set"; }; then
  $as_echo_n "(cached) " >&6
