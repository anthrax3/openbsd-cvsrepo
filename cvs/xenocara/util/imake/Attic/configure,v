		{ $as_echo "$as_me:$LINENO: result: $supported" >&5
$as_echo "$supported" >&6; }
		if test "$supported" = "yes" ; then
			BASE_CFLAGS="$BASE_CFLAGS -Werror=main"
			found="yes"
		fi
	fi















