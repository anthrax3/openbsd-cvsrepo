		dix-config.h xorg-config.h xkb-config.h xwin-config.h \
		kdrive-config.h version-config.h
installdirs:
	for dir in "$(DESTDIR)$(sdkdir)" "$(DESTDIR)$(sdkdir)"; do \
	  test -z "$$dir" || $(MKDIR_P) "$$dir"; \
	done
