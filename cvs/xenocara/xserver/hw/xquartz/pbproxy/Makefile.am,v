@Update to xserver 1.11.2
@
text
@AM_CPPFLAGS=-F/System/Library/Frameworks/ApplicationServices.framework/Frameworks \
	-DBUNDLE_ID_PREFIX=\"$(BUNDLE_ID_PREFIX)\"

AM_CFLAGS=$(XPBPROXY_CFLAGS) 
AM_OBJCFLAGS=$(XPBPROXY_CFLAGS)

