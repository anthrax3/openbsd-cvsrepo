#ifdef HAVE_XORG_CONFIG_H
#include <xorg-config.h>
#endif

#include <X11/X.h>

#include "xf86.h"
#include "xf86Priv.h"
#include "xf86_OSlib.h"

/*
 * No-op functions for OSs without VTs
 */

Bool
xf86VTSwitchPending(void)
{
    return FALSE;
}

Bool
xf86VTSwitchAway(void)
{
    return FALSE;
}

Bool
xf86VTSwitchTo(void)
{
    return TRUE;
}

Bool
xf86VTActivate(int vtno)
{
    return TRUE;
}
