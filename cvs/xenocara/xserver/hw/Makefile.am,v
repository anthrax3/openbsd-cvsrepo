1.5
log
@Update to xserver 1.16.1.

Tested by naddy@@, jsg@@ & kettenis@@
@
text
@if DMX
DMX_SUBDIRS = dmx
endif

if XORG
XORG_SUBDIRS = xfree86
endif

if XVFB
XVFB_SUBDIRS = vfb
endif

if XNEST
XNEST_SUBDIRS = xnest
endif

if XWIN
XWIN_SUBDIRS = xwin
endif

if KDRIVE
KDRIVE_SUBDIRS = kdrive
endif

if XQUARTZ
XQUARTZ_SUBDIRS = xquartz
endif

if XWAYLAND
XWAYLAND_SUBDIRS = xwayland
endif

SUBDIRS =			\
	$(XORG_SUBDIRS)		\
	$(XWIN_SUBDIRS)		\
	$(XVFB_SUBDIRS)		\
	$(XNEST_SUBDIRS)	\
	$(DMX_SUBDIRS)		\
	$(KDRIVE_SUBDIRS)	\
	$(XQUARTZ_SUBDIRS)	\
	$(XWAYLAND_SUBDIRS)

DIST_SUBDIRS = dmx xfree86 vfb xnest xwin xquartz kdrive xwayland

relink:
	$(AM_V_at)for i in $(SUBDIRS) ; do $(MAKE) -C $$i relink || exit 1 ; done
@


