#include <stdarg.h>
#include <stdio.h>

#if 1
int
winDebug(const char *format, ...)
{
  int count;
  va_list ap;
  va_start(ap, format);
  count = fprintf(stderr, "xwinclip: ");
  count += vfprintf(stderr, format, ap);
  va_end(ap);
  return count;
}
#endif

int
ErrorF(const char *format, ...)
{
  int count;
  va_list ap;
  va_start(ap, format);
  count = vfprintf(stderr, format, ap);
  va_end(ap);
  return count;
}
