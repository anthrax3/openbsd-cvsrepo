	@@rm -f $@@
	$(AM_V_GEN)$(MAKE_TEXT) $<

.sgml.ps:
	@@rm -f $@@
	$(AM_V_GEN)$(MAKE_PS) $<

.ps.pdf:
	@@rm -f $@@
	$(AM_V_GEN)$(MAKE_PDF) $<

.sgml.html:
	@@rm -f $@@
	$(AM_V_GEN)$(MAKE_HTML) $<
d26 2
a27 2
