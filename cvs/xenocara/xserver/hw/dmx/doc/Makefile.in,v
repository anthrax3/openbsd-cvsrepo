@@ENABLE_DEVEL_DOCS_TRUE@@@@HAVE_FOP_TRUE@@@@HAVE_STYLESHEETS_TRUE@@@@HAVE_XMLTO_TRUE@@	-x $(STYLESHEET_SRCDIR)/xorg-fo.xsl

