typedef struct {
    Bool	(*RealizeCursor)(
		ScreenPtr /*pScreen*/,
		CursorPtr /*pCursor*/
);
    Bool	(*UnrealizeCursor)(
		ScreenPtr /*pScreen*/,
		CursorPtr /*pCursor*/
);
    Bool	(*PutUpCursor)(
                DeviceIntPtr /*pDev*/,
		ScreenPtr /*pScreen*/,
		CursorPtr /*pCursor*/,
		int /*x*/,
		int /*y*/,
		unsigned long /*source*/,
		unsigned long /*mask*/
);
    Bool	(*SaveUnderCursor)(
                DeviceIntPtr /*pDev*/,
		ScreenPtr /*pScreen*/,
		int /*x*/,
		int /*y*/,
		int /*w*/,
		int /*h*/
);
    Bool	(*RestoreUnderCursor)(
                DeviceIntPtr /*pDev*/,
		ScreenPtr /*pScreen*/,
		int /*x*/,
		int /*y*/,
		int /*w*/,
		int /*h*/
);
    Bool	(*MoveCursor)(
                DeviceIntPtr /*pDev*/,
		ScreenPtr /*pScreen*/,
		CursorPtr /*pCursor*/,
		int /*x*/,
		int /*y*/,
		int /*w*/,
		int /*h*/,
		int /*dx*/,
		int /*dy*/,
		unsigned long /*source*/,
		unsigned long /*mask*/
);
    Bool	(*ChangeSave)(
                DeviceIntPtr /*pDev*/,
		ScreenPtr /*pScreen*/,
		int /*x*/,
		int /*y*/,
		int /*w*/,
		int /*h*/,
		int /*dx*/,
		int /*dy*/
);
    Bool	(*DeviceCursorInitialize)(
                DeviceIntPtr /*pDev*/,
		ScreenPtr /*pScreen*/
);
    void	(*DeviceCursorCleanup)(
                DeviceIntPtr /*pDev*/,
		ScreenPtr /*pScreen*/
);

} miSpriteCursorFuncRec, *miSpriteCursorFuncPtr;

