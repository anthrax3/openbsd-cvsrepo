<category name="GL_ARB_transpose_matrix" number="3">
    <enum name="TRANSPOSE_MODELVIEW_MATRIX_ARB"           value="0x84E3"/>
    <enum name="TRANSPOSE_PROJECTION_MATRIX_ARB"          value="0x84E4"/>
