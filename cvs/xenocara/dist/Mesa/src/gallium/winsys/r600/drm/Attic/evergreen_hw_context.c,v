	/* initialize hash */
	ctx->hash_size = 19;
	ctx->hash_shift = 11;
	for (int i = 0; i < 256; i++) {
		ctx->range[i].start_offset = i << ctx->hash_shift;
		ctx->range[i].end_offset = ((i + 1) << ctx->hash_shift) - 1;
		ctx->range[i].blocks = calloc(1 << ctx->hash_shift, sizeof(void*));
