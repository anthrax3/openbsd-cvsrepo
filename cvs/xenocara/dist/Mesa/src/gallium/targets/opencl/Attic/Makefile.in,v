@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	$(AM_V_GEN)$(MKDIR_P) $(top_builddir)/$(LIB_DIR);	\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	link_dir=$(top_builddir)/$(LIB_DIR)/gallium;		\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	if test x$(egl_LTLIBRARIES) != x; then			\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@		link_dir=$(top_builddir)/$(LIB_DIR)/egl;	\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	fi;							\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	$(MKDIR_P) $$link_dir;					\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	file_list=$(dri_LTLIBRARIES:%.la=.libs/%.so);		\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	file_list+=$(vdpau_LTLIBRARIES:%.la=.libs/%.$(LIB_EXT)*); \
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	file_list+=$(egl_LTLIBRARIES:%.la=.libs/%.$(LIB_EXT)*);	\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	file_list+=$(lib_LTLIBRARIES:%.la=.libs/%.$(LIB_EXT)*);	\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	for f in $$file_list; do 				\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@		if test -h .libs/$$f; then			\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@			cp -d $$f $$link_dir;			\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@		else						\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@			ln -f $$f $$link_dir;			\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@		fi;						\
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	done && touch $@@
