	$(AM_V_CXXLD)$(lib@@OSMESA_LIB@@_la_LINK) -rpath $(libdir) $(lib@@OSMESA_LIB@@_la_OBJECTS) $(lib@@OSMESA_LIB@@_la_LIBADD) $(LIBS)

