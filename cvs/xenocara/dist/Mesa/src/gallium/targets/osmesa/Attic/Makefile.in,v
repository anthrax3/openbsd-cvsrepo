# Provide compatibility with scripts for the old Mesa build system for
# a while by putting a link to the driver into /lib of the build tree.
@@BUILD_SHARED_TRUE@@all-local: lib@@OSMESA_LIB@@.la
@@BUILD_SHARED_TRUE@@	$(MKDIR_P) $(top_builddir)/$(LIB_DIR);
@@BUILD_SHARED_TRUE@@	$(MKDIR_P) $(top_builddir)/$(LIB_DIR)/gallium;
@@BUILD_SHARED_TRUE@@	ln -f .libs/lib@@OSMESA_LIB@@.so $(top_builddir)/$(LIB_DIR)/gallium/lib@@OSMESA_LIB@@.so;
@@BUILD_SHARED_TRUE@@	ln -f .libs/lib@@OSMESA_LIB@@.so.@@OSMESA_VERSION@@ $(top_builddir)/$(LIB_DIR)/gallium/lib@@OSMESA_LIB@@.so.@@OSMESA_VERSION@@;
@@BUILD_SHARED_TRUE@@	ln -f .libs/lib@@OSMESA_LIB@@.so.@@OSMESA_VERSION@@.0.0 $(top_builddir)/$(LIB_DIR)/gallium/

