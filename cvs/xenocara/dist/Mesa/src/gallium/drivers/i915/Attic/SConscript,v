1.7
log
@remove the now unused Mesa 10.2.9 code
@
text
@Import('*')

env = env.Clone()

i915 = env.ConvenienceLibrary(
	target = 'i915',
	source = env.ParseSourceList('Makefile.sources', 'C_SOURCES')
	)

env.Alias('i915', i915)

Export('i915')
@


