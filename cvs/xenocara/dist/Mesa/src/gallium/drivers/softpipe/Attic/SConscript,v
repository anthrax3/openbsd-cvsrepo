1.5
log
@remove the now unused Mesa 10.2.9 code
@
text
@Import('*')

env = env.Clone()


softpipe = env.ConvenienceLibrary(
	target = 'softpipe',
	source = env.ParseSourceList('Makefile.sources', 'C_SOURCES')
	)

env.Alias('softpipe', softpipe)

Export('softpipe')
@


