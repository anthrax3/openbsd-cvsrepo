struct IA2VS
{
	float4 position : POSITION;
	float4 color : COLOR;
};

struct VS2PS
{
	float4 position : SV_POSITION;
	float4 color : COLOR;
};

VS2PS vs(IA2VS input)
{
	VS2PS result;
	result.position = input.position;
	result.color = input.color;
	return result;
}

float4 ps(VS2PS input) : SV_TARGET
{
	return input.color;
}
@


