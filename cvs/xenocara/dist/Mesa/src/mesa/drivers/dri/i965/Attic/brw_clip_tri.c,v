   GLuint clipvert_offset = brw_clip_have_varying(c, VARYING_SLOT_CLIP_VERTEX)
      ? brw_varying_to_offset(&c->vue_map, VARYING_SLOT_CLIP_VERTEX)
      : hpos_offset;

