static void* radeon_alloc_verts( r100ContextPtr rmesa , GLuint nr, GLuint size )
{
   void *rv;
   do {
     radeon_predict_emit_size( rmesa );
     rv = rcommonAllocDmaLowVerts( &rmesa->radeon, nr, size );
   } while (!rv);
   return rv;
}

