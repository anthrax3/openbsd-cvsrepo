/**
 * \file pixelstore.h
 * glPixelStore functions.
 */


#ifndef PIXELSTORE_H
#define PIXELSTORE_H


#include "glheader.h"

struct gl_context;


extern void GLAPIENTRY
_mesa_PixelStorei( GLenum pname, GLint param );


extern void GLAPIENTRY
_mesa_PixelStoref( GLenum pname, GLfloat param );


extern void
_mesa_init_pixelstore( struct gl_context *ctx );


#endif
