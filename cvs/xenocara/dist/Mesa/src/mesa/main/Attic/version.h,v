#ifndef VERSION_H
#define VERSION_H


struct gl_context;


extern void
_mesa_compute_version(struct gl_context *ctx);

extern void
_mesa_override_gl_version(struct gl_context *ctx);

extern void
_mesa_override_glsl_version(struct gl_context *ctx);

extern int
_mesa_get_gl_version_override(void);

#endif /* VERSION_H */
@


