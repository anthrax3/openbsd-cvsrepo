((declare (out) float a) (declare (out) float b)
 (function main
  (signature void (parameters)
   ((loop
     ((assign (x) (var_ref a) (constant float (1.000000))) (return)))
    (assign (x) (var_ref b) (constant float (2.000000)))))))
EOF
