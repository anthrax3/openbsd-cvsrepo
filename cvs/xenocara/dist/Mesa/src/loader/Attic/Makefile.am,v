	$(DEFINES) \
	-I$(top_srcdir)/include \
	$(VISIBILITY_CFLAGS) \
	$(LIBUDEV_CFLAGS)

if !HAVE_LIBDRM
libloader_la_CPPFLAGS += \
	-D__NOT_HAVE_DRM_H
else
libloader_la_CPPFLAGS += \
	$(LIBDRM_CFLAGS)

libloader_la_LIBADD = \
	$(LIBDRM_LIBS)
endif

libloader_la_SOURCES = $(LOADER_C_FILES)
@


