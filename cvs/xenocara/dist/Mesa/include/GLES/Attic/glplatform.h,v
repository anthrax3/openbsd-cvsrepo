/*-------------------------------------------------------------------------
 * Definition of GL_API and GL_APIENTRY
 *-----------------------------------------------------------------------*/

#ifdef _WIN32
#   ifdef __GL_EXPORTS
#       define GL_API __declspec(dllexport)
#   else
#       define GL_API __declspec(dllimport)
#   endif
#   ifdef UNDER_CE
#       define GL_APIENTRY
#   else
#       define GL_APIENTRY __stdcall
#   endif
#else
#   ifdef __GL_EXPORTS
#       define GL_API
#   else
#       define GL_API extern
#   endif
#   define GL_APIENTRY
#endif
d20 1
d22 2
a23 2
#ifndef APIENTRY
#   define APIENTRY GL_APIENTRY
d26 2
a27 2
#ifdef __cplusplus
}
