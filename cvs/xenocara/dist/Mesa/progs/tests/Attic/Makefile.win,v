##### MACROS #####

TOP    = ..
INCDIR = ..\include
LIBDIR = ..\lib

SRCS = cva.c \
	dinoshade.c \
	fogcoord.c \
	manytex.c \
	multipal.c \
	projtex.c \
	seccolor.c \
#	sharedtex.c \
	texline.c \
	texwrap.c \
