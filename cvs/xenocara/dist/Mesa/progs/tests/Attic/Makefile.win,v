	vptest2.c \
	vptest3.c \
	vptorus.c \
	vpwarpmesh.c
	
!include "../mesawin32.mak"

##### TARGETS #####

clean::

clobber::
	@@del readtex.c readtex.h

$(EXES) :$*.obj
	@@echo $@@
	$(link) -out:$@@ $* /LIBPATH:$(LIBDIR) $(LIBS)
