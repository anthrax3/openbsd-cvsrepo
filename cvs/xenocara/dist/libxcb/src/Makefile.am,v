		-s "$(LIB_MAN_SUFFIX)" -p $(XCBPROTO_XCBPYTHONDIR) \
		$(C_CLIENT_PY_EXTRA_ARGS) \
		$(XCBPROTO_XCBINCLUDEDIR)/$(@@:.c=.xml)

$(BUILT_MAN_PAGES): $(EXTSOURCES)
