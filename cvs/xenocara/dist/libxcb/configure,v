  enableval=$enable_xfree86_dri; BUILD_XFREE86_DRI=$enableval
else
  BUILD_XFREE86_DRI=yes
fi



 if test "x$BUILD_XFREE86_DRI" = "xyes"; then
  BUILD_XFREE86_DRI_TRUE=
  BUILD_XFREE86_DRI_FALSE='#'
else
  BUILD_XFREE86_DRI_TRUE='#'
  BUILD_XFREE86_DRI_FALSE=
fi



