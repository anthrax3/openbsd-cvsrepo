if test -z "${XCB_SERVERSIDE_SUPPORT_TRUE}" && test -z "${XCB_SERVERSIDE_SUPPORT_FALSE}"; then
  as_fn_error $? "conditional \"XCB_SERVERSIDE_SUPPORT\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
