 if test "x$BUILD_XINERAMA" = "xyes"; then
  BUILD_XINERAMA_TRUE=
  BUILD_XINERAMA_FALSE='#'
else
  BUILD_XINERAMA_TRUE='#'
  BUILD_XINERAMA_FALSE=
