@
text
@# Common rules for building *.dir files in all xkb subdirectories
# Replaces Imake's MakeXkbDir() rule
# svu: taken from xkbdata

dist_dir_DATA = $(subdir).dir

dirdir = $(xkb_base)

$(subdir).dir: $(dir_data)
	-rm -f $@@
	$(XKBCOMP) -lfhlpR -o $@@ '*'

CLEANFILES = $(subdir).dir

@


