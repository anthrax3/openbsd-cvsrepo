	key <AB08> { [    Cyrillic_be,    Cyrillic_BE                                 ] }; // б Б
	key <AB09> { [    Cyrillic_yu,    Cyrillic_YU                                 ] }; // ю Ю
	key <AB10> { [         period,          comma                                 ] }; // . ,
	key <AC01> { [    Cyrillic_ef,    Cyrillic_EF                                 ] }; // ф Ф
	key <AC02> { [  Cyrillic_yeru,  Cyrillic_YERU                                 ] }; // ы Ы
