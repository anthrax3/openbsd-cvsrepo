	key <AD10> { [ asterisk,	U2363	] };	// asterisk, star diaeresis
	key <AD11> { [ U2190,		U235e	] };	// left arrow, quote quad
	key <AD12> { [ U2192,		U236c	] };	// right arrow, zilde
	key <AE01> { [ diaeresis,	U2336	] };	// i-beam
	key <AE02> { [ macron,		U236B	] };	// deltilde
