	xkb_symbols "dvorak" {
	include "no(dvorak)"
};

partial alphanumeric_keys
	xkb_symbols "smi" {
	include "no(smi)"
};

partial alphanumeric_keys
	xkb_symbols "smi_nodeadkeys" {
	include "no(smi_nodeadkeys)"
};

partial alphanumeric_keys
	xkb_symbols "mac" {
	include "no(mac)"
};

