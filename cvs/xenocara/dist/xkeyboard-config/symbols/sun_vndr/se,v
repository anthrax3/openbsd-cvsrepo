    key <ALGR> { [      Mode_switch     ]       };
    key <LMTA> { [      Meta_L          ]       };
    key <RMTA> { [      Meta_R          ]       };
    key <COMP> { [      Multi_key       ]       };
    // End alphanumeric section

    // Begin "Function" section
    key <FK01> { [ F1           ]                       };
    key <FK02> { [ F2           ]                       };
    key <FK03> { [ F3           ]                       };
    key <FK04> { [ F4           ]                       };
    key <FK05> { [ F5           ]                       };
