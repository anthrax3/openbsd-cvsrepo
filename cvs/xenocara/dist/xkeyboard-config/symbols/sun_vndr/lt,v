    key <AB08>  { [     comma,       less,	ampersand,     multiply ] };
    key <AB09>  { [    period,    greater,	periodcentered,   division ] };
    key <AB10>  { [     slash,   question,	dead_belowdot, dead_abovedot ] };

	include "level3(ralt_switch)"
};

