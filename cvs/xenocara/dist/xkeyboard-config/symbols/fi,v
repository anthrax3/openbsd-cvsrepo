    key <BKSL> { [  asterisk,        slash                                                          ] };
    key <LSGT> { [  f,               F                                                              ] };
    key <AB01> { [  x,               X                                                              ] };
    key <AB02> { [  d,               D                                                              ] };
    key <AB03> { [  m,               M,                mu                                           ] };
    key <AB04> { [  j,               J                                                              ] };
