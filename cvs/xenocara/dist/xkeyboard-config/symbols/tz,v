    key <AD08> {  [  Arabic_kasra,      Arabic_hamzaonyeh      ]  };
    key <AD09> {  [  Arabic_sukun,      0x1000657              ]  };
    key <AD10> {  [  Arabic_peh,        Arabic_tehmarbuta      ]  };
    key <AD12> {  [  VoidSymbol,        VoidSymbol             ]  };

    key <AC01> {  [  Arabic_fatha,      Arabic_alef            ]  };
