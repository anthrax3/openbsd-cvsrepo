xkb_symbols "nicola_f_bs" {
    key <BKSP> {
	type="",
	symbols[Group1]= [ bracketright, braceright ]
    };
    key <AE10> { [ 0, underscore        ] };
    key <AD11> { [ colon, asterisk      ] };
