// Copied from macintosh_vndr/jp
partial alphanumeric_keys
xkb_symbols "mac" {
    include "jp(kana)"
    name[Group1]= "Japanese (Macintosh)";

    replace key <CAPS> { [ Caps_Lock ] };
};

partial alphanumeric_keys
xkb_symbols "hztg_escape" {
    replace key <HZTG> { [ Escape ] };
};

