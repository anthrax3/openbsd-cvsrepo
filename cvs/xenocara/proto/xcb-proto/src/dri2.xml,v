      <field type="CARD32" name="sbc_lo" />
    </reply>
  </request>

  <request name="WaitSBC" opcode="11">
    <field type="DRAWABLE" name="drawable" />
    <field type="CARD32" name="target_sbc_hi" />
    <field type="CARD32" name="target_sbc_lo" />
    <reply>
      <pad bytes="1" />
