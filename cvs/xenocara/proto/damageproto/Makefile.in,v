	for dir in "$(DESTDIR)$(damagedocdir)" "$(DESTDIR)$(pkgconfigdir)" "$(DESTDIR)$(damagedir)"; do \
	  test -z "$$dir" || $(mkdir_p) "$$dir"; \
	done
