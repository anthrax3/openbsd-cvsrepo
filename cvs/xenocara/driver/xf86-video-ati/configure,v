else
  pkg_failed=yes
fi
    fi
else
	pkg_failed=untried
fi



if test $pkg_failed = yes; then

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        LIBDRM_RADEON_PKG_ERRORS=`$PKG_CONFIG --short-errors --errors-to-stdout --print-errors "xorg-server >= 1.6.2 libdrm_radeon"`
        else
	        LIBDRM_RADEON_PKG_ERRORS=`$PKG_CONFIG --errors-to-stdout --print-errors "xorg-server >= 1.6.2 libdrm_radeon"`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$LIBDRM_RADEON_PKG_ERRORS" >&5

	{ $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
                LIBDRM_RADEON=no
elif test $pkg_failed = untried; then
	LIBDRM_RADEON=no
else
	LIBDRM_RADEON_CFLAGS=$pkg_cv_LIBDRM_RADEON_CFLAGS
