  pkg_failed=yes
fi
    fi
else
	pkg_failed=untried
fi
if test -n "$PKG_CONFIG"; then
    if test -n "$XORG_LIBS"; then
        pkg_cv_XORG_LIBS="$XORG_LIBS"
