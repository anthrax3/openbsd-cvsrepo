    conftest$ac_exeext conftest.$ac_ext
fi


		CFLAGS="$xorg_testset_save_CFLAGS"

		eval supported=\$$cacheid
		{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $supported" >&5
$as_echo "$supported" >&6; }
