	@@$(NORMAL_UNINSTALL)
	@@list='$(@@DRIVER_NAME@@_drv_la_LTLIBRARIES)'; test -n "$(@@DRIVER_NAME@@_drv_ladir)" || list=; \
	for p in $$list; do \
