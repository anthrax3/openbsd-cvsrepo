linux_backend=yes
for ac_header in linux/joystick.h
do :
  ac_fn_c_check_header_mongrel "$LINENO" "linux/joystick.h" "ac_cv_header_linux_joystick_h" "$ac_includes_default"
if test "x$ac_cv_header_linux_joystick_h" = xyes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_LINUX_JOYSTICK_H 1
_ACEOF

else
  linux_backend=no
fi

done

 if test "x$linux_backend" = xyes; then
  LINUX_BACKEND_TRUE=
  LINUX_BACKEND_FALSE='#'
else
  LINUX_BACKEND_TRUE='#'
  LINUX_BACKEND_FALSE=
fi

if test "x$linux_backend" = xyes; then

$as_echo "#define LINUX_BACKEND 1" >>confdefs.h

fi

bsd_backend=yes
for ac_header in dev/usb/usb.h
do :
  ac_fn_c_check_header_mongrel "$LINENO" "dev/usb/usb.h" "ac_cv_header_dev_usb_usb_h" "$ac_includes_default"
if test "x$ac_cv_header_dev_usb_usb_h" = xyes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_DEV_USB_USB_H 1
_ACEOF

fi

done

for ac_header in usbhid.h dev/usb/usbhid.h
do :
  as_ac_Header=`$as_echo "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "#if HAVE_DEV_USB_USB_H
#include <dev/usb/usb.h>
#endif

"
if eval test \"x\$"$as_ac_Header"\" = x"yes"; then :
  cat >>confdefs.h <<_ACEOF
#define `$as_echo "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF

else
  bsd_backend=no
fi

done

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for hid_get_item in -lusbhid" >&5
$as_echo_n "checking for hid_get_item in -lusbhid... " >&6; }
if ${ac_cv_lib_usbhid_hid_get_item+:} false; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lusbhid  $LIBS"
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
#ifdef __cplusplus
extern "C"
#endif
char hid_get_item ();
int
main ()
{
return hid_get_item ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  ac_cv_lib_usbhid_hid_get_item=yes
else
  ac_cv_lib_usbhid_hid_get_item=no
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_usbhid_hid_get_item" >&5
$as_echo "$ac_cv_lib_usbhid_hid_get_item" >&6; }
if test "x$ac_cv_lib_usbhid_hid_get_item" = xyes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_LIBUSBHID 1
_ACEOF

  LIBS="-lusbhid $LIBS"

else
  bsd_backend=no
fi

 if test "x$bsd_backend" = xyes; then
