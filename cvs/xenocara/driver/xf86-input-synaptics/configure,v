$as_echo "ps2comm alpscomm psmcomm" >&6; }
	BUILD_PS2COMM="yes"
	BUILD_PSMCOMM="yes"
	;;
*solaris* | *gnu*)
	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: ps2comm alpscomm" >&5
$as_echo "ps2comm alpscomm" >&6; }
	BUILD_PS2COMM="yes"
	;;
