	# Put the nasty error message in config.log where it belongs
	echo "$PCIACCESS_PKG_ERRORS" >&5

	as_fn_error $? "Package requirements (pciaccess >= 0.8.0) were not met:

$PCIACCESS_PKG_ERRORS

