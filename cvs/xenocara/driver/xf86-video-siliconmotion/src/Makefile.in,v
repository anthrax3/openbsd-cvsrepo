.c.obj:
@@am__fastdepCC_TRUE@@	$(AM_V_CC)$(COMPILE) -MT $@@ -MD -MP -MF $(DEPDIR)/$*.Tpo -c -o $@@ `$(CYGPATH_W) '$<'`
