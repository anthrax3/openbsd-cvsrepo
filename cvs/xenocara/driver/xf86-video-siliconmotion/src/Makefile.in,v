install-siliconmotion_drv_laLTLIBRARIES: $(siliconmotion_drv_la_LTLIBRARIES)
	@@$(NORMAL_INSTALL)
	@@list='$(siliconmotion_drv_la_LTLIBRARIES)'; test -n "$(siliconmotion_drv_ladir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(MKDIR_P) '$(DESTDIR)$(siliconmotion_drv_ladir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(siliconmotion_drv_ladir)" || exit 1; \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(siliconmotion_drv_ladir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(siliconmotion_drv_ladir)"; \
	}

uninstall-siliconmotion_drv_laLTLIBRARIES:
	@@$(NORMAL_UNINSTALL)
	@@list='$(siliconmotion_drv_la_LTLIBRARIES)'; test -n "$(siliconmotion_drv_ladir)" || list=; \
	for p in $$list; do \
	  $(am__strip_dir) \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(siliconmotion_drv_ladir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(siliconmotion_drv_ladir)/$$f"; \
	done

clean-siliconmotion_drv_laLTLIBRARIES:
	-test -z "$(siliconmotion_drv_la_LTLIBRARIES)" || rm -f $(siliconmotion_drv_la_LTLIBRARIES)
	@@list='$(siliconmotion_drv_la_LTLIBRARIES)'; \
	locs=`for p in $$list; do echo $$p; done | \
	      sed 's|^[^/]*$$|.|; s|/[^/]*$$||; s|$$|/so_locations|' | \
	      sort -u`; \
	test -z "$$locs" || { \
	  echo rm -f $${locs}; \
	  rm -f $${locs}; \
	}
