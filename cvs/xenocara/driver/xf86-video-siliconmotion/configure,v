			STRICT_CFLAGS="$STRICT_CFLAGS -Werror=attributes"
			found="yes"
		fi
	fi



if test "x$STRICT_COMPILE" = "xyes"; then
    BASE_CFLAGS="$BASE_CFLAGS $STRICT_CFLAGS"
