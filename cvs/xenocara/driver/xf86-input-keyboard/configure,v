  freebsd* | kfreebsd-gnu* | netbsd* | openbsd* | dragonfly*)
    IS_BSD="yes"
    ;;

  solaris*)
    IS_SOLARIS="yes"
    ;;

  gnu*)
    IS_HURD="yes"
    ;;

  *)
    as_fn_error $? "Your operating system is not supported by the kbd driver.
                  Contact xorg@@lists.freedesktop.org if you are interested in
                  porting it." "$LINENO" 5
    ;;
esac
case $host_os in
  netbsd*)
    case $host in
      *powerpc-*|*sparc-*|*sparc64-*|*vax-*)
        OS_FLAGS="$OS_FLAGS -DDEFAULT_TO_WSKBD"
        ;;
    esac
    case "$(uname -m 2>&1)" in
      evbarm|sgimips|shark)
        OS_FLAGS="$OS_FLAGS -DDEFAULT_TO_WSKBD"
        ;;
    esac
    ;;
esac


