	if test "x$SNA" != "xno"; then
		accel="sna"
	else
		if test "x$UXA" != "xno"; then
			accel="uxa"
		fi
	fi
	if test "x$accel" = "xauto" -a "x$KMS" = "xyes"; then
		as_fn_error $? "No default acceleration option" "$LINENO" 5
	fi
fi

have_accel="none"
