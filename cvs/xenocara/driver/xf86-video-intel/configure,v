	SAVE_CFLAGS="$CFLAGS"
	CFLAGS="$CFLAGS -I`$PKG_CONFIG --variable=sdkdir xorg-server`"
	cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include "xorg-server.h"
#if !defined XF86DRI
#error XF86DRI not defined
#endif

int
main ()
{

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  _EXT_CHECK=yes
else
  _EXT_CHECK=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
	CFLAGS="$SAVE_CFLAGS"
	{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if XF86DRI is defined" >&5
$as_echo_n "checking if XF86DRI is defined... " >&6; }
	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $_EXT_CHECK" >&5
$as_echo "$_EXT_CHECK" >&6; }
	if test "$_EXT_CHECK" != no; then
		REQUIRED_MODULES="$REQUIRED_MODULES xf86driproto"
	fi

if test "x$_EXT_CHECK" != "xno" -a "x$DRI" != "xno" -a "x$DRI1" != "xno" -a "x$UMS" = "xyes"; then

pkg_failed=no
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for DRI1" >&5
$as_echo_n "checking for DRI1... " >&6; }

if test -n "$DRI1_CFLAGS"; then
    pkg_cv_DRI1_CFLAGS="$DRI1_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
