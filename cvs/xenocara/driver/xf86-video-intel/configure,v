	if test "x$UXA" != "xno"; then

$as_echo "#define DEFAULT_ACCEL_METHOD UXA" >>confdefs.h

		have_accel="yes"
	else
		as_fn_error $? "UXA requested as default, but is not enabled" "$LINENO" 5
	fi
fi

