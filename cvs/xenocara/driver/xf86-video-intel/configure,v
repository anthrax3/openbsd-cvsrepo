	if test "x$SNA" != "xno"; then

$as_echo "#define DEFAULT_ACCEL_METHOD SNA" >>confdefs.h

		have_accel="yes"
	else
		as_fn_error $? "SNA requested as default, but is not enabled" "$LINENO" 5
	fi
fi

