if test -z "${HAVE_DOT_GIT_TRUE}" && test -z "${HAVE_DOT_GIT_FALSE}"; then
  as_fn_error $? "conditional \"HAVE_DOT_GIT\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
