  pkg_cv_DRI1_CFLAGS=`$PKG_CONFIG --cflags "xf86driproto" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
