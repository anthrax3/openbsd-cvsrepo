	if test "x$GLAMOR" != "xno"; then

$as_echo "#define DEFAULT_ACCEL_METHOD GLAMOR" >>confdefs.h

		have_accel="yes"
	else
		as_fn_error $? "glamor acceleration requested as default, but is not enabled" "$LINENO" 5
	fi
fi

if test "x$have_accel" = "xnone"; then
	if test "x$KMS" = "xyes"; then
		if test "x$SNA" != "xno" -o "x$UXA" != "xno"; then

$as_echo "#define DEFAULT_ACCEL_METHOD NOACCEL" >>confdefs.h

		else
			as_fn_error $? "Invalid default acceleration option" "$LINENO" 5
		fi
	fi
	accel="none"
fi
