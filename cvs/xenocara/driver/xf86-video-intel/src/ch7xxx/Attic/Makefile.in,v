install-ch7xxx_laLTLIBRARIES: $(ch7xxx_la_LTLIBRARIES)
	@@$(NORMAL_INSTALL)
	@@list='$(ch7xxx_la_LTLIBRARIES)'; test -n "$(ch7xxx_ladir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(MKDIR_P) '$(DESTDIR)$(ch7xxx_ladir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(ch7xxx_ladir)" || exit 1; \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(ch7xxx_ladir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(ch7xxx_ladir)"; \
	}

uninstall-ch7xxx_laLTLIBRARIES:
	@@$(NORMAL_UNINSTALL)
	@@list='$(ch7xxx_la_LTLIBRARIES)'; test -n "$(ch7xxx_ladir)" || list=; \
	for p in $$list; do \
	  $(am__strip_dir) \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(ch7xxx_ladir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(ch7xxx_ladir)/$$f"; \
	done

clean-ch7xxx_laLTLIBRARIES:
	-test -z "$(ch7xxx_la_LTLIBRARIES)" || rm -f $(ch7xxx_la_LTLIBRARIES)
	@@list='$(ch7xxx_la_LTLIBRARIES)'; \
	locs=`for p in $$list; do echo $$p; done | \
	      sed 's|^[^/]*$$|.|; s|/[^/]*$$||; s|$$|/so_locations|' | \
	      sort -u`; \
	test -z "$$locs" || { \
	  echo rm -f $${locs}; \
	  rm -f $${locs}; \
	}
