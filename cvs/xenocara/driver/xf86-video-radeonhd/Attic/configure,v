   { (exit 1); exit 1; }; }
fi
if test -z "${ATOM_BIOS_PARSER_TRUE}" && test -z "${ATOM_BIOS_PARSER_FALSE}"; then
  { { $as_echo "$as_me:$LINENO: error: conditional \"ATOM_BIOS_PARSER\" was never defined.
