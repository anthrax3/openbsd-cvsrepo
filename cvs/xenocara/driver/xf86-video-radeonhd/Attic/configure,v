if test -z "${ATOM_BIOS_TRUE}" && test -z "${ATOM_BIOS_FALSE}"; then
  { { $as_echo "$as_me:$LINENO: error: conditional \"ATOM_BIOS\" was never defined.
Usually this means the macro was only invoked conditionally." >&5
