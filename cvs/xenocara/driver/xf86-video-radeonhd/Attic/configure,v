$as_echo_n "checking whether $CC supports $w... " >&6; }
  if $CC $w -c -o /dev/null conftest.c >/dev/null 2>&1 ; then
    { $as_echo "$as_me:$LINENO: result: yes" >&5
$as_echo "yes" >&6; }
