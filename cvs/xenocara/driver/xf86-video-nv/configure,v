#if !defined XV
#error XV not defined
#endif

int
main ()
{

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  _EXT_CHECK=yes
else
  _EXT_CHECK=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
