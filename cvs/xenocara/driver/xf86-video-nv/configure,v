    pkg_cv_PCIACCESS_LIBS="$PCIACCESS_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"pciaccess >= 0.10.7\""; } >&5
  ($PKG_CONFIG --exists --print-errors "pciaccess >= 0.10.7") 2>&5
