		fi

		CFLAGS="$CFLAGS -Werror=implicit"

		{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if $CC supports -Werror=implicit" >&5
$as_echo_n "checking if $CC supports -Werror=implicit... " >&6; }
		cacheid=xorg_cv_cc_flag__Werror_implicit
