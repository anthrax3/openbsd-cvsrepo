for ac_func in asprintf
do :
  ac_fn_c_check_func "$LINENO" "asprintf" "ac_cv_func_asprintf"
if test "x$ac_cv_func_asprintf" = xyes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_ASPRINTF 1
