	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(sunffb_drv_ladir)/$$f"; \
	done

clean-sunffb_drv_laLTLIBRARIES:
	-test -z "$(sunffb_drv_la_LTLIBRARIES)" || rm -f $(sunffb_drv_la_LTLIBRARIES)
	@@list='$(sunffb_drv_la_LTLIBRARIES)'; \
	locs=`for p in $$list; do echo $$p; done | \
	      sed 's|^[^/]*$$|.|; s|/[^/]*$$||; s|$$|/so_locations|' | \
	      sort -u`; \
	test -z "$$locs" || { \
	  echo rm -f $${locs}; \
	  rm -f $${locs}; \
	}
