install-sunffb_drv_laLTLIBRARIES: $(sunffb_drv_la_LTLIBRARIES)
	@@$(NORMAL_INSTALL)
	@@list='$(sunffb_drv_la_LTLIBRARIES)'; test -n "$(sunffb_drv_ladir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(MKDIR_P) '$(DESTDIR)$(sunffb_drv_ladir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(sunffb_drv_ladir)" || exit 1; \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(sunffb_drv_ladir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(sunffb_drv_ladir)"; \
	}

