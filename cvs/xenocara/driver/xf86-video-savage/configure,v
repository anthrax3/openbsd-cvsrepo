$as_echo_n "checking for DRI... " >&6; }

if test -n "$DRI_CFLAGS"; then
    pkg_cv_DRI_CFLAGS="$DRI_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"libdrm >= 2.0 xf86driproto\""; } >&5
  ($PKG_CONFIG --exists --print-errors "libdrm >= 2.0 xf86driproto") 2>&5
  ac_status=$?
  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
