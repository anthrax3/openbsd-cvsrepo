/* end confdefs.h.  */

#include "xorg-server.h"
#if !defined RANDR
#error RANDR not defined
#endif

int
main ()
{

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
