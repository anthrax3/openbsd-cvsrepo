  withval=$with_hal_callouts_dir; halcalloutsdir="$withval"
else
  halcalloutsdir="$libdir/hal"
fi

HAL_CALLOUTS_DIR=${halcalloutsdir}



# Check whether --with-hal-fdi-dir was given.
