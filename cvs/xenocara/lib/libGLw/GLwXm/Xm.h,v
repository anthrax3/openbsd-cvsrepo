    XmCR_MOVING_INSERT_CURSOR,
    XmCR_EXECUTE,
    XmCR_SINGLE_SELECT,
    XmCR_MULTIPLE_SELECT,
    XmCR_EXTENDED_SELECT,
    XmCR_BROWSE_SELECT,
    XmCR_DEFAULT_ACTION,
