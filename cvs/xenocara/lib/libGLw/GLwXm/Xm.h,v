};

typedef unsigned char XmDirection;

typedef struct {
    int reason;
    XEvent *event;
    Window window;
} XmDrawingAreaCallbackStruct;

#define XmUNSPECIFIED_PIXMAP 2

typedef unsigned char XmNavigationType;

