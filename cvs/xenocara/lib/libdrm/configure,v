	  whole_archive_flag_spec_GCJ='${wl}-z ${wl}allextract$convenience ${wl}-z ${wl}defaultextract'
	else
	  whole_archive_flag_spec_GCJ='-z allextract$convenience -z defaultextract'
	fi
	;;
      esac
      link_all_deplibs_GCJ=yes
      ;;
