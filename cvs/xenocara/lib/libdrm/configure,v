  enableval=$enable_vmwgfx; VMWGFX=$enableval
else
  VMWGFX=yes
fi


# Check whether --enable-omap-experimental-api was given.
if test "${enable_omap_experimental_api+set}" = set; then :
  enableval=$enable_omap_experimental_api; OMAP=$enableval
else
  OMAP=no
fi


# Check whether --enable-exynos-experimental-api was given.
if test "${enable_exynos_experimental_api+set}" = set; then :
  enableval=$enable_exynos_experimental_api; EXYNOS=$enableval
else
  EXYNOS=no
fi


# Check whether --enable-freedreno was given.
