fi

if test "x$FREEDRENO_KGSL" = xyes; then
	if test "x$FREEDRENO" != xyes; then
		as_fn_error $? "Cannot enable freedreno KGSL interface if freedreno is disabled" "$LINENO" 5
	fi
fi
 if test "x$FREEDRENO_KGSL" = xyes; then
  HAVE_FREEDRENO_KGSL_TRUE=
  HAVE_FREEDRENO_KGSL_FALSE='#'
else
  HAVE_FREEDRENO_KGSL_TRUE='#'
  HAVE_FREEDRENO_KGSL_FALSE=
