pkgconfigdir=${libdir}/pkgconfig

# Check whether --enable-udev was given.
if test "${enable_udev+set}" = set; then :
  enableval=$enable_udev; UDEV=$enableval
else
  UDEV=no
fi


# Check whether --enable-libkms was given.
if test "${enable_libkms+set}" = set; then :
  enableval=$enable_libkms; LIBKMS=$enableval
else
  LIBKMS=auto
fi


# Check whether --enable-intel was given.
if test "${enable_intel+set}" = set; then :
  enableval=$enable_intel; INTEL=$enableval
else
  INTEL=auto
fi


# Check whether --enable-radeon was given.
if test "${enable_radeon+set}" = set; then :
  enableval=$enable_radeon; RADEON=$enableval
else
  RADEON=auto
fi


# Check whether --enable-amdgpu was given.
if test "${enable_amdgpu+set}" = set; then :
  enableval=$enable_amdgpu; AMDGPU=$enableval
else
  AMDGPU=auto
fi


# Check whether --enable-nouveau was given.
if test "${enable_nouveau+set}" = set; then :
  enableval=$enable_nouveau; NOUVEAU=$enableval
else
  NOUVEAU=auto
fi


