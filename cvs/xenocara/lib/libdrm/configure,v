  enableval=$enable_freedreno_kgsl; FREEDRENO_KGSL=$enableval
else
  FREEDRENO_KGSL=no
fi


# Check whether --enable-tegra-experimental-api was given.
if test "${enable_tegra_experimental_api+set}" = set; then :
  enableval=$enable_tegra_experimental_api; TEGRA=$enableval
else
  TEGRA=no
fi


