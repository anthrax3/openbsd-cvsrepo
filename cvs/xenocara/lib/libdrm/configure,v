	CAIRO_LIBS=$pkg_cv_CAIRO_LIBS
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
	HAVE_CAIRO=yes
fi
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable Cairo tests" >&5
$as_echo_n "checking whether to enable Cairo tests... " >&6; }
