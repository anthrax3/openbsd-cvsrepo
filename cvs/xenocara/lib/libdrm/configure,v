	CAIRO="$HAVE_CAIRO"
fi
if test "x$CAIRO" = xyes; then
	if ! test "x$HAVE_CAIRO" = xyes; then
		as_fn_error $? "Cairo support required but not present" "$LINENO" 5
	fi

$as_echo "#define HAVE_CAIRO 1" >>confdefs.h

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $CAIRO" >&5
$as_echo "$CAIRO" >&6; }
