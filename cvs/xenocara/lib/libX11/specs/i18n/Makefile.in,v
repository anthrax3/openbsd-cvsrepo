@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	$(AM_V_GEN) $(GROFF) $(GROFF_FLAGS) -Thtml -P-Dimages -P-I$*-image $< 2> index.$@@.raw > $@@
