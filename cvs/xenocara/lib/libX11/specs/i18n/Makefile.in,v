# Install html generated images for specs
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@install-data-local:
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	test -z "$(imagesdir)" || $(mkdir_p) "$(DESTDIR)$(imagesdir)"
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	@@d="$(srcdir)/images/"; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	list=`ls $$d`; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	for p in $$list; do \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	  echo " $(INSTALL_DATA) '$$d$$p' '$(DESTDIR)$(imagesdir)/$$p'"; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	  $(INSTALL_DATA) "$$d$$p" "$(DESTDIR)$(imagesdir)/$$p"; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	done;

@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@uninstall-local:
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	@@if test -n $(DESTDIR)$(imagesdir); then \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	  if test -d $(DESTDIR)$(imagesdir); then \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	    list=`ls $(DESTDIR)$(imagesdir)`; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	    for p in $$list; do \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	      echo " rm -f '$(DESTDIR)$(imagesdir)/$$p'"; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	      rm -f "$(DESTDIR)$(imagesdir)/$$p"; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	    done \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	  fi; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	fi;

@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@mostlyclean-local:
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	@@rm -fr images

@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@.ms.ps:
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	-$(AM_V_GEN) $(GROFF) -Tps $(GROFF_FLAGS) $< 2> index.$@@.raw > $@@
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	@@if grep '^[^1-9.]' index.$@@.raw | grep -v warning; then exit 1; \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	 else test $$? -le 1; fi

@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@.ms.txt:
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	$(AM_V_GEN) env GROFF_NO_SGR=TRUE $(GROFF) -Tutf8 $(GROFF_FLAGS) \
@@ENABLE_SPECS_TRUE@@@@HAVE_GROFF_MS_TRUE@@	 $< 2> index.$@@.raw > $@@

