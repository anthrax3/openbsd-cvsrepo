extern XcmsFunctionSet XcmsLinearRGBFunctionSet;

extern XcmsFunctionSet *_XcmsSCCFuncSetsInit[];
extern XcmsFunctionSet **_XcmsSCCFuncSets;

extern XcmsRegColorSpaceEntry _XcmsRegColorSpaces[];

/* functions */
