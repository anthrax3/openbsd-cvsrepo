text
@a118 22
#endif
#ifdef WORD64
int
_XkbReadCopyData32(int *from,long *lp,int num_words)
{
long *lpack;
long mask32 = 0x00000000ffffffff;
long maskw, i, bits;

    lpack = (long *)from;
    bits = 32;

    for (i=0;i<num_words;i++) {
	maskw = mask32 << bits;
	*lp++ = (*lpack & maskw) >> bits;
	bits = bits ^ 32;
	if (bits)
	    lpack++;
    }
    return 1;
}
#endif
