fs0	{
	charset		ISO8859-1:GL
	font            ISO8859-1:GL
}
XCOMM 	fs1 class (Thai) 
fs1	{
	charset         ISO8859-11:GR
	font            ISO8859-11:GR
}
END XLC_FONTSET

