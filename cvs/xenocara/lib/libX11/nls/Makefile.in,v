installdirs:
	for dir in "$(DESTDIR)$(x11localedir)" "$(DESTDIR)$(x11localedir)" "$(DESTDIR)$(x11localedir)"; do \
	  test -z "$$dir" || $(MKDIR_P) "$$dir"; \
	done
