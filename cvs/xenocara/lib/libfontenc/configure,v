			CFLAGS="$CFLAGS -Werror=unused-command-line-argument"
		fi

		CFLAGS="$CFLAGS -Waddress"

		{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if $CC supports -Waddress" >&5
$as_echo_n "checking if $CC supports -Waddress... " >&6; }
