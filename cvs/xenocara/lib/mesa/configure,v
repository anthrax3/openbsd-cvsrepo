radeon_gallium_llvm_check() {
    if test "x$enable_gallium_llvm" != "xyes"; then
        as_fn_error $? "--enable-gallium-llvm is required when building $1" "$LINENO" 5
    fi
    radeon_llvm_check $*
}

swr_llvm_check() {
    gallium_require_llvm $1
    if test ${LLVM_VERSION_INT} -lt 306; then
        as_fn_error $? "LLVM version 3.6 or later required when building $1" "$LINENO" 5
    fi
    if test "x$enable_gallium_llvm" != "xyes"; then
        as_fn_error $? "--enable-gallium-llvm is required when building $1" "$LINENO" 5
    fi
}

swr_require_cxx_feature_flags() {
    feature_name="$1"
    preprocessor_test="$2"
    option_list="$3"
    output_var="$4"

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether $CXX supports $feature_name" >&5
$as_echo_n "checking whether $CXX supports $feature_name... " >&6; }
    ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

    save_CXXFLAGS="$CXXFLAGS"
    save_IFS="$IFS"
    IFS=","
    found=0
    for opts in $option_list
    do
        unset IFS
        CXXFLAGS="$opts $save_CXXFLAGS"
        cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
   #if !($preprocessor_test)
                    #error
                    #endif

int
main ()
{

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_compile "$LINENO"; then :
  found=1; break
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
        IFS=","
    done
    IFS="$save_IFS"
    CXXFLAGS="$save_CXXFLAGS"
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    if test $found -eq 1; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: $opts" >&5
$as_echo "$opts" >&6; }
        eval "$output_var=\$opts"
        return 0
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    as_fn_error $? "swr requires $feature_name support" "$LINENO" 5
    return 1
}

