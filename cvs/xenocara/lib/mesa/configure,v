    return __sync_add_and_fetch(&v, (uint32_t)1);
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  GCC_32BIT_ATOMICS_SUPPORTED=yes
else
  GCC_32BIT_ATOMICS_SUPPORTED=no
