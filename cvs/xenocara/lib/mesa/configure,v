except ImportError as err:
    sys.exit(err)
else:
    ver_req = map(int, '$PYTHON_MAKO_REQUIRED'.split('.'))
    ver_act = map(int, mako.__version__.split('.'))
    sys.exit(int(ver_req > ver_act))
    " | $PYTHON2 -

