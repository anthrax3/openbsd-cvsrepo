    done
fi

# XXX: Keep in sync with LLVM_REQUIRED_SWR
 if test "x$LLVM_VERSION" != x3.9.0 -a \
                                              "x$LLVM_VERSION" != x3.9.1; then
  SWR_INVALID_LLVM_VERSION_TRUE=
  SWR_INVALID_LLVM_VERSION_FALSE='#'
else
  SWR_INVALID_LLVM_VERSION_TRUE='#'
  SWR_INVALID_LLVM_VERSION_FALSE=
