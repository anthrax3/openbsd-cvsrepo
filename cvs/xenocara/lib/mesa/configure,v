 if test x$GCC_ATOMIC_BUILTINS_SUPPORTED = x1; then
  GCC_ATOMIC_BUILTINS_SUPPORTED_TRUE=
  GCC_ATOMIC_BUILTINS_SUPPORTED_FALSE='#'
else
  GCC_ATOMIC_BUILTINS_SUPPORTED_TRUE='#'
  GCC_ATOMIC_BUILTINS_SUPPORTED_FALSE=
fi


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether __sync_add_and_fetch_4 is supported" >&5
$as_echo_n "checking whether __sync_add_and_fetch_4 is supported... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
