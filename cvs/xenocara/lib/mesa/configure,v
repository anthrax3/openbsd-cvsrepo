                as_fn_error $? "Intel Vulkan driver requires the i965 dri driver" "$LINENO" 5
            fi
            if test "x$with_sha1" == "x"; then
                as_fn_error $? "Intel Vulkan driver requires SHA1" "$LINENO" 5
            fi
            HAVE_INTEL_VULKAN=yes;

            ;;
        xradeon)
