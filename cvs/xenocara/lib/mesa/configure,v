gallium_require_drm_loader() {
    if test "x$need_pci_id$have_pci_id" = xyesno; then
        as_fn_error $? "Gallium drm loader requires libudev >= $LIBUDEV_REQUIRED or sysfs" "$LINENO" 5
    fi
}

