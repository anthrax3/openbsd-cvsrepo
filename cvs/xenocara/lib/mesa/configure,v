    DEFINES="$DEFINES -DMISSING_32BIT_ATOMICS"
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $GCC_32BIT_ATOMICS_SUPPORTED" >&5
$as_echo "$GCC_32BIT_ATOMICS_SUPPORTED" >&6; }

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether __sync_add_and_fetch_8 is supported" >&5
$as_echo_n "checking whether __sync_add_and_fetch_8 is supported... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

