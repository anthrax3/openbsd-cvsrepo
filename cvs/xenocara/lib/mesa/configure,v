	if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"xvmc >= \$XVMC_REQUIRED\""; } >&5
  ($PKG_CONFIG --exists --print-errors "xvmc >= $XVMC_REQUIRED") 2>&5
