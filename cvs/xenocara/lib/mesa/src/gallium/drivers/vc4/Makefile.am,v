	-I$(top_builddir)/src/compiler/nir \
	$(LIBDRM_CFLAGS) \
	$(VC4_CFLAGS) \
	$(GALLIUM_DRIVER_CFLAGS) \
	$(SIM_CFLAGS) \
	$(VALGRIND_CFLAGS) \
	$()

