@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	done;
@@BUILD_SHARED_TRUE@@@@HAVE_COMPAT_SYMLINKS_TRUE@@	$(RM) .install-mesa-links

