      (out+j)[1] = (uint)in[i+6];
      (out+j)[2] = (uint)in[i+2];
      (out+j)[3] = (uint)in[i-2];
      (out+j)[4] = (uint)in[i+0];
      (out+j)[5] = (uint)in[i+3];
    }
  }
