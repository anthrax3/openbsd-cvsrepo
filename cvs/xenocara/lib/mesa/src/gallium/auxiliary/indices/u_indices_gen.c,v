  uint *out = (uint*)_out;
  unsigned i, j;
  (void)j;
  for (i = start, j = 0; j < out_nr; i+=2, j+=6) { 
    if (i % 4 == 0) {
      /* even triangle */
      (out+j)[0] = (uint)in[i+4];
      (out+j)[1] = (uint)in[i+5];
      (out+j)[2] = (uint)in[i+0];
      (out+j)[3] = (uint)in[i+1];
      (out+j)[4] = (uint)in[i+2];
      (out+j)[5] = (uint)in[i+3];
