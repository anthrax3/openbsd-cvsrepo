  (void)j;
  for (i = start; i < (out_nr+start); i+=2) { 
      (out+i)[0] = (ushort)in[i];
      (out+i)[1] = (ushort)in[i+1];
   }
}
static void translate_linestrip_ushort2ushort_last2last_prenable(
    const void * _in,
