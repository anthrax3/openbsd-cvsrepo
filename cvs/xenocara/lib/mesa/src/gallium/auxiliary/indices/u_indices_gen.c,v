static void generate_tristripadj_uint_last2last(
    unsigned start,
    unsigned out_nr,
    void *_out )
{
  uint *out = (uint*)_out;
  unsigned i, j;
  (void)j;
  for (i = start, j = 0; j < out_nr; i+=2, j+=6) { 
    if (i % 4 == 0) {
      /* even triangle */
      (out+j)[0] = (uint)(i+0);
      (out+j)[1] = (uint)(i+1);
      (out+j)[2] = (uint)(i+2);
      (out+j)[3] = (uint)(i+3);
      (out+j)[4] = (uint)(i+4);
      (out+j)[5] = (uint)(i+5);
    } else {
      /* odd triangle */
      (out+j)[0] = (uint)(i+2);
      (out+j)[1] = (uint)(i-2);
      (out+j)[2] = (uint)(i+0);
      (out+j)[3] = (uint)(i+3);
      (out+j)[4] = (uint)(i+4);
      (out+j)[5] = (uint)(i+6);
    }
  }
}
