  ushort *out = (ushort*)_out;
  unsigned i, j;
  (void)j;
  for (i = start; i < (out_nr+start); i++) { 
      (out+i)[0] = (ushort)in[i];
   }
}
static void translate_lines_uint2ushort_last2last_prenable(
    const void * _in,
    unsigned start,
    unsigned in_nr,
    unsigned out_nr,
    unsigned restart_index,
