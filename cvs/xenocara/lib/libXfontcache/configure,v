pkg_failed=no
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for XFONTCACHE" >&5
$as_echo_n "checking for XFONTCACHE... " >&6; }

if test -n "$XFONTCACHE_CFLAGS"; then
    pkg_cv_XFONTCACHE_CFLAGS="$XFONTCACHE_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"x11 xext xextproto fontcacheproto\""; } >&5
  ($PKG_CONFIG --exists --print-errors "x11 xext xextproto fontcacheproto") 2>&5
  ac_status=$?
  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_XFONTCACHE_CFLAGS=`$PKG_CONFIG --cflags "x11 xext xextproto fontcacheproto" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
if test -n "$XFONTCACHE_LIBS"; then
    pkg_cv_XFONTCACHE_LIBS="$XFONTCACHE_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"x11 xext xextproto fontcacheproto\""; } >&5
  ($PKG_CONFIG --exists --print-errors "x11 xext xextproto fontcacheproto") 2>&5
  ac_status=$?
  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_XFONTCACHE_LIBS=`$PKG_CONFIG --libs "x11 xext xextproto fontcacheproto" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi



if test $pkg_failed = yes; then
   	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        XFONTCACHE_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors --cflags --libs "x11 xext xextproto fontcacheproto" 2>&1`
        else
	        XFONTCACHE_PKG_ERRORS=`$PKG_CONFIG --print-errors --cflags --libs "x11 xext xextproto fontcacheproto" 2>&1`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$XFONTCACHE_PKG_ERRORS" >&5

	as_fn_error $? "Package requirements (x11 xext xextproto fontcacheproto) were not met:

$XFONTCACHE_PKG_ERRORS

Consider adjusting the PKG_CONFIG_PATH environment variable if you
installed software in a non-standard prefix.

Alternatively, you may set the environment variables XFONTCACHE_CFLAGS
and XFONTCACHE_LIBS to avoid the need to call pkg-config.
See the pkg-config man page for more details." "$LINENO" 5
elif test $pkg_failed = untried; then
     	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
	{ { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "The pkg-config script could not be found or is too old.  Make sure it
is in your PATH or set the PKG_CONFIG environment variable to the full
path to pkg-config.

Alternatively, you may set the environment variables XFONTCACHE_CFLAGS
and XFONTCACHE_LIBS to avoid the need to call pkg-config.
See the pkg-config man page for more details.

To get pkg-config, see <http://pkg-config.freedesktop.org/>.
See \`config.log' for more details" "$LINENO" 5; }
else
	XFONTCACHE_CFLAGS=$pkg_cv_XFONTCACHE_CFLAGS
	XFONTCACHE_LIBS=$pkg_cv_XFONTCACHE_LIBS
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }

