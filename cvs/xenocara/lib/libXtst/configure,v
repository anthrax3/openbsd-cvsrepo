$as_echo "$xorg_cv_cc_flag_unused_command_line_argument" >&6; }
	xorg_testset_cc_unused_command_line_argument=$xorg_cv_cc_flag_unused_command_line_argument
	CFLAGS="$xorg_testset_save_CFLAGS"
fi

