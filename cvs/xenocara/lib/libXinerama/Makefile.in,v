	$(am__cd) $(srcdir) && $(ACLOCAL) $(ACLOCAL_AMFLAGS)
$(am__aclocal_m4_deps):

config.h: stamp-h1
	@@if test ! -f $@@; then rm -f stamp-h1; else :; fi
	@@if test ! -f $@@; then $(MAKE) $(AM_MAKEFLAGS) stamp-h1; else :; fi

