#ifndef _XLBX_H_
#define _XLBX_H_

#include <X11/Xfuncproto.h>
#include <X11/Xdefs.h>
#include <X11/Xlib.h>
#include <X11/extensions/lbx.h>

_XFUNCPROTOBEGIN

Bool XLbxQueryExtension(
    Display*		/* dpy */,
    int*		/* requestp */,
    int*		/* event_basep */,
    int*		/* error_basep */
);

Bool XLbxQueryVersion(
    Display*		/* dpy */,
    int*		/* majorVersion */,
    int*		/* minorVersion */
);

int XLbxGetEventBase(Display *dpy);

_XFUNCPROTOEND

#endif
@


