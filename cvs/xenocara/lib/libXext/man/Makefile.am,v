	DPMSCapable.man				\
	DPMSDisable.man				\
	DPMSEnable.man				\
	DPMSForceLevel.man			\
	DPMSGetTimeouts.man			\
	DPMSGetVersion.man			\
	DPMSInfo.man				\
	DPMSQueryExtension.man			\
	DPMSSetTimeouts.man			\
	XShape.man				\
	XShm.man				\
	XcupGetReservedColormapEntries.man	\
	XcupQueryVersion.man			\
	XcupStoreColors.man			\
	XdbeAllocateBackBufferName.man		\
	XdbeBeginIdiom.man			\
	XdbeDeallocateBackBufferName.man	\
	XdbeEndIdiom.man			\
	XdbeFreeVisualInfo.man			\
	XdbeGetBackBufferAttributes.man		\
	XdbeGetVisualInfo.man			\
	XdbeQueryExtension.man			\
	XdbeSwapBuffers.man			\
	Xevi.man				\
	Xmbuf.man

