#ifdef HAVE_CONFIG_H
#include <config.h>
#endif
#include <X11/Xos.h>
#include <X11/X.h>
#include <X11/Xmd.h>
#include <X11/Xdmcp.h>

int
XdmcpReadCARD16 (XdmcpBufferPtr buffer, CARD16Ptr valuep)
{
    CARD8   high, low;

    if (XdmcpReadCARD8 (buffer, &high) &&
        XdmcpReadCARD8 (buffer, &low))
    {
	*valuep = (((CARD16) high) << 8) | ((CARD16) low);
	return TRUE;
    }
    return FALSE;
}
@


