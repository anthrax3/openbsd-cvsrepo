else
DEFAULT_LIB = libXaw6.@@LIBEXT@@
endif

endif

if BUILD_XAW7

pkgconfig_DATA += xaw7.pc
if PLATFORM_DARWIN
DEFAULT_LIB = libXaw.7.@@LIBEXT@@
else
DEFAULT_LIB = libXaw7.@@LIBEXT@@
endif

endif

