// Check varargs and va_copy.
static void
test_varargs (const char *format, ...)
{
  va_list args;
  va_start (args, format);
  va_list args_copy;
  va_copy (args_copy, args);
d18703 28
a18730 24
  const char *str;
  int number;
  float fnumber;

  while (*format)
    {
