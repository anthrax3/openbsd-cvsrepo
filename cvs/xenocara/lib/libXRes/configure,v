if test -n "$PKG_CONFIG"; then
    if test -n "$XRES_CFLAGS"; then
        pkg_cv_XRES_CFLAGS="$XRES_CFLAGS"
    else
        if test -n "$PKG_CONFIG" && \
    { (echo "$as_me:$LINENO: \$PKG_CONFIG --exists --print-errors \"x11 xext xextproto\"") >&5
  ($PKG_CONFIG --exists --print-errors "x11 xext xextproto") 2>&5
  ac_status=$?
  echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); }; then
  pkg_cv_XRES_CFLAGS=`$PKG_CONFIG --cflags "x11 xext xextproto" 2>/dev/null`
else
  pkg_failed=yes
fi
    fi
else
	pkg_failed=untried
fi
if test -n "$PKG_CONFIG"; then
    if test -n "$XRES_LIBS"; then
        pkg_cv_XRES_LIBS="$XRES_LIBS"
    else
        if test -n "$PKG_CONFIG" && \
    { (echo "$as_me:$LINENO: \$PKG_CONFIG --exists --print-errors \"x11 xext xextproto\"") >&5
  ($PKG_CONFIG --exists --print-errors "x11 xext xextproto") 2>&5
  ac_status=$?
  echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); }; then
  pkg_cv_XRES_LIBS=`$PKG_CONFIG --libs "x11 xext xextproto" 2>/dev/null`
else
  pkg_failed=yes
fi
    fi
else
	pkg_failed=untried
fi



if test $pkg_failed = yes; then

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        XRES_PKG_ERRORS=`$PKG_CONFIG --short-errors --errors-to-stdout --print-errors "x11 xext xextproto"`
        else
	        XRES_PKG_ERRORS=`$PKG_CONFIG --errors-to-stdout --print-errors "x11 xext xextproto"`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$XRES_PKG_ERRORS" >&5

	{ { echo "$as_me:$LINENO: error: Package requirements (x11 xext xextproto) were not met:

$XRES_PKG_ERRORS

