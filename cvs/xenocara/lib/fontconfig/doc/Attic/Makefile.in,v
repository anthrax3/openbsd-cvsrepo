@@USEDOCBOOK_TRUE@@	-test -f local-$@@ && mv local-$@@ $@@
@@USEDOCBOOK_TRUE@@	-test -f $@@ || mv $@@.tmp $@@
@@USEDOCBOOK_TRUE@@	-test -f $@@.tmp && $(RM) $@@.tmp

@@USEDOCBOOK_TRUE@@fontconfig-user.txt: local-fontconfig-user.sgml version.sgml confdir.sgml
