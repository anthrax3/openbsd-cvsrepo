@@USEDOCBOOK_TRUE@@	$(top_srcdir)/missing --run $(DOC2PDF) $< && mv local-$@@ $@@ \
@@USEDOCBOOK_TRUE@@	|| echo Failed to generate $@@ >&2; \
@@USEDOCBOOK_TRUE@@	(test -f $@@ || echo $(DOC2PDF) is required to generate this file >> $@@)
