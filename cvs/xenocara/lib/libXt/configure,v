fi
PERL=$ac_cv_path_PERL
if test -n "$PERL"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $PERL" >&5
$as_echo "$PERL" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


   if test "x$PERL" = "x"; then
        as_fn_error $? "--with-perl=yes specified but perl not found in PATH" "$LINENO" 5
   fi
   have_perl=yes
