1.4
log
@Update to libXt 1.0.8. No API change.
@
text
@d25 1
a25 10
CLEANFILES = $(appman_DATA) $(noinst_DATA)

SED = sed

# Strings to replace in man pages
XORGRELSTRING = @@PACKAGE_STRING@@
  XORGMANNAME = X Version 11

MAN_SUBSTS = \
	-e 's|__xorgversion__|"$(XORGRELSTRING)" "$(XORGMANNAME)"|'
d29 1
@


