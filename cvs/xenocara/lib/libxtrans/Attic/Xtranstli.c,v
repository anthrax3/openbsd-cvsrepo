    {
	prmsg(1,
	      "TLIOpenCLTSServer: Unable to determine device for %s\n",
	      thistrans->TransName);
	return NULL;
    }

    if( (ciptr=TRANS(TLIOpen)(TLItrans2devtab[i].devcltsname)) == NULL )
