a15185 2
      if { (eval echo "$as_me:$LINENO: \"$ac_compile\"") >&5
  (eval $ac_compile) 2>&5
d15187 3
d15191 11
a15201 36
  (exit $ac_status); } 2>conftest.err; then
        soname=conftest
        lib=conftest
