        if test $_pkg_short_errors_supported = yes; then
	        SXPM_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors --cflags --libs "x11 xt xext xextproto xproto >= 7.0.17" 2>&1`
        else
	        SXPM_PKG_ERRORS=`$PKG_CONFIG --print-errors --cflags --libs "x11 xt xext xextproto xproto >= 7.0.17" 2>&1`
        fi
	# Put the nasty error message in config.log where it belongs
