    # maximum length that is only half of the actual maximum length, but
    # we can't tell.
    SHELL=${SHELL-${CONFIG_SHELL-/bin/sh}}
    while (test "X"`$SHELL $0 --fallback-echo "X$teststring" 2>/dev/null` \
d6356 13
a6368 12
