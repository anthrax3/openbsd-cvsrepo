.ft B
.nf
#include <X11/extensions/Xcomposite.h>
.fi
.ft
d16 1
a16 1
.BI "Bool XCompositeQueryExtension(Display\ *" "dpy" ", int\ *" "event_basep" ", int\ *" "error_basep" ");"
d18 1
a18 1
.BI "Status XCompositeQueryVersion(Display\ *" "dpy" ", int\ *" "major_versionp" ", int\ *" "minor_versionp" ");"
