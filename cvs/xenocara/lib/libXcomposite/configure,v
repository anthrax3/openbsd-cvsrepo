    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"xfixes\""; } >&5
  ($PKG_CONFIG --exists --print-errors "xfixes") 2>&5
  ac_status=$?
