    if test "x$mtsafe" = "x" ; then
	{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for gethostbyname_r in -lnsl" >&5
$as_echo_n "checking for gethostbyname_r in -lnsl... " >&6; }
