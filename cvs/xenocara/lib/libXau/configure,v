fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_c_gethostbyname_r" >&5
$as_echo "$ac_cv_lib_c_gethostbyname_r" >&6; }
if test "x$ac_cv_lib_c_gethostbyname_r" = xyes; then :
  mtsafe=yes
