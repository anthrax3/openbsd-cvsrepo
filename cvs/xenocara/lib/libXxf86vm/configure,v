		eval supported=\$$cacheid
		{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $supported" >&5
$as_echo "$supported" >&6; }
		if test "$supported" = "yes" ; then
			BASE_CFLAGS="$BASE_CFLAGS -Wmissing-format-attribute"
			found="yes"
		fi
	fi


