{ $as_echo "$as_me:$LINENO: result: $ac_header_compiler" >&5
$as_echo "$ac_header_compiler" >&6; }

# Is the header present?
{ $as_echo "$as_me:$LINENO: checking sys/mman.h presence" >&5
$as_echo_n "checking sys/mman.h presence... " >&6; }
