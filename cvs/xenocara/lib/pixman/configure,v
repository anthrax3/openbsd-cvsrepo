$as_echo "$have_sse2_intrinsics" >&6; }
if test $enable_sse2 = yes && test $have_sse2_intrinsics = no ; then
   as_fn_error $? "SSE2 intrinsics not detected" "$LINENO" 5
fi

 if test $have_sse2_intrinsics = yes; then
  USE_SSE2_TRUE=
  USE_SSE2_FALSE='#'
else
  USE_SSE2_TRUE='#'
  USE_SSE2_FALSE=
fi



