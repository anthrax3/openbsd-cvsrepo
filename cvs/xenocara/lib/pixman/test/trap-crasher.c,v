1.4
log
@Update to pixman 0.32.4. Tested by naddy@@ and ajacoutot@@
@
text
@#include <stdlib.h>
#include "utils.h"

int
main()
{
    pixman_image_t *dst;
    pixman_trapezoid_t traps[] = {
	{
	    2147483646,
	    2147483647,
	    {
		{ 0, 0 },
		{ 0, 2147483647 }
	    },
	    {
		{ 65536, 0 },
		{ 0, 2147483647 }
	    }
	},
	{
	    32768,
	    - 2147483647,
	    {
		{ 0, 0 },
		{ 0, 2147483647 }
	    },
	    {
		{ 65536, 0 },
		{ 0, 2147483647 }
	    }
	},
    };

    dst = pixman_image_create_bits (PIXMAN_a8, 1, 1, NULL, -1);

    pixman_add_trapezoids (dst, 0, 0, ARRAY_LENGTH (traps), traps);
    return (0);
}
@


