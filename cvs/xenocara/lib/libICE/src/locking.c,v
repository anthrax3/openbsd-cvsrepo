 */


Status
IceInitThreads (
	void
)
{
    return (0);
}


void
IceAppLockConn (
	IceConn	iceConn
)
{
    IceLockConn (iceConn);
}


void
IceAppUnlockConn (
	IceConn iceConn
)
{
    IceUnlockConn (iceConn);
}
