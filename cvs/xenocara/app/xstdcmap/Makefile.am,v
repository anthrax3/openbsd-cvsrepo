SUBDIRS = man
bin_PROGRAMS = xstdcmap

AM_CFLAGS = $(XSTDCMAP_CFLAGS) -D_BSD_SOURCE $(CWARNFLAGS)
xstdcmap_LDADD = $(XSTDCMAP_LIBS)

xstdcmap_SOURCES =	\
        xstdcmap.c

MAINTAINERCLEANFILES = ChangeLog INSTALL

.PHONY: ChangeLog INSTALL

INSTALL:
	$(INSTALL_CMD)

ChangeLog:
	$(CHANGELOG_CMD)

dist-hook: ChangeLog INSTALL
@


