$as_echo_n "checking for XLOGO... " >&6; }

if test -n "$XLOGO_CFLAGS"; then
    pkg_cv_XLOGO_CFLAGS="$XLOGO_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"\$XLOGO_DEPS\""; } >&5
  ($PKG_CONFIG --exists --print-errors "$XLOGO_DEPS") 2>&5
