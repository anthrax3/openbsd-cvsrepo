text
@WindowTitle {Screen Grabber}
WindowSize 360 360		# Taille
WindowPosition 191 50		# Position
ForeColor	{black}
BackColor	{grey85}
ShadowColor	{grey55}
HilightColor	{grey100}
Font		-adobe-helvetica-bold-r-*-*-12-*

Init
 Begin
  WarpPointer 1
  Set $Ext = {.gif}
  Set $Format = {| ppmtogif}
 End
 
Widget 1
Property
 Size 50 30
 Position 190 320
 Type PushButton
 Title {OK}
Main
 Case message of
  SingleClic :
  Begin
   If (GetValue 5)==1 Then
    Do {Exec xwd } $Opt1 { } $Opt2 { | xwdtopnm } $Format { > }  (GetTitle 7) $Ext
   Else
    Do {Exec xwd } $Opt1 { } $Opt2 { | xwdtopnm | ppmtopgm | pnmtops | } (GetTitle 11)
   Quit
  End
End

Widget 2
Property
 Size 50 30
 Position 280 320
 Type PushButton
 Title {Cancel}
Main
 Case message of
  SingleClic :
  Begin
   Do {Exec xset s } $TimeOut
   Quit
  End
End

