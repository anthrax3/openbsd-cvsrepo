	@@rm -f xwd$(EXEEXT)
	$(AM_V_CCLD)$(LINK) $(xwd_OBJECTS) $(xwd_LDADD) $(LIBS)

mostlyclean-compile:
	-rm -f *.$(OBJEXT)

distclean-compile:
	-rm -f *.tab.c

