        if test $_pkg_short_errors_supported = yes; then
	        XI2_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors --cflags --libs "xi >= 1.2.99.2 inputproto >= 1.9.99.15" 2>&1`
        else
	        XI2_PKG_ERRORS=`$PKG_CONFIG --print-errors --cflags --libs "xi >= 1.2.99.2 inputproto >= 1.9.99.15" 2>&1`
