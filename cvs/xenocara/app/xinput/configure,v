        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
	HAVE_XI2="yes";
$as_echo "#define HAVE_XI2 1" >>confdefs.h

fi;
 if  test "$HAVE_XI2" = "yes" ; then
  HAVE_XI2_TRUE=
