#include "xinput.h"

#ifdef HAVE_CONFIG_H
#include <config.h>
#endif

static const char version_id[] = VERSION;

int
version(Display	*display,
	int	argc,
	char	*argv[],
	char	*name,
	char	*desc)
{
    printf("%s %s\n", name, version_id);
    return EXIT_SUCCESS;
}

/* end of version.c */
