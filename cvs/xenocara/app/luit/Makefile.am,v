SUBDIRS=man
bin_PROGRAMS = luit

AM_CFLAGS =		\
	$(CWARNFLAGS)	\
	$(LUIT_CFLAGS)	\
	$(OS_CFLAGS)	\
	-DLOCALE_ALIAS_FILE=\"$(LOCALEALIASFILE)\"

luit_LDADD = $(LUIT_LIBS)

luit_SOURCES =		\
        charset.c	\
        charset.h	\
        iso2022.c	\
        iso2022.h	\
        locale.c	\
        luit.c		\
        luit.h		\
        other.c		\
        other.h		\
        parser.c	\
        parser.h	\
        sys.c		\
        sys.h

MAINTAINERCLEANFILES = ChangeLog INSTALL

.PHONY: ChangeLog INSTALL

INSTALL:
	$(INSTALL_CMD)

ChangeLog:
	$(CHANGELOG_CMD)

dist-hook: ChangeLog INSTALL
@


