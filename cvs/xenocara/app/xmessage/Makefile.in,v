	@@rm -f xmessage$(EXEEXT)
	$(AM_V_CCLD)$(LINK) $(xmessage_OBJECTS) $(xmessage_LDADD) $(LIBS)

mostlyclean-compile:
	-rm -f *.$(OBJEXT)

distclean-compile:
	-rm -f *.tab.c

