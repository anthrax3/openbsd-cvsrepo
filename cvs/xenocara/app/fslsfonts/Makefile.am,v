SUBDIRS = man
bin_PROGRAMS = fslsfonts

AM_CFLAGS = $(FSLSFONTS_CFLAGS) $(CWARNFLAGS)
fslsfonts_LDADD = $(FSLSFONTS_LIBS)

fslsfonts_SOURCES =	\
        fslsfonts.c





MAINTAINERCLEANFILES = ChangeLog INSTALL




.PHONY: ChangeLog INSTALL

INSTALL:
	$(INSTALL_CMD)

ChangeLog:
	$(CHANGELOG_CMD)

dist-hook: ChangeLog INSTALL
@


