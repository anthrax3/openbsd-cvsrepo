  pkg_cv_X11_LIBS=`$PKG_CONFIG --libs "xproto >= 7.0.25" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi



