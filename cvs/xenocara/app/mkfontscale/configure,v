    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"xproto >= 7.0.25\""; } >&5
  ($PKG_CONFIG --exists --print-errors "xproto >= 7.0.25") 2>&5
