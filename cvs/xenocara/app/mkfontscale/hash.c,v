        *dst = lwr(*src);
a54 21
static int
strcmp_lwr(char *a, char *b)
{
    while(*a != '\0' && *b != '\0') {
        if(lwr(*a) != lwr(*b)) {
            if(lwr(*a) < lwr(*b))
                return -1;
            if(lwr(*a) > lwr(*b))
                return 1;
        }
        a++;
        b++;
    }
    if (*a != '\0')
        return -1;
    else if(*b == '\0')
        return 1;
    else
        return 0;
}

