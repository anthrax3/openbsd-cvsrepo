#ifndef _MKS_DATA_H_
#define _MKS_DATA_H_ 1

/* Order is significant.  For example, some B&H fonts are hinted by
   URW++, and both strings appear in the notice. */

static const char *notice_foundries[][2] =
    {{"Iorsh", "culmus"},
     {"Gnat", "culmus"},
     {"Bigelow", "b&h"},
     {"Adobe", "adobe"},
     {"Bitstream", "bitstream"},
     {"Monotype", "monotype"},
     {"Linotype", "linotype"},
