	$(AM_V_CCLD)$(LINK) $(xrdb_OBJECTS) $(xrdb_LDADD) $(LIBS)

mostlyclean-compile:
	-rm -f *.$(OBJEXT)

distclean-compile:
	-rm -f *.tab.c

