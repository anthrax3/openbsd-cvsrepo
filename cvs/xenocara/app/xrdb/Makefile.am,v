SUBDIRS = man
bin_PROGRAMS = xrdb

AM_CFLAGS = $(CWARNFLAGS) $(XRDB_CFLAGS)
xrdb_LDADD = $(XRDB_LIBS)

xrdb_SOURCES =	\
        xrdb.c

MAINTAINERCLEANFILES = ChangeLog INSTALL

.PHONY: ChangeLog INSTALL

INSTALL:
	$(INSTALL_CMD)

ChangeLog:
	$(CHANGELOG_CMD)

dist-hook: ChangeLog INSTALL


@


