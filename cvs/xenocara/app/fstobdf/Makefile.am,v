SUBDIRS = man
bin_PROGRAMS = fstobdf

AM_CFLAGS = $(FSTOBDF_CFLAGS) $(CWARNFLAGS)
fstobdf_LDADD = $(FSTOBDF_LIBS)

fstobdf_SOURCES =	\
        chars.c \
        fstobdf.c \
        fstobdf.h \
        header.c \
        props.c

MAINTAINERCLEANFILES = ChangeLog INSTALL

.PHONY: ChangeLog INSTALL

INSTALL:
	$(INSTALL_CMD)

ChangeLog:
	$(CHANGELOG_CMD)

dist-hook: ChangeLog INSTALL
@


