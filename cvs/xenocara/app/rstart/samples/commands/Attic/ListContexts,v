list=@@List

for i in $RSTART_GLOBAL_CONTEXTS			\
	$HOME/$RSTART_LOCAL_CONTEXTS
do
	if test -f $i/$list
	then
		sed -e '/^$/d' -e '/^#/d' $i/$list
	fi
done
@


