  (exit \$1)
}
as_func_success () {
  as_func_return 0
}
as_func_failure () {
  as_func_return 1
}
as_func_ret_success () {
  return 0
}
as_func_ret_failure () {
  return 1
}

exitcode=0
if as_func_success; then
  :
d427 1
a427 2
  exitcode=1
  echo as_func_success failed.
d430 2
a431 7
if as_func_failure; then
  exitcode=1
  echo as_func_failure succeeded.
fi

