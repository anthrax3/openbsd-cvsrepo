#ifndef __TUBE_H__
#define __TUBE_H__

extern void tube (GLfloat x1, GLfloat y1, GLfloat z1,
                  GLfloat x2, GLfloat y2, GLfloat z2,
                  GLfloat diameter, GLfloat cap_size,
                  int faces, Bool smooth, Bool wire);

extern void cone (GLfloat x1, GLfloat y1, GLfloat z1,
                  GLfloat x2, GLfloat y2, GLfloat z2,
                  GLfloat diameter, GLfloat cap_size,
                  int faces, Bool smooth, Bool wire);

#endif /* __TUBE_H__ */
