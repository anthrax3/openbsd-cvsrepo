{ $as_echo "$as_me:$LINENO: result: $ac_cv_header_elf_h" >&5
$as_echo "$ac_cv_header_elf_h" >&6; }
else
  # Is the header compilable?
{ $as_echo "$as_me:$LINENO: checking elf.h usability" >&5
$as_echo_n "checking elf.h usability... " >&6; }
