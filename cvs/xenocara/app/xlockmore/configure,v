case "x$use_button_logout" in
x|xyes*)
  { $as_echo "$as_me:$LINENO: result: defining option USE_BUTTON_LOGOUT = 10 minutes" >&5
$as_echo "defining option USE_BUTTON_LOGOUT = 10 minutes" >&6; }
  cat >>confdefs.h <<\_ACEOF
#define USE_BUTTON_LOGOUT 10
_ACEOF

  ;;
x[0-9]*|x-0-9*)
  { $as_echo "$as_me:$LINENO: result: defining option USE_BUTTON_LOGOUT = $enableval minutes" >&5
$as_echo "defining option USE_BUTTON_LOGOUT = $enableval minutes" >&6; }
  cat >>confdefs.h <<_ACEOF
#define USE_BUTTON_LOGOUT $enableval
_ACEOF

  ;;
xno*)
  ;;
*)
  { $as_echo "$as_me:$LINENO: result: Warning: Illegal time value \"$use_button_logout\" given" >&5
$as_echo "Warning: Illegal time value \"$use_button_logout\" given" >&6; }
  { $as_echo "$as_me:$LINENO: result: defining option USE_BUTTON_LOGOUT = 10 minutes" >&5
$as_echo "defining option USE_BUTTON_LOGOUT = 10 minutes" >&6; }
  cat >>confdefs.h <<\_ACEOF
#define USE_BUTTON_LOGOUT 10
_ACEOF

  ;;
esac

