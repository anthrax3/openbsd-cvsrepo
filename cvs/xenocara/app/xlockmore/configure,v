  ac_cv_header_minix_config_h=$ac_header_preproc
fi
{ $as_echo "$as_me:$LINENO: result: $ac_cv_header_minix_config_h" >&5
$as_echo "$ac_cv_header_minix_config_h" >&6; }

fi
if test $ac_cv_header_minix_config_h = yes; then
  MINIX=yes
