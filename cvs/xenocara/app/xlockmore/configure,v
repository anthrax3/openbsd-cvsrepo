# Check whether --enable-bitmapdir was given.
if test "${enable_bitmapdir+set}" = set; then
  enableval=$enable_bitmapdir; bitmap_dir=$enableval
else
  bitmap_dir=no
fi

case "x$bitmap_dir" in
x/*|x.*)
  { $as_echo "$as_me:$LINENO: result: setting BITMAPDIR = $enableval" >&5
$as_echo "setting BITMAPDIR = $enableval" >&6; }
  BITMAPDIR=$enableval
  if test ! -d $bitmap_dir; then
    { $as_echo "$as_me:$LINENO: result: Warning: Directory $enableval does not exist" >&5
$as_echo "Warning: Directory $enableval does not exist" >&6; }
  fi
  ;;
*)
  BITMAPDIR='$(top_srcdir)/bitmaps'
  bitmap_dir='$top_srcdir/bitmaps'
  ;;
esac

# Check whether --enable-bitmaptype was given.
if test "${enable_bitmaptype+set}" = set; then
  enableval=$enable_bitmaptype; bitmap_type=$enableval
else
  bitmap_type=no
fi

case "x$bitmap_type" in
xyes*|xno*)
  ;;
*)
  { $as_echo "$as_me:$LINENO: result: setting BITMAPTYPE = $enableval" >&5
$as_echo "setting BITMAPTYPE = $enableval" >&6; }
  BITMAPTYPE=$enableval
  if test ! -f $bitmap_dir/l-$BITMAPTYPE.xbm; then
    { $as_echo "$as_me:$LINENO: result: Warning: Bitmap $BITMAPDIR/l-$BITMAPTYPE.xbm does not exist" >&5
$as_echo "Warning: Bitmap $BITMAPDIR/l-$BITMAPTYPE.xbm does not exist" >&6; }
  fi
  ;;
esac

