    if test "${CC}" = "cc" ; then
      CFLAGS="${CFLAGS} -Aa -z -D_HPUX_SOURCE"
      GPROF_CFLAGS="${GPROF_CFLAGS} -Aa -z -D_HPUX_SOURCE"
    fi
    XLOCKLIBS="-lXhp11 ${XLOCKLIBS}"
    cat >>confdefs.h <<\_ACEOF
