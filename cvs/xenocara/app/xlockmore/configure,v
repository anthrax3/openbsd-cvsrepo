# Check whether --with-krb5-config was given.
if test "${with_krb5_config+set}" = set; then
  withval=$with_krb5_config;
  if test $withval = yes; then
    # Extract the first word of "krb5-config", so it can be a program name with args.
set dummy krb5-config; ac_word=$2
{ $as_echo "$as_me:$LINENO: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if test "${ac_cv_path_krb5_config+set}" = set; then
  $as_echo_n "(cached) " >&6
else
  case $krb5_config in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_krb5_config="$krb5_config" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_krb5_config="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done
IFS=$as_save_IFS

  test -z "$ac_cv_path_krb5_config" && ac_cv_path_krb5_config="not_found"
  ;;
esac
fi
krb5_config=$ac_cv_path_krb5_config
if test -n "$krb5_config"; then
  { $as_echo "$as_me:$LINENO: result: $krb5_config" >&5
$as_echo "$krb5_config" >&6; }
else
  { $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
fi


    if test X$krb5_config = Xnot_found; then
      { { $as_echo "$as_me:$LINENO: error: unable to find krb5-config" >&5
$as_echo "$as_me: error: unable to find krb5-config" >&2;}
   { (exit 1); exit 1; }; }
    fi
  else
    krb5_config=$withval
    if test ! -x $krb5_config; then
      { { $as_echo "$as_me:$LINENO: error: unable to find krb5-config at $krb5_config" >&5
$as_echo "$as_me: error: unable to find krb5-config at $krb5_config" >&2;}
   { (exit 1); exit 1; }; }
    fi
  fi
  krb5_cflags=`$krb5_config --cflags krb5`
  retval=$?
  if test $retval -ne 0; then
    { { $as_echo "$as_me:$LINENO: error: $krb5_config failed with error code of $retval" >&5
$as_echo "$as_me: error: $krb5_config failed with error code of $retval" >&2;}
   { (exit 1); exit 1; }; }
  fi
  krb5_libs=`$krb5_config --libs krb5`
  retval=$?
  if test $retval -ne 0; then
    { { $as_echo "$as_me:$LINENO: error: $krb5_config failed with error code of $retval" >&5
$as_echo "$as_me: error: $krb5_config failed with error code of $retval" >&2;}
   { (exit 1); exit 1; }; }
  fi
  { $as_echo "$as_me:$LINENO: result: Adding $krb5_cflags to XLOCKINC" >&5
$as_echo "Adding $krb5_cflags to XLOCKINC" >&6; }
  { $as_echo "$as_me:$LINENO: result: Adding $krb5_libs to XLOCKLIBS" >&5
$as_echo "Adding $krb5_libs to XLOCKLIBS" >&6; }
  XLOCKINC="$XLOCKINC $krb5_cflags"
  XLOCKLIBS="$XLOCKLIBS $krb5_libs"

fi

# Check whether --enable-write_krb5 was given.
if test "${enable_write_krb5+set}" = set; then
  enableval=$enable_write_krb5; write_krb5=$enableval
else
  write_krb5=no
