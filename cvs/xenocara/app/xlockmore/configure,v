$as_echo "defining option DEF_AUTO_LOGOUT = $enableval minutes" >&6; }
  cat >>confdefs.h <<_ACEOF
#define DEF_AUTO_LOGOUT "$enableval"
_ACEOF

  ;;
xno*)
  ;;
*)
  { $as_echo "$as_me:$LINENO: result: Warning: Illegal time value \"$def_auto_logout\" given" >&5
$as_echo "Warning: Illegal time value \"$def_auto_logout\" given" >&6; }
  { $as_echo "$as_me:$LINENO: result: defining option DEF_AUTO_LOGOUT = 120 minutes" >&5
$as_echo "defining option DEF_AUTO_LOGOUT = 120 minutes" >&6; }
  cat >>confdefs.h <<\_ACEOF
#define DEF_AUTO_LOGOUT "120"
_ACEOF

  ;;
esac

# Check whether --enable-button-logout was given.
if test "${enable_button_logout+set}" = set; then
