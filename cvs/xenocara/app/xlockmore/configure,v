dirent=yes

case "${canonical}" in

  *-*-aix* )
    BITMAPTYPE="ibm"
    PIXMAPTYPE="ibm"
    cat >>confdefs.h <<\_ACEOF
#define AIXV3 1
_ACEOF

    aixv3=yes
    case "${canonical}" in
      *-*-aix2* | *-*-aix3.0* | *-*-aix3.1* )
        cat >>confdefs.h <<\_ACEOF
