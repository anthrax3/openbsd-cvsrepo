case "x$use_auto_logout" in
x|xyes*)
  { $as_echo "$as_me:$LINENO: result: defining option USE_AUTO_LOGOUT = 240 minutes" >&5
$as_echo "defining option USE_AUTO_LOGOUT = 240 minutes" >&6; }
  cat >>confdefs.h <<\_ACEOF
#define USE_AUTO_LOGOUT 240
_ACEOF

  ;;
x[0-9]*)
  { $as_echo "$as_me:$LINENO: result: defining option USE_AUTO_LOGOUT = $enableval minutes" >&5
$as_echo "defining option USE_AUTO_LOGOUT = $enableval minutes" >&6; }
  cat >>confdefs.h <<_ACEOF
#define USE_AUTO_LOGOUT $enableval
_ACEOF

  ;;
xno*)
  ;;
*)
  { $as_echo "$as_me:$LINENO: result: Warning: Illegal time value \"$use_auto_logout\" given" >&5
$as_echo "Warning: Illegal time value \"$use_auto_logout\" given" >&6; }
  { $as_echo "$as_me:$LINENO: result: defining option USE_AUTO_LOGOUT = 240 minutes" >&5
$as_echo "defining option USE_AUTO_LOGOUT = 240 minutes" >&6; }
  cat >>confdefs.h <<\_ACEOF
#define USE_AUTO_LOGOUT 240
_ACEOF

  ;;
esac

