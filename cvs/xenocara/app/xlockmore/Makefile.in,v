@d12 1
a12 1
MAKEIN_SUBDIR = for dir in $(SUBDIRS) ; do ( cd $$dir ; sed s/@@MODULES@@/\#/ Makefile.in > Makefile ; $(MAKE) $@@ ) ; done
@


