		{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if $CC supports -Wold-style-definition" >&5
$as_echo_n "checking if $CC supports -Wold-style-definition... " >&6; }
		cacheid=xorg_cv_cc_flag__Wold_style_definition
