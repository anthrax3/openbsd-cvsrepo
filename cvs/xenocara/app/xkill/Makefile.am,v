SUBDIRS = man
bin_PROGRAMS = xkill

AM_CFLAGS = $(CWARNFLAGS) $(XKILL_CFLAGS)
xkill_LDADD = $(XKILL_LIBS)

xkill_SOURCES =	\
        xkill.c

MAINTAINERCLEANFILES = ChangeLog INSTALL

.PHONY: ChangeLog INSTALL

INSTALL:
	$(INSTALL_CMD)

ChangeLog:
	$(CHANGELOG_CMD)

dist-hook: ChangeLog INSTALL
@


