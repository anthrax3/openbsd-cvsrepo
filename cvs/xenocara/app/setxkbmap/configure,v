$as_echo "$xorg_cv_cc_flag_unknown_warning_option" >&6; }
	xorg_testset_cc_unknown_warning_option=$xorg_cv_cc_flag_unknown_warning_option
	CFLAGS="$xorg_testset_save_CFLAGS"
fi

