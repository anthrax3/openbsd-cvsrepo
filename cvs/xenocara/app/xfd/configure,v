# Checks for pkg-config packages

pkg_failed=no
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for XFD" >&5
$as_echo_n "checking for XFD... " >&6; }

if test -n "$XFD_CFLAGS"; then
    pkg_cv_XFD_CFLAGS="$XFD_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"xaw7 fontconfig xft xrender xmu xproto >= 7.0.17\""; } >&5
  ($PKG_CONFIG --exists --print-errors "xaw7 fontconfig xft xrender xmu xproto >= 7.0.17") 2>&5
  ac_status=$?
  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_XFD_CFLAGS=`$PKG_CONFIG --cflags "xaw7 fontconfig xft xrender xmu xproto >= 7.0.17" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
if test -n "$XFD_LIBS"; then
    pkg_cv_XFD_LIBS="$XFD_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"xaw7 fontconfig xft xrender xmu xproto >= 7.0.17\""; } >&5
  ($PKG_CONFIG --exists --print-errors "xaw7 fontconfig xft xrender xmu xproto >= 7.0.17") 2>&5
  ac_status=$?
  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_XFD_LIBS=`$PKG_CONFIG --libs "xaw7 fontconfig xft xrender xmu xproto >= 7.0.17" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi



if test $pkg_failed = yes; then
   	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        XFD_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors --cflags --libs "xaw7 fontconfig xft xrender xmu xproto >= 7.0.17" 2>&1`
        else
	        XFD_PKG_ERRORS=`$PKG_CONFIG --print-errors --cflags --libs "xaw7 fontconfig xft xrender xmu xproto >= 7.0.17" 2>&1`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$XFD_PKG_ERRORS" >&5

	as_fn_error $? "Package requirements (xaw7 fontconfig xft xrender xmu xproto >= 7.0.17) were not met:

$XFD_PKG_ERRORS

Consider adjusting the PKG_CONFIG_PATH environment variable if you
installed software in a non-standard prefix.

Alternatively, you may set the environment variables XFD_CFLAGS
and XFD_LIBS to avoid the need to call pkg-config.
See the pkg-config man page for more details." "$LINENO" 5
elif test $pkg_failed = untried; then
     	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
	{ { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "The pkg-config script could not be found or is too old.  Make sure it
is in your PATH or set the PKG_CONFIG environment variable to the full
path to pkg-config.

Alternatively, you may set the environment variables XFD_CFLAGS
and XFD_LIBS to avoid the need to call pkg-config.
See the pkg-config man page for more details.

To get pkg-config, see <http://pkg-config.freedesktop.org/>.
See \`config.log' for more details" "$LINENO" 5; }
else
	XFD_CFLAGS=$pkg_cv_XFD_CFLAGS
	XFD_LIBS=$pkg_cv_XFD_LIBS
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }

fi


pkg_failed=no
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for APPDEFS" >&5
$as_echo_n "checking for APPDEFS... " >&6; }

