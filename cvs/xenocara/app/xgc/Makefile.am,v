AM_YFLAGS = -d

SUBDIRS = man
bin_PROGRAMS = xgc

AM_CFLAGS = $(CWARNFLAGS) $(XGC_CFLAGS) -D_BSD_SOURCE
xgc_LDADD = $(XGC_LIBS) -lm

xgc_SOURCES =	\
        choice.c \
        constants.h \
        dashlist.c \
        getfile.c \
	gram.y \
        interpret.c \
	lex.l \
        main.c \
        planemask.c \
        record.c \
        testfrac.c \
        tests.c \
        text.c \
        xgc.h

BUILT_SOURCES = gram.h
MAINTAINERCLEANFILES = ChangeLog INSTALL

# App default files

DISTCHECK_CONFIGURE_FLAGS = --with-appdefaultdir=\$${datadir}/X11/app-defaults

appdefaultdir = @@appdefaultdir@@

dist_appdefault_DATA = \
        app-defaults/Xgc \
        app-defaults/Xgc-color

EXTRA_DIST = tile Bugs	\
	Written/FilledRects						\
	Written/Interface						\
	Written/Jim							\
	Written/Notes							\
	Written/Notes2							\
	Written/Outline							\
	Written/Widget

.PHONY: ChangeLog INSTALL

INSTALL:
	$(INSTALL_CMD)

ChangeLog:
	$(CHANGELOG_CMD)

dist-hook: ChangeLog INSTALL
