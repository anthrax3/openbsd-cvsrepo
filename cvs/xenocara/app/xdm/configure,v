{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if the greeter should be built as a dynamically loaded object" >&5
$as_echo_n "checking if the greeter should be built as a dynamically loaded object... " >&6; }

# Check whether --enable-dynamic-greeter was given.
if test "${enable_dynamic_greeter+set}" = set; then :
  enableval=$enable_dynamic_greeter; DYNAMIC_GREETER="$enableval"
else
  DYNAMIC_GREETER="auto"
fi


if test "x$DYNAMIC_GREETER" = "xauto" ; then
	case $host_os in
	openbsd*)
		DYNAMIC_GREETER="no"
		;;
	    *)	DYNAMIC_GREETER="yes" ;;

	esac
fi
if test "x$DYNAMIC_GREETER" = "xno"; then
	STATIC_GREETER_CFLAGS="-DGREET_USER_STATIC"
fi

{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $DYNAMIC_GREETER" >&5
$as_echo "$DYNAMIC_GREETER" >&6; }
 if test x$DYNAMIC_GREETER = xyes; then
  DYNAMIC_GREETER_TRUE=
  DYNAMIC_GREETER_FALSE='#'
else
  DYNAMIC_GREETER_TRUE='#'
  DYNAMIC_GREETER_FALSE=
fi



