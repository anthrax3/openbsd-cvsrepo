# Defaults for X session - should probably be configure --with-* flags
# Currently just trying to mirror what defaults were set in Imake

# -DDEF_SERVER_LINE=":0 local $(DEF_SERVER) :0"
if test -z "$DEF_SERVER" ; then
	DEF_SERVER="${bindir}/X"
fi
if test -z "$DEF_SERVER_LINE" ; then
	DEF_SERVER_LINE=":0 local ${DEF_SERVER} :0"
fi

  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$DEF_SERVER_LINE\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_SERVER_LINE="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define DEF_SERVER_LINE "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DXRDB_PROGRAM="$(BINDIR)/xrdb"

# Check whether --with-xrdb was given.
if test "${with_xrdb+set}" = set; then :
  withval=$with_xrdb; XRDB=$withval
else
  XRDB=${bindir}/xrdb
fi


  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$XRDB\""
  eval ax_define_dir="\"$ax_define_dir\""
  XRDB_PROGRAM="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define XRDB_PROGRAM "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DDEF_SESSION="$(BINDIR)/xterm -ls"

# Check whether --with-default-session was given.
if test "${with_default_session+set}" = set; then :
  withval=$with_default_session; DEF_SESSION=$withval
else
  DEF_SESSION="${bindir}/xterm -ls"
fi


  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$DEF_SESSION\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_SESSION="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define DEF_SESSION "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DDEF_USER_PATH="$(DEF_USER_PATH)"
# Default values taken from DefaultUserPath setting in the
# old Imake *.cf files for each platform
if test -z "$DEF_USER_PATH" ; then
	case $host_os in
	    dragonfly*)
		DEF_USER_PATH="/bin:/usr/bin:/sbin:/usr/sbin:${bindir}:/usr/local/bin"
		;;
	    freebsd*)
		DEF_USER_PATH="/bin:/usr/bin:/sbin:/usr/sbin:${bindir}:/usr/gnu/bin:/usr/local/bin"
		;;
	    netbsd*)
		DEF_USER_PATH="/bin:/usr/bin:/usr/pkg/bin:/usr/local/bin:${bindir}"
		;;
	    openbsd*)
		DEF_USER_PATH="/bin:/usr/bin:/sbin:/usr/sbin:${bindir}:/usr/local/bin:/usr/local/sbin"
		;;
	    bsdi*)
		DEF_USER_PATH="/bin:/usr/bin:/usr/local/bin:${bindir}"
		;;
	    solaris*)
		DEF_USER_PATH="/usr/bin:${bindir}"
		;;
	    *)
		DEF_USER_PATH="/bin:/usr/bin:${bindir}:/usr/ucb"
		;;
	esac
fi

  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$DEF_USER_PATH\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_USER_PATH="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define DEF_USER_PATH "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DDEF_SYSTEM_PATH="$(DEF_SYSTEM_PATH)"
# Default values taken from DefaultSystemPath setting in the
# old Imake *.cf files for each platform
if test -z "$DEF_SYSTEM_PATH" ; then
	case $host_os in
	    freebsd*|dragonfly*)
		DEF_SYSTEM_PATH="/bin:/usr/bin:/sbin:/usr/sbin:${bindir}"
		;;
	    netbsd*|openbsd*|bsdi*)
		DEF_SYSTEM_PATH="/sbin:/usr/sbin:/bin:/usr/bin:${bindir}"
		;;
	    solaris*)
		DEF_SYSTEM_PATH="/sbin:/usr/sbin:/usr/bin:${bindir}"
		;;
	    *)
		DEF_SYSTEM_PATH="/etc:/bin:/usr/bin:${bindir}:/usr/ucb"
		;;
	esac
fi

  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$DEF_SYSTEM_PATH\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_SYSTEM_PATH="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define DEF_SYSTEM_PATH "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DDEF_SYSTEM_SHELL="$(BOURNE_SHELL)"
if test -z "$BOURNE_SHELL" ; then
	BOURNE_SHELL=/bin/sh
fi

  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$BOURNE_SHELL\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_SYSTEM_SHELL="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define DEF_SYSTEM_SHELL "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DDEF_FAILSAFE_CLIENT="$(BINDIR)/xterm"


  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$bindir/xterm\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_FAILSAFE_CLIENT="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define DEF_FAILSAFE_CLIENT "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DDEF_XDM_CONFIG="$(XDMDIR)/xdm-config"


  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$XDMCONFIGDIR/xdm-config\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_XDM_CONFIG="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define DEF_XDM_CONFIG "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DDEF_CHOOSER="$(CHOOSERPATH)"


  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$XDMLIBDIR/chooser\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_CHOOSER="$ax_define_dir"


cat >>confdefs.h <<_ACEOF
#define DEF_CHOOSER "$ax_define_dir"
_ACEOF

  test "$prefix_NONE" && prefix=NONE
  test "$exec_prefix_NONE" && exec_prefix=NONE


# -DDEF_AUTH_DIR="$(XDMDIR)"


# Check whether --with-authdir was given.
if test "${with_authdir+set}" = set; then :
  withval=$with_authdir; XDMXAUTHDIR="$withval"
else
  XDMXAUTHDIR="/var/lib/xdm"
fi




  prefix_NONE=
  exec_prefix_NONE=
  test "x$prefix" = xNONE && prefix_NONE=yes && prefix=$ac_default_prefix
  test "x$exec_prefix" = xNONE && exec_prefix_NONE=yes && exec_prefix=$prefix
  eval ax_define_dir="\"$XDMXAUTHDIR\""
  eval ax_define_dir="\"$ax_define_dir\""
  DEF_AUTH_DIR="$ax_define_dir"


