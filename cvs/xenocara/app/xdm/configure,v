    pkg_cv_XDMGREET_CFLAGS="$XDMGREET_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"xt >= 1.0 x11 xext\""; } >&5
  ($PKG_CONFIG --exists --print-errors "xt >= 1.0 x11 xext") 2>&5
