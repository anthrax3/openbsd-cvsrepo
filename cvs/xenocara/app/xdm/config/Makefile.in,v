	for dir in "$(DESTDIR)$(xdmscriptdir)" "$(DESTDIR)$(xdmscriptdir)" "$(DESTDIR)$(pixmapdir)" "$(DESTDIR)$(xdmconfigdir)" "$(DESTDIR)$(xdmconfigdir)"; do \
	  test -z "$$dir" || $(MKDIR_P) "$$dir"; \
	done
