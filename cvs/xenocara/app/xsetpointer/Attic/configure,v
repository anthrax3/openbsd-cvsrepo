s,@@host_vendor@@,$host_vendor,;t t
s,@@host_os@@,$host_os,;t t
s,@@APP_MAN_SUFFIX@@,$APP_MAN_SUFFIX,;t t
s,@@LIB_MAN_SUFFIX@@,$LIB_MAN_SUFFIX,;t t
s,@@FILE_MAN_SUFFIX@@,$FILE_MAN_SUFFIX,;t t
