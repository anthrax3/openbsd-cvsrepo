s,@@LIB_MAN_DIR@@,$LIB_MAN_DIR,;t t
s,@@FILE_MAN_DIR@@,$FILE_MAN_DIR,;t t
s,@@MISC_MAN_DIR@@,$MISC_MAN_DIR,;t t
s,@@DRIVER_MAN_DIR@@,$DRIVER_MAN_DIR,;t t
