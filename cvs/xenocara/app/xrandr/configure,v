    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"xrandr >= 1.4 xrender x11 xproto >= 7.0.17\""; } >&5
  ($PKG_CONFIG --exists --print-errors "xrandr >= 1.4 xrender x11 xproto >= 7.0.17") 2>&5
