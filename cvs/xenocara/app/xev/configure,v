// Check varargs and va_copy.
static void
test_varargs (const char *format, ...)
{
  va_list args;
  va_start (args, format);
  va_list args_copy;
  va_copy (args_copy, args);
d3240 11
a3250 24
  const char *str;
  int number;
  float fnumber;

  while (*format)
    {
      switch (*format++)
	{
