	@@rm -f xmodmap$(EXEEXT)
	$(AM_V_CCLD)$(LINK) $(xmodmap_OBJECTS) $(xmodmap_LDADD) $(LIBS)

mostlyclean-compile:
	-rm -f *.$(OBJEXT)

distclean-compile:
	-rm -f *.tab.c

