/* #undef NO_ACTIVE_ICON */		/* CF_ARG_DISABLE(active-icon) */
/* #undef NO_LEAKS */			/* CF_ARG_DISABLE(leaks) */
#define OPT_256_COLORS 1		/* CF_ARG_ENABLE(256-color) */
