	done
	;;
(*)
	for cf_regex_hdr in regex.h
	do
		cat >conftest.$ac_ext <<_ACEOF
#line 18251 "configure"
#include "confdefs.h"
#include <sys/types.h>
#include <$cf_regex_hdr>
int
main ()
{

			regex_t *p;
