if test "$cf_cv_have_utempter" = yes ; then

cat >>confdefs.h <<\EOF
#define USE_UTEMPTER 1
EOF

cf_add_libs="-lutempter"
# Filter out duplicates - this happens with badly-designed ".pc" files...
for cf_add_1lib in $LIBS
