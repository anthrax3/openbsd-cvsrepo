  fi
else
  enableval=yes
  cf_disable_rpath_hack=no

fi;
echo "$as_me:20797: result: $cf_disable_rpath_hack" >&5
echo "${ECHO_T}$cf_disable_rpath_hack" >&6
if test "$cf_disable_rpath_hack" = no ; then

