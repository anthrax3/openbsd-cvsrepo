# Check whether --enable-desktop or --disable-desktop was given.
if test "${enable_desktop+set}" = set; then
  enableval="$enable_desktop"
  test "$enableval" != no && enableval=yes
  if test "$enableval" != "yes" ; then
    enable_desktop=$enableval
  else
    enable_desktop=$enableval
  fi
