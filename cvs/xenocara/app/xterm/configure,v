    ECHO_LD=''
    RULE_CC=''
    SHOW_CC=''
    ECHO_CC=''

  fi
else
  enableval=yes

    ECHO_LT=''
    ECHO_LD=''
    RULE_CC=''
    SHOW_CC=''
    ECHO_CC=''

fi;
echo "$as_me:19910: result: $enableval" >&5
