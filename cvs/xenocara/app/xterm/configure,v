         { ac_try='test -s conftest$ac_exeext'
  { (eval echo "$as_me:7019: \"$ac_try\"") >&5
  (eval $ac_try) 2>&5
  ac_status=$?
  echo "$as_me:7022: \$? = $ac_status" >&5
