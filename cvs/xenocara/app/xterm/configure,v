rm -f conftest.$ac_objext conftest$ac_exeext
if { (eval echo "$as_me:16223: \"$ac_link\"") >&5
  (eval $ac_link) 2>&5
  ac_status=$?
  echo "$as_me:16226: \$? = $ac_status" >&5
  (exit $ac_status); } &&
         { ac_try='test -s conftest$ac_exeext'
  { (eval echo "$as_me:16229: \"$ac_try\"") >&5
