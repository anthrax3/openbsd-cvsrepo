#ifdef USE_OK_BCOPY
#define memmove(d,s,n) bcopy(s,d,n)
#endif

#ifdef USE_MY_MEMMOVE
#define memmove(d,s,n) my_memmove(d,s,n)
extern void * my_memmove(void * s1, void * s2, size_t n);
#endif

#ifndef HAVE_STRERROR
#define strerror(n) my_strerror(n)
