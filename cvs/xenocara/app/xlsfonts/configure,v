SHELL'
ac_subst_files=''
ac_user_opts='
enable_option_checking
enable_maintainer_mode
enable_dependency_tracking
enable_selective_werror
enable_strict_compilation
enable_silent_rules
'
      ac_precious_vars='build_alias
host_alias
target_alias
CC
