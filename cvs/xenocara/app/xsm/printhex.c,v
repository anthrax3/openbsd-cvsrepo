    "f8", "f9", "fa", "fb", "fc", "fd", "fe", "ff", 
};


void
fprintfhex(register FILE *fp, unsigned int len, char *cp)
{
    unsigned char *ucp = (unsigned char *) cp;

    for (; len > 0; len--, ucp++)
    {
	register const char *s = hex_table[*ucp];
	putc (s[0], fp);
	putc (s[1], fp);
    }
}
