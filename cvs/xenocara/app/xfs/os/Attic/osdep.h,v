#ifndef OPEN_MAX
#if defined(__UNIXOS2__) || defined(__QNX__)
#define OPEN_MAX 256
#else
#ifdef SVR4
#define OPEN_MAX 128
#else
#include <sys/param.h>
#ifdef __GNU__
#define OPEN_MAX (sysconf(_SC_OPEN_MAX))
#endif /*__GNU__*/
#ifndef OPEN_MAX
#ifdef SCO325
#define OPEN_MAX (sysconf(_SC_OPEN_MAX))
#else
#if defined(NOFILE) && !defined(NOFILES_MAX)
#define OPEN_MAX NOFILE
#else
#define OPEN_MAX NOFILES_MAX
#endif
#endif
#endif
#endif
#endif
#endif
