SUBDIRS=man
bin_PROGRAMS = showrgb

showrgb_SOURCES =  showrgb.c

if RGB_DB
bin_PROGRAMS += rgb

rgb_SOURCES = rgb.c

$(RGB_DB_FILES): rgb rgb.txt
	-rm -f $(RGB_DB_FILES)
	./rgb rgb < $(srcdir)/rgb.txt
endif

rgbdir = $(db_dir)
rgb_DATA = $(db_file).txt $(RGB_DB_FILES)
RGB_DB_DEFINES = -DRGB_DB=\"$(db_dir)/$(db_file)\"
AM_CFLAGS = $(RGB_CFLAGS) $(RGB_DB_DEFINES) $(CWARNFLAGS)

EXTRA_DIST= \
	rgb.txt \
	others/README \
	others/old-rgb.txt \
	others/raveling.txt \
	others/thomas.txt

MAINTAINERCLEANFILES = ChangeLog INSTALL

.PHONY: ChangeLog INSTALL

INSTALL:
	$(INSTALL_CMD)

ChangeLog:
	$(CHANGELOG_CMD)

dist-hook: ChangeLog INSTALL
@


