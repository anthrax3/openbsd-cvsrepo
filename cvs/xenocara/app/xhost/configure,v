fi


 # POSIX.1g changed the type of pointer passed to getsockname/getpeername/etc.
 ac_fn_c_check_type "$LINENO" "socklen_t" "ac_cv_type_socklen_t" "
$ac_includes_default
#include <sys/socket.h>
"
if test "x$ac_cv_type_socklen_t" = xyes; then :

