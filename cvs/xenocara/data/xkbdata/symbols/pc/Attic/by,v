    // End alphanumeric section
};

partial alphanumeric_keys
xkb_symbols "winkeys" {
    include "pc/by(basic)"
    name[Group1]= "Belarus - Winkeys";
    key	<AE04> {	[		4,       semicolon	]	};
    key	<AE03> {	[		3,      numbersign	]	};
    key	<AE05> {	[		5,	   percent	]	};
    key	<AE06> {	[		6,	     colon	]	};
    key	<AE07> {	[		7,	  question	]	};
    key	<AE08> {	[		8,	  asterisk	]	};
    key	<AB10> {	[	   period,	     comma	]	};
    key	<BKSL> {	[	    slash,             bar	]	};
};
