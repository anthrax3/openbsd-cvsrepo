    key <AD08> {	[	    i,	I		],
                	[ icircumflex,	idiaeresis      ] 	};
    key <AD09> {	[	    o,	O		]	};
    key <AD10> {	[	    p,	P		]	};
    key <AD11> {	[      igrave,  asciicircum     ]	};
    key <AD12> {	[      dollar,	asterisk        ]	};
