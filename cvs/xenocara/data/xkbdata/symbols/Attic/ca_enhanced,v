		};

    key <AD01> {
		symbols[Group1]= [ q, Q ],
		symbols[Group2]= [ oslash, Ooblique ]
		};
    key <AD04> {
		symbols[Group1]= [ r, R ],
		symbols[Group2]= [ thorn, Thorn ]
		};
    key <AD09> {
