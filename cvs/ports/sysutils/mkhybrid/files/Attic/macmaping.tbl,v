#	"Ujf " is created by Netscape
.html	Ascii	'Ujf '	  'TEXT'   "WWW Hypertext Document"
.htm	Ascii	'Ujf '	  'TEXT'   "WWW Hypertext Document"
*	Ascii	'ttxt'	  'TEXT'   "Text file"
@


