1.4
log
@- cc -> ${CC}
- ${COPY} -> {INSTALL_COPY}, ${STRIP} -> ${INSTALL_STRIP}
@
text
@d1 1
a1 1
# $OpenBSD: Makefile,v 1.3 1999/04/01 02:16:38 brad Exp $
d3 2
a4 2
DISTNAME=       ha0999
PKGNAME=        ha-0.999b
d6 2
a7 2
MASTER_SITES=   ftp://ftp.kiarchive.ru/pub/unix/arcers/
EXTRACT_SUFX=   .tgz
d9 1
a9 1
MAINTAINER=     angelos@@openbsd.org
d11 1
a11 1
USE_GMAKE=      yes
d13 3
a15 4
NO_WRKSUBDIR=   yes
MAKEFILE=       makefile.nix
MAKE_FLAGS=     CC=${CC} -f
ALL_TARGET=     ha
d18 1
a18 1
	@@cd ${WRKSRC}; ${INSTALL} ${INSTALL_COPY} ${INSTALL_STRIP} -o ${BINOWN} -g ${BINGRP} -m ${BINMODE} ha ${PREFIX}/bin
@


