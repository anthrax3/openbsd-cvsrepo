@$OpenBSD$
--- server/util.c.orig	Wed Nov  6 05:09:47 1996
+++ server/util.c	Wed Mar 12 14:59:07 2003
@@@@ -218,4 +218,17 @@@@ Ushort *ws;
 }
 
+const Ushort *
+ushortmemchr(ws, ch, len)
+const Ushort *ws;
+int ch;
+size_t len;
+{
+  const Ushort *p, *end;
+  for (p = ws, end = ws + len; p < end; ++p)
+      if (*p == (Ushort)ch)
+	  return p;
+  return NULL;
+}
+
 int
 ushortstrcpy(wd, ws)
