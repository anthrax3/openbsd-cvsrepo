@@@@ -78,6 +78,15 @@@@ static char THIS_FILE[]=__FILE__;
 #define new DEBUG_NEW
 #endif
 
+#ifdef __OpenBSD__
+size_t strnlen(const char *string, size_t maxlen)
+{
+	size_t i;
+	for (i= 0; i < maxlen && *string != '\0'; i++, string++);
+	return i;
+}
+#endif
+
