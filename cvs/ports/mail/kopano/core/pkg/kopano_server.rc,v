@#!/bin/sh
#
# $OpenBSD: kopano_server.rc,v 1.7 2016/04/01 18:08:10 robert Exp $

daemon="${TRUEPREFIX}/sbin/kopano-server"

. /etc/rc.d/rc.subr

rc_pre() {
	install -d -o _kopano -g _kopano /var/run/kopano
}

rc_cmd $1
@


