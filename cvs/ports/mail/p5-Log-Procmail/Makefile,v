MAINTAINER=		Anil Madhavapeddy <avsm@@openbsd.org>

# GPL/Artistic
PERMIT_PACKAGE_CDROM=	Yes

MODULES=		cpan
PKG_ARCH=		*

.include <bsd.port.mk>
@


