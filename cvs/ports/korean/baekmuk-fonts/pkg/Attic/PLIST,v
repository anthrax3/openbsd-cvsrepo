@@exec /usr/X11R6/bin/mkfontdir %B || true
@@unexec cat %B/fonts.alias-* >%B/fonts.alias
@@unexec /usr/X11R6/bin/mkfontdir %B || true
@@unexec echo "Remove %B manually if it's now empty"
