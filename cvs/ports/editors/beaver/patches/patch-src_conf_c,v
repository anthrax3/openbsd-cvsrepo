     for (j = 0; conf->file_content[i][j]; j++)
       if (conf->file_content[i][j] == COMMENT &&
-	  (conf->file_content[i][j - 1] != '\\' || !j))
+	  (!j || conf->file_content[i][j - 1] != '\\'))
