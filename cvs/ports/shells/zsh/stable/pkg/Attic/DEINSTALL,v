1.3
log
@nuke stable/snapshot trees
@
text
@#!/bin/sh
# $OpenBSD: DEINSTALL,v 1.2 2003/07/03 12:56:52 lebel Exp $
#
# zsh de-installation

set -e
PATH=/bin:/usr/bin:/sbin:/usr/sbin
PREFIX=${PKG_PREFIX:-/usr/local}

if grep -q ${PREFIX}/bin/zsh /etc/shells; then
    echo
    echo "+---------------"
    echo "| To completely deinstall the $1 package you need to edit"
    echo "| /etc/shells and remove this line:"
    echo "|"
    echo "|           ${PREFIX}/bin/zsh"
    echo "|"
    echo "+---------------"
    echo
fi

exit 0
@


