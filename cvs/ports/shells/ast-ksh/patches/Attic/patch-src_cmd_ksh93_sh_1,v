 .if \nZ=0 .TH SH 1
-.if \nZ=1 .TH KSH 1 "User Environment Utilities" "RDS Standard"
+.TH KSH 1 "User Environment Utilities" "RDS Standard"
 .SH NAME
 .if \nZ=0 sh, rsh, pfsh \- shell, the
