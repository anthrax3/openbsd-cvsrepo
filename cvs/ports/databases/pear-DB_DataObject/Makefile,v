PKGNAME=	pear-${DISTNAME}# does not substitute _ for -
CATEGORIES=	databases

MODULES=	www/pear

# PHP License
PERMIT_PACKAGE_CDROM=	Yes

RUN_DEPENDS =	databases/pear-MDB2 \
		devel/pear-Date

