@# $OpenBSD: Makefile,v 1.16 2016/03/20 19:56:11 naddy Exp $

COMMENT =	perl binding for Redis database

DISTNAME =	Redis-1.991
CATEGORIES =	databases

EPOCH =		0

MAINTAINER =	Benoit Lecocq <benoit@@openbsd.org>

# Perl
PERMIT_PACKAGE_CDROM =	Yes

MODULES =		cpan
PKG_ARCH =		*

