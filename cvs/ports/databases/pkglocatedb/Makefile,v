DISTNAME =		pkglocatedb-1.0
a25 1
USE_X11 = Yes
d28 1
a28 15
ARGS = -u -a -p ${PORTSDIR}

SRCDB = /usr/lib/locate/src.db
XDB = ${X11BASE}/lib/locate/xorg.db

post-extract:
	@@status=0; if !  test -f ${SRCDB}; then \
		echo "Error ${SRCDB} missing"; \
		status=1; \
	fi; \
	if !  test -f ${XDB}; then \
		echo "Error ${XDB} missing"; \
		status=1;\
	fi; \
	exit $${status}
d51 1
a51 2
	unset REPORT_PROBLEM; 
	locate -d ${SRCDB} -d ${XDB} : | \
