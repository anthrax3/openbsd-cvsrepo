1.11
log
@Fix linking in libzrtp

REVISION bump in baresip to force update
@
text
@# $OpenBSD: Makefile,v 1.10 2017/06/13 07:57:18 czarkoff Exp $

COMMENT =		ZRTP library

REVISION =		0
DISTNAME =		libzrtp-0.20170606
CATEGORIES =		telephony security

MAINTAINER =		Dmitrij D. Czarkoff <czarkoff@@openbsd.org>

# AGPLv3
PERMIT_PACKAGE_CDROM =	Yes

GH_ACCOUNT =		juha-h
GH_PROJECT =		libzrtp
GH_COMMIT =		edf1d3ecece9e01ff10dadc176a9ea57aaa7e53b

BUILD_DEPENDS =		${MODGNU_AUTOCONF_DEPENDS} \
			${MODGNU_AUTOMAKE_DEPENDS} \
			devel/libtool
CONFIGURE_STYLE =	gnu
CONFIGURE_ARGS =	--disable-docs
AUTOCONF_VERSION =	2.69
AUTOMAKE_VERSION =	1.9
.include <bsd.port.arch.mk>
.if ${PROPERTIES:Mclang}
MAKE_FLAGS += GCCMAGIC2=-lcompiler_rt
.endif

pre-configure:
	@@cd ${WRKSRC} && AUTOCONF_VERSION=${AUTOCONF_VERSION} \
			 AUTOMAKE_VERSION=${AUTOMAKE_VERSION} \
			 ./bootstrap.sh

.include <bsd.port.mk>
@


