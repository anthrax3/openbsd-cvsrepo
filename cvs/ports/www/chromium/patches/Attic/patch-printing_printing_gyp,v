+        ['(OS=="linux" or os_bsd ==1) and chromeos==0', {
           'sources': [
             'printing_context_linux.cc',
             'printing_context_linux.h',
@@@@ -279,6 +279,12 @@@@
               ['os_bsd==1', {
                 'cflags': [
                   '<!@@(python cups_config_helper.py --cflags <(sysroot))',
+                ],
+              }],
