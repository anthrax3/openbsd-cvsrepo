text
@#!/bin/sh
#
# $OpenBSD: tinyproxy.rc,v 1.1 2012/06/22 13:26:59 ajacoutot Exp $

daemon="${TRUEPREFIX}/sbin/tinyproxy"

. /etc/rc.d/rc.subr

rc_pre() {
	install -d -o _tinyproxy -g _tinyproxy /var/run/tinyproxy
}

rc_cmd $1
@


