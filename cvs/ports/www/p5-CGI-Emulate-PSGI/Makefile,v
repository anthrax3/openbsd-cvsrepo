CATEGORIES =	www

MAINTAINER =	Andrew Fresh <afresh1@@openbsd.org>

# Perl
PERMIT_PACKAGE_CDROM =	Yes

RUN_DEPENDS =	www/p5-HTTP-Message
TEST_DEPENDS =	devel/p5-Test-Requires>=0.08

# Additional dependencies to avoid warnings and skipping test
