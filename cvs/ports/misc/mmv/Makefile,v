	 ${INSTALL_PROGRAM} ${WRKSRC}/mmv ${PREFIX}/bin ; \
	 ln -fs ${PREFIX}/bin/mmv ${PREFIX}/bin/mad ; \
	 ln -fs ${PREFIX}/bin/mmv ${PREFIX}/bin/mcp ; \
	 ln -fs ${PREFIX}/bin/mmv ${PREFIX}/bin/mln ; \
	 ${INSTALL_MAN} ${WRKSRC}/mmv.1 ${PREFIX}/man/man1
