-MKDEP=		@@echo "	==> MAKING DEPENDENCIES \"$@@\""; $(RMDEP); gcc -M
+MKDEP=		@@echo "	==> MAKING DEPENDENCIES \"$@@\""; $(RMDEP); $(CCOM) -M
