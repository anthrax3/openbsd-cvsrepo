@@exec if [ ! -d /etc/samba ]; then mkdir -p /etc/samba; fi
@@exec if [ ! -f /etc/samba/smbpasswd ]; then chown -R root.wheel /etc/samba; cat /etc/passwd | %D/bin/mksmbpasswd.sh > /etc/samba/smbpasswd; chmod 600 /etc/samba/smbpasswd; fi
@@exec if [ ! -f /etc/samba/smb.conf.default ]; then cp -f %D/share/examples/samba/smb.conf.default /etc/samba; fi
@@exec mkdir -p /var/spool/samba; chmod 1777 /var/spool/samba
@@exec more %D/share/doc/samba/README.OpenBSD
@@unexec rm -f /etc/samba/smb.conf.default
@@unexec echo "Warning: If you will *NOT* use this package anymore, please remove /etc/samba manually."
@@unexec rm -rf /var/spool/samba
