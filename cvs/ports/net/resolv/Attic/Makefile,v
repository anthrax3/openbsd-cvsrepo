NO_REGRESS=		Yes

do-install:
	${INSTALL_PROGRAM} ${WRKSRC}/resolv ${PREFIX}/bin
	${INSTALL_DATA_DIR} ${PREFIX}/share/doc/resolv
	${INSTALL_DATA} ${WRKSRC}/README ${PREFIX}/share/doc/resolv
	${INSTALL_DATA} ${WRKSRC}/LEEME ${PREFIX}/share/doc/resolv
