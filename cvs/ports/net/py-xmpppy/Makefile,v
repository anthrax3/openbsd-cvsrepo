@# $OpenBSD: Makefile,v 1.5 2013/03/11 11:35:55 espie Exp $

COMMENT =		Python XMPP and Jabber implementation

MODPY_EGG_VERSION =	0.5.0rc1
DISTNAME =		xmpppy-${MODPY_EGG_VERSION}
PKGNAME =		py-${DISTNAME}
REVISION =		3
CATEGORIES =		net devel

