log
@Remove some needless HOMEPAGE assignments for GitHub projects.

With input from ajacoutot, sthen.
@
text
@# $OpenBSD: Makefile,v 1.2 2015/11/29 14:39:53 benoit Exp $

COMMENT=	rabbitmq C library and commandline AMQP utilities

V= 		0.7.1
GH_ACCOUNT=	alanxz
GH_PROJECT=	rabbitmq-c
GH_TAGNAME=	v${V}
DISTNAME=	${GH_PROJECT}-${V}

SHARED_LIBS =	rabbitmq 0.0 # 4.1

CATEGORIES=	net

MAINTAINER=	Gregor Best <gbe@@unobtanium.de>

# MIT
PERMIT_PACKAGE_CDROM =	Yes

MODULES=	devel/cmake

WANTLIB=	c crypto popt pthread ssl

LIB_DEPENDS=	devel/popt
BUILD_DEPENDS=	textproc/xmlto

CONFIGURE_ARGS=	-DBUILD_TOOLS_DOCS=ON \
		-DCMAKE_DISABLE_FIND_PACKAGE_Doxygen=True \
		-DBUILD_API_DOCS=OFF

.include <bsd.port.mk>
@


