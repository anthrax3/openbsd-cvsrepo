USE_GMAKE=	yes

do-install:
		${INSTALL_PROGRAM} ${WRKSRC}/bin/xicq ${PREFIX}/bin
		${MKDIR} ${PREFIX}/share/examples/xicq
.for file in contact config
		${INSTALL_DATA} ${WRKSRC}/${file} ${PREFIX}/share/examples/xicq
.endfor

.include <bsd.port.mk>
