@don't pickup protobuf; spotted by nigel@@
@
text
@# $OpenBSD: Makefile,v 1.22 2016/09/10 13:03:42 ajacoutot Exp $

COMMENT=	implementation of Gadu-Gadu IM protocol

VER=		1.12.1
DISTNAME=	libgadu-${VER}
EPOCH=		0
CATEGORIES=	net
MASTER_SITES=	http://github.com/wojtekka/libgadu/releases/download/${VER}/
REVISION=	0

SHARED_LIBS=	gadu	7.0

HOMEPAGE=	http://libgadu.net/

# LGPLv2.1
PERMIT_PACKAGE_CDROM=	Yes

WANTLIB=	pthread z

USE_GMAKE=	Yes
CONFIGURE_STYLE=gnu
CONFIGURE_ARGS=	--disable-tests \
		--without-bind \
		--without-gnutls \
		--without-openssl \
		--without-protobuf
CONFIGURE_ENV=	ac_cv_prog_DOXYGEN=no

NO_TEST=	Yes

.include <bsd.port.mk>
@


