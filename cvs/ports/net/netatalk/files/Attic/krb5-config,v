@Unbreak with new Heimdal; get rid of self-cooked krb5-config since we
have it in base now.
@
text
@#! /bin/sh

case x$1 in
x--libs)
	echo '-lgssapi -lkrb5 -lasn1 -lcrypto';;
x--cflags)
	echo '-I/usr/include/kerberosV';;
esac
exit 0
@


