@# $OpenBSD: Makefile,v 1.69 2017/06/28 17:04:10 bluhm Exp $

COMMENT =		perl interface to the domain name system

DISTNAME =		Net-DNS-1.12
SUPDISTFILES =		${DISTFILES}.asc

CATEGORIES =		net

