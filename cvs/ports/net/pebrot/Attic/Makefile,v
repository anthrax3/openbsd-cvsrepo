@remove clients for discontinued Microsoft Messenger service
noticed by Frederic Cambus
@
text
@# $OpenBSD: Makefile,v 1.24 2015/06/29 15:39:44 sthen Exp $

COMMENT=	text-based MSN messenger client

MODPY_EGG_VERSION=	0.8.9
DISTNAME=	pebrot-${MODPY_EGG_VERSION}
REVISION=	5
CATEGORIES=	net

HOMEPAGE=	http://pebrot.sourceforge.net/

# GPLv2
PERMIT_PACKAGE_CDROM=	Yes

MASTER_SITES=	${MASTER_SITE_SOURCEFORGE:=pebrot/}

NO_TEST=	Yes

MODULES=	lang/python

.include <bsd.port.mk>
@


