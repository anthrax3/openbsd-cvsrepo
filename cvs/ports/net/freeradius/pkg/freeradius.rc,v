text
@#!/bin/sh
#
# $OpenBSD: radiusd.rc,v 1.2 2014/10/31 13:26:38 ajacoutot Exp $

daemon="${TRUEPREFIX}/sbin/radiusd"

. /etc/rc.d/rc.subr

rc_pre() {
	/usr/bin/install -d -o _freeradius /var/run/radiusd
}

rc_cmd $1
@
