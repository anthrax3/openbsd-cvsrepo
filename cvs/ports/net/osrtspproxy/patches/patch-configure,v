+    CXXFLAGS="${CCDBG} ${CXXFLAGS} -D_UNIX"
+    LD=ld
+    SYSLIBS="-pthread"
+    ;;
   *)
     CC=cc
     CFLAGS="-DNDEBUG -D_UNIX -I/usr/local/include"
