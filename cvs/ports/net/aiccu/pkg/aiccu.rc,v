@#!/bin/sh
#
# $OpenBSD: aiccu.rc,v 1.1 2011/07/13 15:27:44 dcoppa Exp $

daemon="${TRUEPREFIX}/sbin/aiccu"
daemon_flags="start"

. /etc/rc.d/rc.subr

rc_reload=NO

rc_stop() {
	${daemon} stop
}

rc_cmd $1
@


