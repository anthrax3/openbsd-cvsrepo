text
@d5 1
a5 1
# $OpenBSD$
d31 2
a32 2
	case `${CIPHER} <$1` in
	${CKSUM}) 
d34 2
a35 2
		exit 0;;
	*) 
d37 2
a38 2
		exit 1;;
	esac
@

