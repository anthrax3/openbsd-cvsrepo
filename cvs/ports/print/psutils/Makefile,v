@
text
@d1 1
a1 1
# $OpenBSD: Makefile,v 1.31 2010/11/20 09:48:39 espie Exp $
d8 1
a8 1
REVISION =	1
d30 2
a31 13
FLAVORS=	a3 a4 a5 b5 b4 letter legal ledger tabloid statement \
		executive folio quarto s10x14
FLAVOR?=letter

.for i in ${FLAVORS}
.  if ${FLAVOR:L} == $i
OK=Yes
.  endif
.endfor
OK?=No
.if ${OK} == "No"
ERRORS+="Fatal: Invalid flavor ${FLAVOR}"
.endif
d33 2
a34 2
.if ${FLAVOR:L} == s10x14
PAPERSIZE=	10x14
d36 1
a36 1
PAPERSIZE=	${FLAVOR:L}
@


