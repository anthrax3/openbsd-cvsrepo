+		# the default install, but it could be a regular file
+		if test -e /etc/localtime; then
+			export TZ=/etc/localtime
+		fi
+
+		IS_ON=:
+		;;
+
 	*)
 		IS_ON=/bin/true
 		ECHO=echo
