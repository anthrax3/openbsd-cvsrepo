     { "image/gif", "GIF", 0 },
     { "image/jpeg", "\377\330\377", 0 },
+    { "image/png", "\211PNG", 0 },
     { "video/mpeg", "\0\0\001\263", 4 },
     { "application/postscript", "%!", 0 },
 };
