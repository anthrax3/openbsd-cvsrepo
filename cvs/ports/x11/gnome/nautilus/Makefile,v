CONFIGURE_ENV=		CPPFLAGS="-I${LOCALBASE}/include -DWEB_NAVIGATION_ENABLED" \
a51 3

post-install:
	@@${INSTALL_DATA_DIR} ${PREFIX}/share/examples/nautilus; 
