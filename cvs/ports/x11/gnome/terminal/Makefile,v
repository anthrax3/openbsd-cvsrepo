# $OpenBSD: Makefile,v 1.20 2006/11/20 08:07:40 steven Exp $
d3 1
a3 1
COMMENT=		"GNOME terminal"
d5 2
a6 3
DISTNAME=		gnome-terminal-2.10.0
PKGNAME=		${DISTNAME}p2
CATEGORIES=		x11/gnome
d8 1
a8 1
HOMEPAGE=		http://www.gnome.org/
d15 33
a47 33
WANTLIB=	ICE ORBit-2 ORBitCosNaming-2 SM X11 Xft Xext \
		Xrender art_lgpl_2 atk-1.0 audiofile bonobo-2 \
		bonobo-activation bonoboui-2 c crypto esd fontconfig freetype \
		gconf-2 gdk-x11-2.0 gdk_pixbuf-2.0 glade-2.0 \
		glib-2.0 gmodule-2.0 gnome-2 gnome-keyring \
		gnomecanvas-2 gnomevfs-2 gobject-2.0 gthread-2.0 \
		gtk-x11-2.0 jpeg m ncurses pango-1.0 pangoft2-1.0 \
		pangox-1.0 pangoxft-1.0 popt pthread ssl xml2 z \
		pangocairo-1.0 cairo glitz png

MASTER_SITES=		${MASTER_SITE_GNOME:=sources/gnome-terminal/2.10/}
EXTRACT_SUFX=		.tar.bz2

MODULES=		devel/gettext

BUILD_DEPENDS=		:p5-XML-Parser-*:textproc/p5-XML-Parser \
			:scrollkeeper->=0.3.12:textproc/scrollkeeper
RUN_DEPENDS=		:scrollkeeper->=0.3.12:textproc/scrollkeeper
LIB_DEPENDS=		gnomeui-2::x11/gnome/libgnomeui \
			startup-notification-1::devel/startup-notification \
			vte::devel/vte

USE_X11=		Yes
USE_GMAKE=		Yes
USE_LIBTOOL=		Yes
CONFIGURE_STYLE=	gnu
CONFIGURE_ARGS+=	--disable-gtk-doc
CONFIGURE_ARGS+=	--with-widget=vte
CONFIGURE_ARGS+=	--disable-schemas-install
CONFIGURE_ARGS+=	--with-gconf-schema-file-dir=${LOCALBASE}/share/schemas/gnome-terminal
CONFIGURE_ENV=		CPPFLAGS="-I${LOCALBASE}/include" \
			LDFLAGS="-L${LOCALBASE}/lib" \
			PKG_CONFIG_PATH="${LOCALBASE}/lib/pkgconfig:${X11BASE}/lib/pkgconfig"
@


