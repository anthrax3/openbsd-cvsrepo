		ac_cv_path_INKSCAPE=false

# do not enforce build dependency on gtk-update-icon-cache
CONFIGURE_ENV +=ac_cv_path_GTK_UPDATE_ICON_CACHE=/usr/bin/true

.include <bsd.port.mk>
@


