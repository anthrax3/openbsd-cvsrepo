1.19
log
@Enforce XFCE_VERSION back to 4.10.0 for components which didnt see a release.
@
text
@# $OpenBSD: Makefile,v 1.18 2013/03/11 11:46:18 espie Exp $
PKG_ARCH=	*

COMMENT=	Xfwm4 window decoration themes

XFCE_VERSION =	4.10.0
XFCE_ARTWORK=	xfwm4-themes

# GPLv2
PERMIT_PACKAGE_CDROM=	Yes

MODULES=	x11/xfce4
XFCE_NO_SRC=	Yes

RUN_DEPENDS=	x11/xfce4/xfwm4

.include <bsd.port.mk>
@


