@# $OpenBSD: Makefile,v 1.1.1.1 2014/07/09 19:53:18 zhuk Exp $

COMMENT =	patch tools library for KDE
DISTNAME =	libkomparediff2-${MODKDE4_VERSION}
REVISION =	0

CATEGORIES =	devel
HOMEPAGE =	http://projects.kde.org/projects/kde/kdesdk/kompare

