COMMENT=		K Desktop Environment, development
DISTNAME=		kdevelop-${VERSION}
REVISION=		0

SHARED_LIBS +=	kdevcmakecommon      0.0      # .1.0
SHARED_LIBS +=	kdevplatforminterfaces 0.0    # .4.0
SHARED_LIBS +=	kdevplatformproject  0.0      # .4.0
