case x$WXCONFIG in
x)	# figure out manually what to do
	echo "Provide a WXCONFIG environment variable, please"
	exit 1
	;;
x*)
	if ! test -x $WXCONFIG; then
		echo "WXCONFIG ($WXCONFIG) not found"
	else
		exec $WXCONFIG "$@@"
	fi
	;;
esac
@


