DISTNAME=	aspell6-ru-0.99f7-1
MASTER_SITES=	${MASTER_SITE_GNU:=aspell/dict/ru/}
REVISION=	0

# see Copyright file
PERMIT_PACKAGE_CDROM=	Yes

.include <bsd.port.mk>
@


