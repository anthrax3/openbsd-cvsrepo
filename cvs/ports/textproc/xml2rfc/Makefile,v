d19 1
a22 3
do-build:
	@@${SUBST_CMD} -c ${FILESDIR}/xml2rfc ${WRKSRC}/xml2rfc

d24 2
a25 1
	@@${INSTALL_SCRIPT} ${WRKSRC}/xml2rfc ${PREFIX}/bin/xml2rfc
@


