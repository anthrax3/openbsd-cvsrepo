log
@PERMIT_* / REGRESS->TEST sweep
@
text
@# $OpenBSD: Makefile,v 1.18 2011/09/16 11:41:41 espie Exp $

COMMENT=	Python tool for generate Python API documents

DISTNAME=	HappyDoc_r2_1
PKGNAME=	py-HappyDoc-2.1
REVISION=	6
CATEGORIES=    	textproc

HOMEPAGE=	http://happydoc.sf.net/
MASTER_SITES=	${MASTER_SITE_SOURCEFORGE:=happydoc/}

# BSD License, badly worded
PERMIT_PACKAGE_CDROM=	Yes

MODULES=	lang/python

NO_TEST=	Yes
WRKDIST=	${WRKDIR}/HappyDoc-r2_1

.include <bsd.port.mk>
@


