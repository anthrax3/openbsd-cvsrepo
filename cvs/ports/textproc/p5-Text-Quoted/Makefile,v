DISTNAME =		Text-Quoted-2.09
CATEGORIES=		textproc

# perl
PERMIT_PACKAGE_CDROM=   Yes

RUN_DEPENDS=		textproc/p5-Text-Autoformat

.include <bsd.port.mk>
@


