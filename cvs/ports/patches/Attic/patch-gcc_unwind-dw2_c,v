+/* Figure out StackGhost cookie.  */
+_Unwind_Word uw_get_wcookie(void);
+
+asm(".text\n"
+    "uw_get_wcookie:\n"
+    "  add  %o7, %g0, %g4\n"
+    "  save %sp, -176, %sp\n"
+    "  save %sp, -176, %sp\n"
+    "  flushw\n"
+    "  restore\n"
+    "  ldx [%sp + 2047 + 120], %g5\n"
+    "  xor %g4, %g5, %i0\n"
+    "  ret\n"
+    "   restore\n");
+#endif
+
 
 /* Get the value of register INDEX as saved in CONTEXT.  */
 
