1.113
log
@update p5-IO-Socket-SSL to 2.050
Test t/npn.t currently fails due to a LibreSSL incompatibility.
This is unrelated to this module update.
@
text
@# $OpenBSD: Makefile,v 1.112 2017/07/29 20:01:26 bluhm Exp $

COMMENT =	Perl SSL sockets with IO::Socket interface

DISTNAME =	IO-Socket-SSL-2.050

CATEGORIES =	security

MAINTAINER =		Alexander Bluhm <bluhm@@openbsd.org>

# perl
PERMIT_PACKAGE_CDROM =	Yes

MODULES =		cpan
BUILD_DEPENDS =		${RUN_DEPENDS}
RUN_DEPENDS =		security/p5-Net_SSLeay>=1.59
TEST_DEPENDS =		net/p5-IO-Socket-INET6>=2.62 \
			converters/p5-Net-IDN-Encode \
			www/p5-URI>=1.50

PKG_ARCH =		*

MODCPAN_EXAMPLES =	Yes
MODCPAN_EXAMPLES_DIST =	example

.include <bsd.port.mk>
@


