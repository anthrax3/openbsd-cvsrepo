@#!/bin/sh
# $OpenBSD: runsocks.in,v 1.1 2000/03/27 23:02:45 form Exp $

if [ -n "${LD_PRELOAD}" ]; then
	LD_PRELOAD="@@PREFIX@@/lib/libsocks5_sh.so:${LD_PRELOAD}"
else
	LD_PRELOAD="@@PREFIX@@/lib/libsocks5_sh.so"
fi
export LD_PRELOAD

exec "$@@"
@


