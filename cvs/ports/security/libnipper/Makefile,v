1.20
log
@add pthread to COMPILER_LIBCXX.
white lie, but it allows clang and gcc to be more similar
bump accordingly.
@
text
@# $OpenBSD: Makefile,v 1.19 2017/07/16 19:19:01 espie Exp $

COMMENT =		library for nipper

DISTNAME =		libnipper-0.12.6
REVISION =		4
CATEGORIES =		security net

SHARED_LIBS =		nipper 0.0

HOMEPAGE =		http://nipper.sf.net/

# GPLv3, but "cannot be used as part of a commercial product"
PERMIT_PACKAGE_CDROM =	No
PERMIT_PACKAGE_FTP =	Yes

EXTRACT_SUFX =		.tgz

WANTLIB=		m ${COMPILER_LIBCXX}
MODULES +=		devel/cmake

NO_TEST =		Yes

pre-configure:
	@@${SUBST_CMD} ${WRKSRC}/CMakeLists.txt

.include <bsd.port.mk>
@


