@#!/bin/sh
#
# $OpenBSD$

daemon="${TRUEPREFIX}/sbin/ods-control"

. /etc/rc.d/rc.subr

rc_reload=NO

pexp="${TRUEPREFIX}/sbin/ods-(enforcerd|signerd)"

rc_start() {
	${rcexec} "${daemon} start"
}

rc_stop() {
	${daemon} stop
}

rc_cmd $1
@


