text
@#! /bin/sh

cat <<- E-O-F > ${WRKSRC}/cfsd.sh
	#! /bin/sh
	# Start the Cryptographic File System daemon.

	if [ -x ${PREFIX}/sbin/cfsd ]; then
	    echo -n ' cfsd'
	    ${PREFIX}/sbin/cfsd >/dev/null 2>&1
	fi
E-O-F
@


