text
@#!/bin/sh
#
# $OpenBSD: DEINSTALL-idle,v 1.1 2003/12/13 15:03:08 sturm Exp $
#
# Clean up python symlink, if it was ours.
#

set -e
PATH=/bin
PREFIX=${PKG_PREFIX:-/usr/local}

if [ ${PREFIX}/bin/idle -ef ${PREFIX}/bin/idle2.1 ]
then
	rm -f ${PREFIX}/bin/idle
fi

@


