+	  f=`echo $$file | cut -c$$((srcdir_cnt))-`; \
 	  fdir=`dirname $$f`; \
 	  if test ! -d $(DESTDIR)$(pkgdatadir)/examples/$$fdir; then \
 	    echo "$(mkinstalldirs) $(DESTDIR)$(pkgdatadir)/examples/$$fdir"; \
