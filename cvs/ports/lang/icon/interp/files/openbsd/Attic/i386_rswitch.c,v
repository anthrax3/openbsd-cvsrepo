1.2
log
@Update to icon 9.4.
Most local patches are no longer necessary.
Many thanks to Gregg Townsend for working with me on this one.
@
text
@/*
 * This is the co-expression context switch for the Intel 80386
 * under Microport Unix System V/386
 */

/*
 * coswitch
 */

coswitch(old_cs, new_cs, first)
int *old_cs, *new_cs;
int first;
   {
   asm("  movl 8(%ebp),%eax");
   asm("  movl %esp,0(%eax)");
   asm("  movl %ebp,4(%eax)");
   asm("  movl 12(%ebp),%eax");
   if (first == 0) {		/* this is the first activation */
      asm("  movl 0(%eax),%esp");
      asm("  movl $0,%ebp");
      new_context(0, 0);
      syserr("interp() returned in coswitch");
      }
   else {
      asm("  movl 0(%eax),%esp");
      asm("  movl 4(%eax),%ebp");
      }
   }
@


