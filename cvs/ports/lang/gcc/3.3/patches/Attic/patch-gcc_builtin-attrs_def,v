 DEF_ATTR_TREE_LIST (ATTR_NOTHROW_LIST, ATTR_NOTHROW, ATTR_NULL, ATTR_NULL)
@@@@ -136,6 +143,45 @@@@ DEF_FORMAT_ATTRIBUTE(STRFMON,3,3_4)
 DEF_FORMAT_ARG_ATTRIBUTE(1)
