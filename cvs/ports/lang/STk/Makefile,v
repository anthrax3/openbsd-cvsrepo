pre-install:
	@@rm -f ${PLIST}
	@@sed -e's:%%STK_VERSION%%:${VER}:' \
	     -e's:%%STK_ARCH%%:$(OPSYS)-$(OPSYS_VER)-$(MACHINE):' \
		${PLIST_ORIG} > ${PLIST}
