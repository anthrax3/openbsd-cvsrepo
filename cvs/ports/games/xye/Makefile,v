@# $OpenBSD: Makefile,v 1.1.1.1 2013/12/15 19:24:15 bcallah Exp $

COMMENT =	puzzle game collecting diamonds and avoiding obstacles
DISTNAME =	xye-0.12.2
CATEGORIES =	games x11
