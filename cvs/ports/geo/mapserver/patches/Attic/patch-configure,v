+++ configure	Thu May 26 19:59:16 2011
@@@@ -3361,7 +3361,7 @@@@ if test $ac_cv_lib_m_exp = yes; then
   XTRALIBS="$XTRALIBS -lm"
 fi
 
-XTRALIBS=$XTRALIBS
+XTRALIBS="$XTRALIBS -pthread"
 
 
 echo "$as_me:$LINENO: checking for __gxx_personality_v0 in -lstdc++" >&5
@


