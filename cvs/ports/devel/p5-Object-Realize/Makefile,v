CATEGORIES=	devel

HOMEPAGE=	http://perl.overmeer.net/

MAINTAINER=	Jasper Lievisse Adriaanse <jasper@@openbsd.org>

# perl
PERMIT_PACKAGE_CDROM=	Yes

.include <bsd.port.mk>
@


