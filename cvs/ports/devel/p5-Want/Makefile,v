@# $OpenBSD: Makefile,v 1.25 2016/03/20 19:56:41 naddy Exp $

COMMENT=	generalisation of wantarray

MODULES=	cpan
DISTNAME=	Want-0.29
CATEGORIES=	devel
REVISION=	0

MAINTAINER=	Jasper Lievisse Adriaanse <jasper@@openbsd.org>

# perl
PERMIT_PACKAGE_CDROM=	Yes

WANTLIB += c perl

CPAN_AUTHOR=	ROBIN

.include <bsd.port.mk>
@


