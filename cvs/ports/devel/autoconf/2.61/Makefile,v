			--mandir=${PREFIX}/man \
			--infodir=${PREFIX}/info
MODGNU_CONFIG_GUESS_DIRS = ${WRKSRC}/config
MAKE_FLAGS =		EMACS=no
FAKE_FLAGS =		EMACS=no

.include <bsd.port.mk>
@


