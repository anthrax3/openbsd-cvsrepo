 	-e 's,@@AWK\@@,$(AWK),g' \
 	-e 's,@@VERSION\@@,$(VERSION),g' \
 	-e 's,@@PACKAGE_NAME\@@,$(PACKAGE_NAME),g' \
-	-e 's,@@configure_input\@@,Generated from $<; do not edit by hand.,g'
+	-e 's,@@configure_input\@@,Generated from $$a; do not edit by hand.,g' $$a
 
 
 # autoconf is written in M4sh.
