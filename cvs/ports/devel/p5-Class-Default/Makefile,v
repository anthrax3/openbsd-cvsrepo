REVISION=	1
CATEGORIES=     devel

# Perl
PERMIT_PACKAGE_CDROM=   Yes

TEST_DEPENDS=devel/p5-Test-Pod

MAKE_ENV+=		AUTOMATED_TESTING=1

.include <bsd.port.mk>
