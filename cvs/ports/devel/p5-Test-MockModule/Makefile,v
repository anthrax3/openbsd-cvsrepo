CATEGORIES =	devel

# Artistic
PERMIT_PACKAGE_CDROM =	Yes

CONFIGURE_STYLE =	modbuild

RUN_DEPENDS =		devel/p5-SUPER
 
.include <bsd.port.mk>
@


