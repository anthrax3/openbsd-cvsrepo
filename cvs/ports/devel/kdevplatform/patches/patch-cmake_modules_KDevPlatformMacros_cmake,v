             COMMAND tar ARGS -c -C ${CMAKE_CURRENT_SOURCE_DIR}/${_templateName}
-                --exclude .kdev_ignore --exclude .svn
+                -s "@@^\\(.*/\\)*\\.kdev_ignore@@@@"
+                -s "@@^\\(.*/\\)*\\.svn@@@@"
                 -j -f ${_template} .
             DEPENDS ${_deps}
         )
