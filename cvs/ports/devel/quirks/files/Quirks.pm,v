		    $handle->pkgname);

		for my $i (@@{$plist1->{items}}) {
			if ($i->isa("OpenBSD::PackingElement::Unexec")) {
				bless $i, "OpenBSD::PackingElement::Comment";
				$i->{name} = "quirk: ".$i->{name};
