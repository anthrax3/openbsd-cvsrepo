# $OpenBSD: Quirks.pm,v 1.55 2011/07/08 11:08:11 landry Exp $
a246 16
sub quick_add
{
	my ($plist, $fullname) = @@_;

	return unless -f $fullname;
	my $fname = $fullname;
	$fname =~ s,^/usr/local/,,;
	my $o = OpenBSD::PackingElement::File->new($fname);
	if (-l $fullname) {
		$o->{symlink} = readlink($fullname);
	}
	# avoid checksumming them
	$o->{nochecksum} = 1;
	$o->add_object($plist);
}

