@Add missing BUILD_DEPENDS on autohell & libtool for libtoolize. Maybe
that port can use one of the auto* CONFIGURE_STYLE but at least now it
builds.
@
text
@d1 1
a1 1
# $OpenBSD: Makefile,v 1.4 2012/04/08 17:40:41 jasper Exp $
d5 1
a5 2
DISTNAME=	libyubikey-1.8
REVISION=	1
a20 3
AUTOMAKE_VERSION=	1.11
AUTOCONF_VERSION=	2.68

a21 3
BUILD_DEPENDS += ${MODGNU_AUTOMAKE_DEPENDS}
BUILD_DEPENDS += ${MODGNU_AUTOCONF_DEPENDS}
BUILD_DEPENDS += devel/libtool #libtoolize
a23 5

pre-configure:
	cd ${WRKSRC} && ${SETENV} -i AUTOMAKE_VERSION=${AUTOMAKE_VERSION} \
				AUTOCONF_VERSION=${AUTOCONF_VERSION} \
				autoreconf-${AUTOCONF_VERSION} --install
@


