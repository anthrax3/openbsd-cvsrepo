Sample chunk: most complicated function


void
vetherstart(struct ifnet *ifp)
{
        struct mbuf                *m;
        int                                 s;

        for (;;) {
                s = splnet();
                IFQ_DEQUEUE(&ifp->if_snd, m);
                splx(s);
                if (m == NULL)
                        return;
                ifp->if_opackets++;
                m_freem(m);
        }
}

