@Include <netinet/in.h> to get in_addr_t and in_port_t compiled.
@
text
@/* $OpenBSD: nm1.C,v 1.1 2013/07/06 23:06:36 espie Exp $ */
#include <sys/types.h>
#include <sys/socket.h>
#include <netinet/in.h>

#define D(T)	void witness_##T(T) {}

D(cpuid_t)
D(dev_t)
D(fixpt_t)
D(fsblkcnt_t)
D(gid_t)
D(id_t)
D(in_addr_t)
D(in_port_t)
