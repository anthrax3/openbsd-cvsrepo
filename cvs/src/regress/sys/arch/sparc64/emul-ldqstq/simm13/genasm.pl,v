print "#include <machine/asm.h>\n";
print "#include <machine/ctlreg.h>\n\n";

for ($i = -4096; $i <= 4095; $i++) {
	if ($i < 0) {
		$name = -$i;
		$name = "_$name";
	} else {
		$name = $i;
	}
	print "ENTRY(simm13_ldq_$name)\n";
	print "	sub	%o0, $i, %o0\n";
	print "	retl\n";
	print "	 ldq [%o0 + $i], %f0\n\n";

	print "ENTRY(simm13_stq_$name)\n";
	print "	sub	%o0, $i, %o0\n";
	print "	retl\n";
	print "	 stq %f0, [%o0 + $i]\n\n";
}
@


