#include <machine/asm.h>

	.space 8192

#if defined(__amd64__) || defined(__i386__)
ENTRY(testfly)
	ret
#endif

#ifdef __alpha__
LEAF(testfly, 0)
	ret	zero,(ra),1
	nop
END(testfly)
#endif

#ifdef __hppa__
LEAF_ENTRY(testfly)
	bv	r0(rp)
	nop
EXIT(testfly)
#endif

#ifdef __arm__
ENTRY(testfly)
	mov	pc, lr
#endif

#if defined(__sparc64__)
ENTRY(testfly)
	retl
	 nop
#endif
	
	.space 8192
@


1.2
log
@Add support for arm and enable on armv7.
@
text
@d1 1
a1 1
/*	$OpenBSD: testfly.S,v 1.1 2014/01/10 13:45:00 jsing Exp $	*/
d35 1
a35 2
#if defined(__amd64__) || defined(__i386__) || \
    defined(__sparc__) || defined(__sparc64__)
a37 1
	nop
d59 6
@


1.1
log
