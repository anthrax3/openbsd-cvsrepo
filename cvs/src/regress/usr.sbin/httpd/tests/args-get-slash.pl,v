@Fix regress test, server returns 400 instead of 500 now
@
text
@use strict;
use warnings;

our %args = (
    client => {
	func => sub {
	    my $self = shift;
	    print "GET /\r\n\r\n";
	},
        nocheck => 1
    },
    httpd => {
	loggrep => {
	    qr/"GET \/" 400 0/ => 1,
	},
    },
);

1;

