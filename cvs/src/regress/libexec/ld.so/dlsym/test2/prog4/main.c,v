#include <dlfcn.h>
#include <stdio.h>

int mainSymbol;

/*
 * checks that dependent libs are not promoted to RTLD_GLOBAL by mistake.
 */
int
main()
{
	int ret = 0;
	void *libbb = dlopen("libbb.so", RTLD_LAZY);
	void *libcc = dlopen("libcc.so", RTLD_LAZY);

	if (libbb == NULL) {
		printf("dlopen(\"libbb.so\", RTLD_LAZY) FAILED\n");
		return (1);
	}

	if (libcc == NULL) {
		printf("dlopen(\"libcc.so\", RTLD_LAZY) FAILED\n");
		return (1);
	}

	/* RTLD_DEFAULT should *not* see ccSymbol */
	if (dlsym(RTLD_DEFAULT, "ccSymbol") != NULL) {
		printf("dlsym(RTLD_DEFAULT, \"ccSymbol\") != NULL\n");
		ret = 1;
	}

	dlclose(libbb);
	dlclose(libcc);

	return (ret);
}
@


