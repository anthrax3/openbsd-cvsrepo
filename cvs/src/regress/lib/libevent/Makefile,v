1.1
log
@libevent regress from niels
@
text
@d1 1
a1 1
#       $OpenBSD$
d3 3
a5 1
NOMAN=	# defined
d7 2
a8 4
PROG=	eventtest
CFLAGS+=-Wall
DPADD+=	${LIBEVENT}
LDADD+=	-levent
d10 2
a11 3
libevent-regress: ${PROG}
	@@EVENT_SHOW_METHOD="yes" EVENT_NOPOLL="yes" ./eventtest
	@@EVENT_SHOW_METHOD="yes" EVENT_NOKQUEUE="yes" ./eventtest
d13 2
a14 2
REGRESS_TARGETS=libevent-regress
.PHONY: ${REGRESS_TARGETS}
a16 1

@

