log
@Add qsort(3) regress based on Bentley & McIlroy's "Engineering a Sort Function"
@
text
@#       $OpenBSD$

PROG=	qsort_test
CFLAGS+=-Wall
LDADD+=	-lm
DPADD+=	${LIBM}

qsort-regress: ${PROG}
	./${PROG}

REGRESS_TARGETS=qsort-regress
.PHONY: ${REGRESS_TARGETS}

.include <bsd.regress.mk>
@
