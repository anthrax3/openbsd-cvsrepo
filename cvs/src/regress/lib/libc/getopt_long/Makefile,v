@d1 1
a1 1
#	$OpenBSD$
d11 2
a12 2
	  ./${PROG} onefile twofile --best --illiterate -i foo.in threefile ; \
	  ./${PROG} --fast --drinking guiness -i foo.in somefile ; \
d15 2
a16 2
	  ./${PROG} onefile twofile --best --illiterate -i foo.in threefile ; \
	  ./${PROG} --fast --drinking guiness -i foo.in somefile ; \
