1.1
log
@Add regress tests to make sure arc4random(3) is reinitialized
correctly in fork children.
@
text
@#	$OpenBSD$

PROG=	arc4random-fork

REGRESS_TARGETS=	\
	run		\
	run-buf		\
	run-prefork	\
	run-buf-prefork

run: ${PROG}
	./${PROG}

run-buf: ${PROG}
	./${PROG} -b

run-prefork: ${PROG}
	./${PROG} -p

run-buf-prefork: ${PROG}
	./${PROG} -bp

.PHONY: ${REGRESS_TARGETS}

.include <bsd.regress.mk>
@
