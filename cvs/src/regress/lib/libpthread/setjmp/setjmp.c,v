#include <setjmp.h>
#include <stdlib.h>
#include "test.h"

int reached;

static void *
_jump(void *arg)
{
	jmp_buf foo;

	reached = 0;
	if (_setjmp(foo)) {
		ASSERT(reached);
		return NULL;
	}
	reached = 1;
	_longjmp(foo, 1);
	PANIC("_longjmp");
}

static void *
jump(void *arg)
{
	jmp_buf foo;

	reached = 0;
	if (setjmp(foo)) {
		ASSERT(reached);
		return NULL;
	}
	reached = 1;
	longjmp(foo, 1);
	PANIC("longjmp");
}

int
main(int argc, char *argv[])
{
	pthread_t child;
	void *res;

	printf("jumping in main thread\n");
	(void)jump(NULL);
	printf("_jumping in main thread\n");
	(void)_jump(NULL);

	printf("jumping in child thread\n");
	CHECKr(pthread_create(&child, NULL, jump, NULL));
	CHECKr(pthread_join(child, &res));

	printf("_jumping in child thread\n");
	CHECKr(pthread_create(&child, NULL, _jump, NULL));
	CHECKr(pthread_join(child, &res));

	SUCCEED;
}
@


