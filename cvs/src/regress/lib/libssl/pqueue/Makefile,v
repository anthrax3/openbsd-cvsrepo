1.1
log
@Move pqueue regress from libcrypto to libssl, since that's where the pqueue
code now lives. Also unbreak the regress following the symbol hiding
changes in libssl.
@
text
@#	$OpenBSD: Makefile,v 1.7 2016/09/02 17:02:53 beck Exp $

PROG=	pq_test
SRC=	${.CURDIR}/../../../../lib/libssl
CFLAGS+=	-I${SRC}

LDADD=	${SSL_INT} -lcrypto
DPADD=	${LIBSSL} ${LIBCRYPTO}
WARNINGS=	Yes
CFLAGS+=	-DLIBRESSL_INTERNAL -Werror

REGRESS_TARGETS= regress-pq_test

regress-pq_test: ${PROG}
	${.OBJDIR}/pq_test | cmp -s ${.CURDIR}/expected.txt /dev/stdin

.include <bsd.regress.mk>
@
