# Check whether --with-hdbdir was given.
if test "${with_hdbdir+set}" = set; then :
  withval=$with_hdbdir;
else
  with_hdbdir=/var/heimdal
fi

DIR_hdbdir="$with_hdbdir"



with_krb4=no


 if false; then
  KRB4_TRUE=
  KRB4_FALSE='#'
else
  KRB4_TRUE='#'
  KRB4_FALSE=
fi


 if true; then
  KRB5_TRUE=
  KRB5_FALSE='#'
