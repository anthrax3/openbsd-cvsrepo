_ACEOF

	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
	;;
	no)
	eval "ac_cv_func_bswap16=no"
	eval "LIB_bswap16="
	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
	;;
	*)
	eval "ac_cv_func_bswap16=yes"
	eval "ac_cv_lib_`echo "$ac_res" | sed 's/-l//'`=yes"
	cat >>confdefs.h <<_ACEOF
