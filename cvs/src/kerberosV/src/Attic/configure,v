#define HAVE_INET_ATON 1
_ACEOF

  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
  case " $LIBOBJS " in
  *" inet_aton.$ac_objext "* ) ;;
  *) LIBOBJS="$LIBOBJS inet_aton.$ac_objext"
 ;;
esac

fi

