_ACEOF

	with_libintl=yes
else
	with_libintl=no
	INCLUDE_libintl=
	LIB_libintl=
	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
