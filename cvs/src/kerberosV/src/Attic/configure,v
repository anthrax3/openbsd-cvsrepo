			LIB_XauReadAuth="$LIB_XauReadAuth $LIB_XauWriteAuth"
		else
			LIB_XauReadAuth="$LIB_XauReadAuth $LIB_XauWriteAuth $LIB_XauFileName"
		fi
	fi
	;;
esac

if test "$AUTOMAKE" != ""; then
	 if test "$ac_cv_func_XauWriteAuth" != "yes"; then
  NEED_WRITEAUTH_TRUE=
  NEED_WRITEAUTH_FALSE='#'
