		cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#ifdef HAVE_DLFCN_H
#include <dlfcn.h>
#endif
int
main ()
{
dlopen(0,0)
  ;
  return 0;
}
