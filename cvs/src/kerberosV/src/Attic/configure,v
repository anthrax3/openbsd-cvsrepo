if eval "test \"\$ac_cv_func_getsockopt\" != yes" ; then
	ac_save_LIBS="$LIBS"
	for ac_lib in "" ; do
		case "$ac_lib" in
		"") ;;
		yes) ac_lib="" ;;
		no) continue ;;
		-l*) ;;
		*) ac_lib="-l$ac_lib" ;;
		esac
		LIBS=" $ac_lib  $ac_save_LIBS"
		cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
