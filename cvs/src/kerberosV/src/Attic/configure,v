cat >>confdefs.h <<\_ACEOF
#define ENABLE_PTHREAD_SUPPORT 1
_ACEOF

            LIBS="$PTHREADS_LIBS $LIBS"
else
  PTHREADS_CFLAGS=""
  PTHREADS_LIBS=""
fi




