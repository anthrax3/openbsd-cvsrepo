{ $as_echo "$as_me:${as_lineno-$LINENO}: checking checking for __sync_add_and_fetch" >&5
$as_echo_n "checking checking for __sync_add_and_fetch... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
