main ()
{
struct utmpx x; memset(&x, 0, sizeof(x)); x.ut_id
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  ac_cv_type_struct_utmpx_ut_id=yes
else
  ac_cv_type_struct_utmpx_ut_id=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext

fi
