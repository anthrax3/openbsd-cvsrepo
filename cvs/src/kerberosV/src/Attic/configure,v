  $as_echo_n "(cached) " >&6
else

if eval "test \"\$ac_cv_func_db_create\" != yes" ; then
	ac_save_LIBS="$LIBS"
	for ac_lib in "" $dbheader db5 db4 db3 db; do
		case "$ac_lib" in
		"") ;;
		yes) ac_lib="" ;;
