_ACEOF

	cat >>confdefs.h <<_ACEOF
#define $ac_tr_lib 1
_ACEOF

	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: yes, in $ac_res" >&5
$as_echo "yes, in $ac_res" >&6; }
	;;
esac






