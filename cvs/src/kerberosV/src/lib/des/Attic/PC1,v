	);

foreach (@@PC1)
	{
	if ($_ ne "-")
		{
		$_--;
		$_=int($_/8)*8+7-($_%8);
		printf "%2d  ",$_;
		}
	else
		{ print "--  "; }
	print "\n" if (((++$i) % 8) == 0);
	print "\n" if ((($i) % 32) == 0);
	}

@


