AC_MSG_RESULT($ac_foo)
if test "$ac_foo" = yes; then
d23 7
a29 4
if false; then
	AC_CHECK_TYPES($1)
fi
  AC_DEFINE_UNQUOTED($ac_tr_hdr, 1, [Define if you have type `$1'])
