@@PC2_D=(41,52,31,37,47,55,
	30,40,51,45,33,48,
	44,49,39,56,34,53,
	46,42,50,36,29,32,
	);

foreach (@@PC2_C) {
	if ($_ ne "-")
		{
		$_--;
		printf "%2d  ",$_; }
	else { print "--  "; }
	$C{$_}=1;
	print "\n" if (((++$i) % 8) == 0);
	}
$i=0;
print "\n";
foreach (@@PC2_D) {
	if ($_ ne "-")
		{
		$_-=29;
		printf "%2d  ",$_; }
	else { print "--  "; }
	$D{$_}=1;
	print "\n" if (((++$i) % 8) == 0); }

print "\n";
foreach $i (0 .. 27)
	{
	$_=$C{$i};
	if ($_ ne "-") {printf "%2d ",$_;}
	else { print "--  "; }
	print "\n" if (((++$i) % 8) == 0);
	}
print "\n";

print "\n";
foreach $i (0 .. 27)
	{
	$_=$D{$i};
	if ($_ ne "-") {printf "%2d  ",$_;}
	else { print "--  "; }
	print "\n" if (((++$i) % 8) == 0);
	}
print "\n";
