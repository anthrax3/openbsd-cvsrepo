		| thumb | d10v | d30v | fr30 | avr)
		basic_machine=$basic_machine-unknown
		;;
	m6811 | m68hc11 | m6812 | m68hc12)
		# Motorola 68HC11/12.
a187 1
		os=-none
d195 1
a195 1
	i[234567]86 | x86_64)
d205 1
a205 1
	vax-* | tahoe-* | i[234567]86-* | i860-* | ia64-* | m32r-* | m68k-* | m68000-* \
d210 1
