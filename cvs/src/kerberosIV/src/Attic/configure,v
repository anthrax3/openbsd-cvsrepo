	CLAGS="$save_CFLAGS"
	if test $ac_res = yes; then
		# New GCC
		{ { echo "$as_me:8062: error: $CC does not support the $with_mips_abi ABI" >&5
echo "$as_me: error: $CC does not support the $with_mips_abi ABI" >&2;}
   { (exit 1); exit 1; }; }
	fi
	# Old GCC
	abi=''
	abilibdirext=''
	;;
	-mabi=n32|-mabi=64)
		if test $with_mips_abi = yes; then
			# Old GCC, default to O32
			abi=''
			abilibdirext=''
		else
			# Some broken GCC
			{ { echo "$as_me:8077: error: $CC does not support the $with_mips_abi ABI" >&5
echo "$as_me: error: $CC does not support the $with_mips_abi ABI" >&2;}
   { (exit 1); exit 1; }; }
		fi
	;;
esac
fi #if test $ac_res = no; then
fi #if test -n "$abi" ; then
else
