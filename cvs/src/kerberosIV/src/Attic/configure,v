else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
fi
rm -f conftest*
	done
	eval "ac_cv_funclib_readline=\${ac_cv_funclib_readline-no}"
	LIBS="$ac_save_LIBS"
fi

fi


eval "ac_res=\$ac_cv_funclib_readline"

