			ac_lib=""
		fi
		LIBS=" $ac_lib $LIB_tgetent $ac_save_LIBS"
		cat > conftest.$ac_ext <<EOF
#line 10658 "configure"
#include "confdefs.h"

int main() {
el_init()
; return 0; }
EOF
if { (eval echo configure:10665: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
