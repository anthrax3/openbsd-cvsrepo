fi
rm -f conftest*
fi

echo "$ac_t""$ac_cv_func_el_init_four" 1>&6
	if test "$ac_cv_func_el_init_four" = yes; then
		cat >> confdefs.h <<\EOF
#define HAVE_FOUR_VALUED_EL_INIT 1
