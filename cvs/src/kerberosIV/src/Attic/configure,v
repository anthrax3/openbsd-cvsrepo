then
  krb_cv_sys_x_libs_rpath="$rflag"; krb_cv_sys_x_libs="$foo"; break
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -fr conftest*
  :
fi
rm -fr conftest*
fi

	done
	LIBS="$ac_save_libs"
	CFLAGS="$ac_save_cflags"
	
fi

echo "$ac_t""$krb_cv_sys_x_libs_rpath" 1>&6
	X_LIBS="$krb_cv_sys_x_libs"
fi

