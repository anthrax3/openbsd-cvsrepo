test -n "$with_db_dir" &&

cat >>confdefs.h <<EOF
#define DB_DIR "$with_db_dir"
EOF

# Check whether --enable-random-mkey or --disable-random-mkey was given.
if test "${enable_random_mkey+set}" = set; then
  enableval="$enable_random_mkey"

if test "$enableval" = "yes"; then

cat >>confdefs.h <<\EOF
