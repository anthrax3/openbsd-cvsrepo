// The test spams to stderr and stdout.
#include <assert.h>
#include <cstdint>
#include <cstdio>
#include <cstddef>
#include <iostream>

extern "C" int LLVMFuzzerTestOneInput(const uint8_t *Data, size_t Size) {
  assert(Data);
  printf("PRINTF_STDOUT\n");
  fflush(stdout);
  fprintf(stderr, "PRINTF_STDERR\n");
  std::cout << "STREAM_COUT\n";
  std::cout.flush();
  std::cerr << "STREAM_CERR\n";
  return 0;
}

