#include "llvm/CodeGen/GCStrategy.h"

using namespace llvm;

GCStrategy::GCStrategy()
    : UseStatepoints(false), NeededSafePoints(0), CustomReadBarriers(false),
      CustomWriteBarriers(false), CustomRoots(false), InitRoots(true),
      UsesMetadata(false) {}
