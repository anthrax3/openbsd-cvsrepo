  ADDITIONAL_HEADER_DIRS
  ${LLVM_MAIN_INCLUDE_DIR}/llvm/IR
  )

# PR24785: Workaround for hanging compilation.
if( MSVC_VERSION EQUAL 1800)
  set_property(
    SOURCE Function.cpp
    PROPERTY COMPILE_FLAGS "/Og-"
    )
endif()

add_dependencies(LLVMCore intrinsics_gen)
