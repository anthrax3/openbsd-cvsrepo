    x86/RegisterContextWindows_x86.cpp
    )
elseif (CMAKE_SIZEOF_VOID_P EQUAL 8)
  set(PROC_WINDOWS_COMMON_SOURCES ${PROC_WINDOWS_COMMON_SOURCES}
    x64/RegisterContextWindows_x64.cpp
    )
endif()

