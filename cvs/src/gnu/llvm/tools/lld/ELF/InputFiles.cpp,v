static ELFKind getBitcodeELFKind(MemoryBufferRef MB) {
  Triple T(check(getBitcodeTargetTriple(MB)));
  if (T.isLittleEndian())
    return T.isArch64Bit() ? ELF64LEKind : ELF32LEKind;
  return T.isArch64Bit() ? ELF64BEKind : ELF32BEKind;
