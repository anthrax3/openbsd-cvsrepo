#define HAVE_MMAP 1
EOF

fi


echo $ac_n "checking for working strncmp""... $ac_c" 1>&6
echo "configure:2991: checking for working strncmp" >&5
if eval "test \"`echo '$''{'ac_cv_func_strncmp_works'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
