fi

rm -f core core.* *.core
fi

echo "$ac_t""$ac_cv_func_strncmp_works" 1>&6
if test $ac_cv_func_strncmp_works = no ; then
  LIBOBJS="$LIBOBJS strncmp.o"
fi


