/* By Ruediger Kuhlmann. */
      #include <sys/types.h>
      #if HAVE_UNISTD_H
      # include <unistd.h>
      #endif
      /* Some systems only have a dummy stub for fork() */
      int main ()
      {
	if (fork() < 0)
	  exit (1);
	exit (0);
      }
_ACEOF
rm -f conftest$ac_exeext
