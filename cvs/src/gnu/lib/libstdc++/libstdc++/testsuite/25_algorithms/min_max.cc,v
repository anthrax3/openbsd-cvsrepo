#include <algorithm>
#include <testsuite_hooks.h>

void test01()
{
  bool test = true;
  const int& x = std::max(1, 2);
  const int& y = std::max(3, 4);
  VERIFY( x == 2 );
  VERIFY( y == 4 );

  const int& z = std::min(1, 2);
  const int& w = std::min(3, 4);
  VERIFY( z == 1 );
  VERIFY( w == 3 );

#ifdef DEBUG_ASSERT
  assert(test);
#endif
}

int main()
{
  test01();
  return 0;
}
