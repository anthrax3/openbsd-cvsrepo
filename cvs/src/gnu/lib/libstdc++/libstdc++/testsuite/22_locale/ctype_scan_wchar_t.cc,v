  VERIFY_SCAN_IS (std::ctype_base::cntrl, cA, cA+5);
  VERIFY_SCAN_IS (std::ctype_base::digit, cA, cA+5);
  VERIFY_SCAN_IS (std::ctype_base::graph, cA, cA);
