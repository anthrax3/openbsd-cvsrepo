@// Use all the g++ headerfiles

// $Author: millert $
// $Revision: 1.2 $
// $Date: 1998/03/03 21:41:09 $

#include <_G_config.h>
// If we have the old iostream library, it defines _OLD_STREAMS
#include <stream.h>

#include <std.h>

#include <sys/types.h>
#include <sys/time.h>
#include <sys/file.h>
#if _G_HAVE_SYS_WAIT
#include <sys/wait.h>
#endif
