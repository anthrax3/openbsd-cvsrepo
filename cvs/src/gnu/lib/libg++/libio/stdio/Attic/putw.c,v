1.2
log
@old libg++, replaced by part of egcs.
@
text
@#include "libioP.h"
#include "stdio.h"

#undef putw

int
putw(w, fp)
     int w;
     FILE *fp;
{
  _IO_size_t written;
  CHECK_FILE(fp, EOF);
  written = _IO_sputn(fp, (const char *)&w, sizeof(w));
  return written == sizeof(w) ? 0 : EOF;
}
@


