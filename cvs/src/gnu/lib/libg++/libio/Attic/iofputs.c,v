#include "libioP.h"
#include <string.h>

int
DEFUN(_IO_fputs, (str, fp),
      const char *str AND _IO_FILE *fp)
{
  _IO_size_t len = strlen(str);
  CHECK_FILE(fp, EOF);
  if (_IO_sputn(fp, str, len) != len)
    return EOF;
  return 1;
}
