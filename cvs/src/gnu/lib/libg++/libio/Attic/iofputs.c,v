_IO_fputs (str, fp)
      const char *str;
      _IO_FILE *fp;
d32 5
a36 11
  _IO_size_t len = strlen (str);
  int result;
  CHECK_FILE (fp, EOF);
  _IO_cleanup_region_start ((void (*) __P ((void *))) _IO_funlockfile, fp);
  _IO_flockfile (fp);
  if (_IO_sputn (fp, str, len) != len)
    result = EOF;
  else
    result = 1;
  _IO_cleanup_region_end (1);
  return result;
a37 4

#ifdef weak_alias
weak_alias (_IO_fputs, fputs)
