1.4
log
@remove libf2c, hasn't been built since it was moved to ports in 2010.
ok miod@@ espie@@
@
text
@#include "f2c.h"

#undef abs
#include "math.h"
void
z_exp (doublecomplex * r, doublecomplex * z)
{
  double expx, zi = z->i;

  expx = exp (z->r);
  r->r = expx * cos (zi);
  r->i = expx * sin (zi);
}
@


