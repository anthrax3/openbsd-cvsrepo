1.2
log
@uucp goes away
@
text
@#
# Makefile for xchat 1.1
#
# Bob Denny - Tue Sep  1 15:58:22 1992
#
CC=cc
SHELL=/bin/sh
BIN=/usr/local/lib/uucp
PROGS=xchat

#-----------

all: $(PROGS)

install: $(PROGS)
	@@for i in $(PROGS) ; do \
		echo "Install $$i into $(BIN)..." ; \
		cp $$i $(BIN) ; \
		echo "Set ownership and protection..." ; \
		/bin/chmod 0555 $(BIN)/$$i ; \
		/bin/chown bin $(BIN)/$$i ; \
		/bin/chgrp bin $(BIN)/$$i ; \
	done

clean:
	rm -f $(PROGS) core





@


1.1
log
@Initial revision
@
text
