#include "libiberty.h"

char _hex_value[_hex_array_size];

void hex_init ()
{
  int i;
  for (i = 0; i < _hex_array_size; i++)
    _hex_value[i] = _hex_bad;
  for (i = 0; i < 10; i++)
    _hex_value['0' + i] = i;
  for (i = 0; i < 6; i++)
    _hex_value['a' + i] = _hex_value['A' + i] = 10 + i;
}
@


