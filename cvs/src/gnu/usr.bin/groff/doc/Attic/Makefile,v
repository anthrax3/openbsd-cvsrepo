DOCS=meref.PS meintro.PS pic.PS
d29 1
a29 1
.SUFFIXES: .tr .me .ms .PS .dit
a44 5
# This production must use -p -e -t so pic/eqn processing is done
pic.PS: pic.ms
	sed $< -e "s;@@VERSION@@;`cat ../VERSION`;" \
	| ../groff/groff -p -e -t -Tps $(FFLAG) -ms  >$@@

d46 1
