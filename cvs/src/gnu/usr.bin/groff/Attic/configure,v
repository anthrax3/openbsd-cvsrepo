if eval "test \"`echo '$''{'ac_cv_path_GROPS'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  case "$GROPS" in
  /*)
  ac_cv_path_GROPS="$GROPS" # Let the user override the test with a path.
