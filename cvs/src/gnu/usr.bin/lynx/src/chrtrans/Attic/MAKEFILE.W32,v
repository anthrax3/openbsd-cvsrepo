.\makeuctb.obj
$<,$*
D:\BC45\LIB\bidsf.lib+
D:\BC45\LIB\import32.lib+
D:\BC45\LIB\cw32.lib

|

.\makeuctb.obj :  makeuctb.c
  $(BCC32) -P- -c @@&&|
 $(CEAT_dbmakeuctbdexe) $(CNIEAT_dbmakeuctbdexe) -o$@@ makeuctb.c
|

