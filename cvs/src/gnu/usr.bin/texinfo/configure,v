    if test -z "$result"; then
      if test "$fits_in_uint" = 1; then
        result="$res_hi$res_lo"U
      else
        result="$res_hi$res_lo"UL
      fi
    else
            result='~(size_t)0'
    fi
  fi
  echo "$as_me:$LINENO: result: $result" >&5
echo "${ECHO_T}$result" >&6
