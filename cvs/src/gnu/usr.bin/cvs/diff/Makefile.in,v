	source='$<' object='$@@' libtool=no @@AMDEPBACKSLASH@@
	depfile='$(DEPDIR)/$*.Po' tmpdepfile='$(DEPDIR)/$*.TPo' @@AMDEPBACKSLASH@@
	$(CCDEPMODE) $(depcomp) @@AMDEPBACKSLASH@@
	$(COMPILE) -c -o $@@ `test -f $< || echo '$(srcdir)/'`$<

.c.obj:
	source='$<' object='$@@' libtool=no @@AMDEPBACKSLASH@@
	depfile='$(DEPDIR)/$*.Po' tmpdepfile='$(DEPDIR)/$*.TPo' @@AMDEPBACKSLASH@@
	$(CCDEPMODE) $(depcomp) @@AMDEPBACKSLASH@@
	$(COMPILE) -c -o $@@ `cygpath -w $<`


