GZIP=gzip --best
GZIP_EXT=.gz
TAR_VERBOSE=
dist: spec
	rm -rf `cat .fname`
	${MAKE} dist-dir DISTDIR="`cat .fname`"
	for dir in ${SUBDIRS}; do \
	  ( DISTDIR="../`cat .fname`/$${dir}"; \
            cd $${dir} && \
	    ${MAKE} dist-dir DISTDIR="$${DISTDIR}" \
	  ); \
	done
	(unset GZIP; tar chf${TAR_VERBOSE} - `cat .fname` | ${GZIP} > "`cat .fname`.tar${GZIP_EXT}")
	rm -rf `cat .fname` .fname .version
