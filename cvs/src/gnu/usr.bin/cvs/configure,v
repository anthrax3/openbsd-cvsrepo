  cat >> confdefs.h <<EOF
#define $ac_tr_func 1
EOF
 
else
  echo "$ac_t""no" 1>&6
fi
done

fi

if test "$ac_cv_func_crypt" = yes; then
  cat >> confdefs.h <<\EOF
#define AUTH_SERVER_SUPPORT 1
EOF

fi
fi # enable_server

