  echo $ac_n "(cached) $ac_c" 1>&6
else
  cat > conftest.$ac_ext <<EOF
#line 1829 "configure"
#include "confdefs.h"
#include <alloca.h>
int main() { return 0; }
int t() {
char *p = alloca(2 * sizeof(int));
; return 0; }
EOF
if eval $ac_link; then
  rm -rf conftest*
  ac_cv_header_alloca_h=yes
else
  rm -rf conftest*
  ac_cv_header_alloca_h=no
fi
rm -f conftest*

fi
