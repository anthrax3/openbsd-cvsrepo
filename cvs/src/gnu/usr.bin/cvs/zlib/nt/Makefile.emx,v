	$(CC) $(CFLAGS) -c $< -o $@@

zlib.a: $(OBJS)
	$(AR) $@@ $(OBJS)

%.exe : %.o $(LIBS)
	$(LD) $@@ $< $(LDLIBS)


.PHONY : clean

clean:
	$(RM) *.d
	$(RM) *.o
	$(RM) *.exe
	$(RM) zlib.a
	$(RM) foo.gz

DEPS := $(wildcard *.d)
ifneq ($(DEPS),)
include $(DEPS)
