dist-dir: $(DISTFILES) $(OBJDIR_DISTFILES)
	mkdir ${DISTDIR}
	for i in ${DISTFILES}; do \
	  ln $(srcdir)/$${i} ${DISTDIR}; \
	done
	ln ${OBJDIR_DISTFILES} ${DISTDIR}
	if [ -f cvs.info-1 ]; \
	  then ln -f cvs.info-* ${DISTDIR}; \
	  else : Pacify Ultrix sh; \
	fi
	if [ -f cvsclient.info-1 ]; \
	  then ln -f cvsclient.info-* ${DISTDIR}; \
	  else : Pacify Ultrix sh; \
	fi
