#ifndef SVR4_SHARED_LIBS

/* Return non-zero if we are in a shared library trampoline code stub. */

#define IN_SOLIB_CALL_TRAMPOLINE(pc, name) \
  (name && !strcmp(name, "_DYNAMIC"))

#endif /* !SVR4_SHARED_LIBS */
