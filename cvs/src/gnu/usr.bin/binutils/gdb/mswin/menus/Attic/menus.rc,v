        MENUITEM "&Cont\tF5",                       ID_REAL_CMD_BUTTON_CONT
        MENUITEM "&Run",                        ID_REAL_CMD_BUTTON_RUN
	MENUITEM "&In", ID_REAL_CMD_BUTTON_IN
	MENUITEM "&Out", ID_REAL_CMD_BUTTON_OUT
	MENUITEM "S&ync", ID_REAL_CMD_BUTTON_SYNC
    END

	    POPUP "&Help"
    BEGIN
        MENUITEM "&Help Index",                 ID_HELP_INDEX
        MENUITEM "Help for &GDB",               ID_HELP
        MENUITEM SEPARATOR
        MENUITEM "&About Gdb...",               ID_APP_ABOUT
    END

