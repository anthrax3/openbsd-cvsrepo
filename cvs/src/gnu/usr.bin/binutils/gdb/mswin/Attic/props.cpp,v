CSrcState::CSrcState()
{
  addresses = FALSE;
  breakpoint_ok = FALSE;
  disassembly = FALSE;
  instruction_data = FALSE;
  linenumbers = TRUE;
  source = TRUE;
}

CSrcState defs;

extern  CColorInfo  colinfo_s;
extern  CColorInfo  colinfo_a ;
extern CColorInfo  colinfo_b;
extern  CFontInfo srcfontinfo;

/////////////////////////////////////////////////////////////////////////////

