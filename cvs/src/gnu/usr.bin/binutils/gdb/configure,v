echo "$ac_t""$ac_cv_search_waddstr" 1>&6
if test "$ac_cv_search_waddstr" != "no"; then
  test "$ac_cv_search_waddstr" = "none required" || LIBS="$ac_cv_search_waddstr $LIBS"
  
