    enable_gdbtk=yes
  else
    enable_gdbtk=no
  fi
fi

# We unconditionally disable gdbtk tests on selected platforms.
case $host_os in
  go32* | windows*)
    echo "configure: warning: gdbtk isn't supported on $host; disabling" 1>&2
    enable_gdbtk=no ;;
esac

