s%@@WARN_CFLAGS@@%$WARN_CFLAGS%g
s%@@WERROR_CFLAGS@@%$WERROR_CFLAGS%g
s%@@SER_HARDWIRE@@%$SER_HARDWIRE%g
s%@@WIN32LIBS@@%$WIN32LIBS%g
s%@@LIBGUI@@%$LIBGUI%g
s%@@GUI_CFLAGS_X@@%$GUI_CFLAGS_X%g
s%@@WIN32LDAPP@@%$WIN32LDAPP%g
s%@@TCL_VERSION@@%$TCL_VERSION%g
