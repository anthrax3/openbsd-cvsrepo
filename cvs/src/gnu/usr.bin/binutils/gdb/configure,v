  echo $ac_n "(cached) $ac_c" 1>&6
else
  cat > conftest.$ac_ext <<EOF
#line 7522 "configure"
#include "confdefs.h"
#include <sys/param.h>
#include <sys/proc.h>
int main() {
struct thread td; td.td_pcb;
; return 0; }
EOF
if { (eval echo configure:7530: \"$ac_compile\") 1>&5; (eval $ac_compile) 2>&5; }; then
  rm -rf conftest*
  gdb_cv_struct_thread_td_pcb=yes
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -rf conftest*
  gdb_cv_struct_thread_td_pcb=no
fi
rm -f conftest*
fi

echo "$ac_t""$gdb_cv_struct_thread_td_pcb" 1>&6
if test $gdb_cv_struct_thread_td_pcb = yes; then
  cat >> confdefs.h <<\EOF
#define HAVE_STRUCT_THREAD_TD_PCB 1
EOF

fi

