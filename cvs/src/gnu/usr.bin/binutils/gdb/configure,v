$gdb_ptrace_headers
int main() {
extern int ptrace ();
; return 0; }
EOF
if { (eval echo configure:7234: \"$ac_compile\") 1>&5; (eval $ac_compile) 2>&5; }; then
  rm -rf conftest*
  gdb_cv_func_ptrace_ret='int'
else
  echo "configure: failed program was:" >&5
