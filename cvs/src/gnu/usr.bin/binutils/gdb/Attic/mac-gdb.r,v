	0,
	0,
	0,
	0,
	verUS,
	VERSION_STRING,
	VERSION_STRING  " (C) 1986-95 FSF, Inc."
};

resource 'vers' (2, purgeable)  {
	0,
	0,
	0,
	0,
	verUs,
	VERSION_STRING,
	"GDB " VERSION_STRING " for MPW"
};

#ifdef WANT_CFRG

