#define ABS16DST 	(DST | ABS | L_16)
#define ABS24SRC 	(SRC | ABS | L_24)
#define ABS24DST 	(DST | ABS | L_24)
#define ABS32SRC 	(SRC | ABS | L_32)
#define ABS32DST 	(DST | ABS | L_32)

