  /* Allocate/free functions, and a data pointer to pass to them.  */
  splay_tree_allocate_fn allocate;
  splay_tree_deallocate_fn deallocate;
  PTR GTY((skip (""))) allocate_data;

};
typedef struct splay_tree_s *splay_tree;
