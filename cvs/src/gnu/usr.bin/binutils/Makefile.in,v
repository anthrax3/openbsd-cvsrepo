	$(SET_LIB_PATH) \
	(cd flex && $(MAKE) $(FLAGS_TO_PASS) all)

.PHONY: check-flex maybe-check-flex
maybe-check-flex:

# This module is only tested in a native toolchain.
check-flex:
	@@if [ '$(host)' = '$(target)' ] ; then \
	  r=`${PWD_COMMAND}`; export r; \
	  s=`cd $(srcdir); ${PWD_COMMAND}`; export s; \
	  $(SET_LIB_PATH) \
	  (cd flex && $(MAKE) $(FLAGS_TO_PASS) check); \
	fi


