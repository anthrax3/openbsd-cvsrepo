.PHONY: do-TAGS
do-TAGS: TAGS-host TAGS-target

.PHONY: TAGS-host
TAGS-host: maybe-TAGS-gcc  \
    maybe-TAGS-ash \
    maybe-TAGS-autoconf \
    maybe-TAGS-automake \
    maybe-TAGS-bash \
    maybe-TAGS-bfd \
    maybe-TAGS-opcodes \
    maybe-TAGS-binutils \
