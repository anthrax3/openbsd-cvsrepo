GCC_FOR_TARGET = $$r/gcc/xgcc -B$$r/gcc/

CHILL_FOR_TARGET = ` \
  if [ -f $$r/gcc/xgcc ] ; then \
    echo $$r/gcc/xgcc -B$$r/gcc/ -L$$r/gcc/ch/runtime/; \
  else \
    if [ "$(host_canonical)" = "$(target_canonical)" ] ; then \
      echo $(CC); \
    else \
      t='$(program_transform_name)'; echo gcc | sed -e 's/x/x/' $$t; \
