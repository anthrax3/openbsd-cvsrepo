maybe-install-zip:
install-zip: installdirs
	@@r=`${PWD}`; export r; \
	s=`cd $(srcdir); ${PWD}`; export s; \
	$(SET_LIB_PATH) \
	(cd zip && $(MAKE) $(FLAGS_TO_PASS) install)


