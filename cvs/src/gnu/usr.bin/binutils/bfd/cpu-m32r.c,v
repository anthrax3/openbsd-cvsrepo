#include "bfd.h"
#include "sysdep.h"
#include "libbfd.h"

const bfd_arch_info_type bfd_m32r_arch =
{
    32,	/* bits in a word */
    32,	/* bits in an address */
    8,	/* bits in a byte */
    bfd_arch_m32r,
    0,
    "m32r",
    "m32r",
    4, /* section alignment power */
    true, /* the default */
    bfd_default_compatible,
    bfd_default_scan,
    0
};
@


