  R_D10V_GNU_VTINHERIT,
  R_D10V_GNU_VTENTRY,
a156 30
  /* GNU extension to record C++ vtable hierarchy */
  HOWTO (R_D10V_GNU_VTINHERIT, /* type */
         0,                     /* rightshift */
         2,                     /* size (0 = byte, 1 = short, 2 = long) */
         0,                     /* bitsize */
