 /* COFF symbol classifications.  */

enum coff_symbol_classification
{
   /* Global symbol.  */
  COFF_SYMBOL_GLOBAL,
   /* Common symbol.  */
  COFF_SYMBOL_COMMON,
   /* Undefined symbol.  */
  COFF_SYMBOL_UNDEFINED,
   /* Local symbol.  */
  COFF_SYMBOL_LOCAL,
   /* PE section symbol.  */
  COFF_SYMBOL_PE_SECTION
};

