	@@for dir in .. ${SUBDIRS}; do \
	if [ x$${dir} != x.. ]; then \
	    echo "$$dir:"; \
	    (cd $$dir; $(MAKE) $@@); \
	    fi ; \
	done
	-rm -f *~ core
	-rm -f Makefile config.status 

.PHONY: info install-info clean-info
info:
install-info:
clean-info:

force:

Makefile: Makefile.in configure.in $(host_makefile_frag) $(target_makefile_frag)
	$(SHELL) config.status





