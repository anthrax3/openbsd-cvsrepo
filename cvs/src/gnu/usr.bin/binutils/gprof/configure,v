s%@@OBJEXT@@%$OBJEXT%g
s%@@EXEEXT@@%$EXEEXT%g
s%@@RANLIB@@%$RANLIB%g
s%@@STRIP@@%$STRIP%g
s%@@LIBTOOL@@%$LIBTOOL%g
s%@@CPP@@%$CPP%g
