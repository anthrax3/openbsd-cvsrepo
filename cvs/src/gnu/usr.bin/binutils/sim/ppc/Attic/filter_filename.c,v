#include "config.h"
#include "ppc-config.h"
#include "filter_filename.h"

/* Shorten traces by eliminating the directory component to filenames.  */
extern const char *
filter_filename (const char *filename)
{
  const char *p = filename;
  const char *last = filename;
  int ch;

  while ((ch = *p++) != '\0' && ch != ':')
    if (ch == '/')
      last = p;

  return last;
}
