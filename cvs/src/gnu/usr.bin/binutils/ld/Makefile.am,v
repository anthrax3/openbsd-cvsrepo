  $(INCDIR)/symcat.h sysdep.h config.h $(INCDIR)/fopen-same.h \
  $(INCDIR)/bfdlink.h $(INCDIR)/safe-ctype.h ld.h $(INCDIR)/bin-bugs.h \
  ldexp.h ldlang.h ldmisc.h ldgram.h ldmain.h ldctor.h
