addib_nullified_tests: 
	addib,n -1,%r4,addib_tests
	addib,=,n -1,%r4,addib_tests
	addib,<,n -1,%r4,addib_tests
	addib,<=,n -1,%r4,addib_tests
	addib,nuv,n -1,%r4,addib_tests
	addib,znv,n -1,%r4,addib_tests
	addib,sv,n -1,%r4,addib_tests
	addib,od,n -1,%r4,addib_tests
	addib,tr,n -1,%r4,addib_tests
