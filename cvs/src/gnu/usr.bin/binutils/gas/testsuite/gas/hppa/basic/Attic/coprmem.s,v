1.1
log
@Initial revision
@
text
@d1 1
a1 10
	.SPACE $PRIVATE$
	.SUBSPA $DATA$,QUAD=1,ALIGN=8,ACCESS=31
	.SUBSPA $BSS$,QUAD=1,ALIGN=8,ACCESS=31,ZERO,SORT=82
	.SPACE $TEXT$
	.SUBSPA $LIT$,QUAD=0,ALIGN=8,ACCESS=44
	.SUBSPA $CODE$,QUAD=0,ALIGN=8,ACCESS=44,CODE_ONLY

	.SPACE $TEXT$
	.SUBSPA $CODE$

d12 8
a19 8
	cldwx,4 5(0,4),26
	cldwx,4,s 5(0,4),26
	cldwx,4,m 5(0,4),26
	cldwx,4,sm 5(0,4),26
	clddx,4 5(0,4),26
	clddx,4,s 5(0,4),26
	clddx,4,m 5(0,4),26
	clddx,4,sm 5(0,4),26
d22 8
a29 8
	cstwx,4 26,5(0,4)
	cstwx,4,s 26,5(0,4)
	cstwx,4,m 26,5(0,4)
	cstwx,4,sm 26,5(0,4)
	cstdx,4 26,5(0,4)
	cstdx,4,s 26,5(0,4)
	cstdx,4,m 26,5(0,4)
	cstdx,4,sm 26,5(0,4)
d32 12
a43 12
	cldws,4 0(0,4),26
	cldws,4,mb 0(0,4),26
	cldws,4,ma 0(0,4),26
	cldds,4 0(0,4),26
	cldds,4,mb 0(0,4),26
	cldds,4,ma 0(0,4),26
	cstws,4 26,0(0,4)
	cstws,4,mb 26,0(0,4)
	cstws,4,ma 26,0(0,4)
	cstds,4 26,0(0,4)
	cstds,4,mb 26,0(0,4)
	cstds,4,ma 26,0(0,4)
d46 1
