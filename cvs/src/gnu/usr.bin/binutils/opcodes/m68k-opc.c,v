{"tstw",	one(0045100),	one(0177700), "*w", m68020up | mcf5200 },
{"tstw",	one(0045100),	one(0177700), "@@w", m68000up },
{"tstl",	one(0045200),	one(0177700), "*l", m68020up | mcf5200 },
{"tstl",	one(0045200),	one(0177700), "@@l", m68000up },
