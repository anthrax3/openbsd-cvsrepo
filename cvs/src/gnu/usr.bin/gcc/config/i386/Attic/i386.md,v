(define_insn "movxfcc_1"
  [(set (match_operand:XF 0 "register_operand" "=f,=f,=f,=f")
	(if_then_else:XF (match_operator 1 "comparison_operator" 
				[(cc0) (const_int 0)])
		      (match_operand:XF 2 "register_operand" "0,f,f,fFm")
