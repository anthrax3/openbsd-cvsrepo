#ifndef CGEN_TYPES_H
#define CGEN_TYPES_H

#include <stdint.h>

typedef  int8_t   QI;
typedef uint8_t  UQI;
typedef  int16_t  HI;
typedef uint16_t UHI;
typedef  int32_t  SI;
typedef uint32_t USI;
typedef  int64_t  DI;
typedef uint64_t UDI;

typedef int INT;
typedef unsigned int UINT;

typedef float SF;
typedef double DF;
typedef long double XF, TF;

#endif /* CGEN_TYPES_H */
