@	;; sequence control operands need to be packed with nop's
	;;  bl, jl, trap, sleep, stop, wait, dbt, bra, jmp, rte, rtd

	.text
	.align 2
test0:
	bl.s test1
	ldi r0, #1
test1:
	jl r1
	ldi r0, #1
test2:
