	lb	$4,small_local_common+0x1a5a5($5)

# Several macros are handled like lb.  Sanity check them.
	lbu	$4,0
	lh	$4,0
	lhu	$4,0
	lw	$4,0
	lwl	$4,0
	lwr	$4,0
	lwc0	$4,0
	lwc1	$4,0
	lwc2	$4,0
	lwc3	$4,0

# Round to a 16 byte boundary, for ease in testing multiple targets.
	nop
	nop
	nop
