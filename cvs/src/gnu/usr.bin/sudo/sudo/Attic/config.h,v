/* #undef HAVE_PAM */

/* Define if you use AFS.  */
/* #undef HAVE_AFS */

/* Define if you use OSF DCE.  */
/* #undef HAVE_DCE */

/* Define if you have POSIX signals.  */
#define HAVE_SIGACTION 1
#ifdef HAVE_SIGACTION
