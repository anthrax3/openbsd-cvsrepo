    0x2b5c, 0x0003, /* cond_expr */
    0x2b5c, 0x0e18, 0x0256, 0x028c, 0x3fc8, 0x3e04, 0x2481, /* entersub */
    0x33d8, 0x0003, /* leavesub, leavesublv, leavewrite, leaveeval */
    0x00bc, 0x012f, /* caller */
    0x21f5, /* nextstate, dbstate */
