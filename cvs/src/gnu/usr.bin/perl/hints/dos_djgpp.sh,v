1.1.1.5
log
@perl 5.8.2 from CPAN
@
text
@d43 1
a43 6
	case `gcc -v 2>&1|grep "gcc version"` in
	"gcc version 3."*)
	  optimize="-O2 -falign-loops=2 -falign-jumps=2 -falign-functions=2" ;;
	*)
	  optimize="-O2 -malign-loops=2 -malign-jumps=2 -malign-functions=2" ;;
	esac
@


