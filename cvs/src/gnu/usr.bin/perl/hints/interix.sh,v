1.1.1.2
log
@Import perl-5.20.1
@
text
@a16 10
plibpth=''

case "$plibpth" in
'') plibpth=`LANG=C LC_ALL=C $cc -print-search-dirs | grep libraries |
        cut -f2- -d= | tr ':' $trnl | grep -v 'gcc' | sed -e 's:/$::'`
    set X $plibpth # Collapse all entries on one line
    shift
    plibpth="$*"
    ;;
esac
@

