BEGIN
  {
  $| = 1;
  # to locate the testing files
  my $location = $0; $location =~ s/bigratpm.t//i;
  if ($ENV{PERL_CORE})
    {
    # testing with the core distribution
    @@INC = qw(../lib);
    }
  unshift @@INC, '../lib';
  if (-d 't')
