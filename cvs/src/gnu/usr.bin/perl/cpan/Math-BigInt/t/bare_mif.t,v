#!/usr/bin/perl -w
d3 2
a4 2
# test rounding, accuracy, precicion and fallback, round_mode and mixing
# of classes under BareCalc
d7 1
a7 1
use Test;
d9 2
a10 25
BEGIN
  {
  $| = 1;
  # to locate the testing files
  my $location = $0; $location =~ s/bare_mif.t//i;
  if ($ENV{PERL_CORE})
    {
    @@INC = qw(../t/lib); 		# testing with the core distribution
    }
  unshift @@INC, '../lib';	# for testing manually
  if (-d 't')
    {
    chdir 't';
    require File::Spec;
    unshift @@INC, File::Spec->catdir(File::Spec->updir, $location);
    }
  else
    {
    unshift @@INC, $location;
    }
  print "# INC = @@INC\n";

  plan tests => 684
    + 1;		# our own tests
  }
d12 1
a12 1
print "# ",Math::BigInt->config()->{lib},"\n";
d14 1
a14 1
use Math::BigInt lib => 'BareCalc';
d17 1
a17 2
use vars qw/$mbi $mbf/;

d21 2
a22 3
ok (Math::BigInt->config()->{lib},'Math::BigInt::BareCalc');

require 'mbimbf.inc';
d24 1
