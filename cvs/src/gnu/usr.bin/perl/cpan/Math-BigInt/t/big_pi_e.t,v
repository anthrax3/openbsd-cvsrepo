#!/usr/bin/perl -w
a4 1
use Test::More;
d6 1
d8 1
a8 25
BEGIN
  {
  $| = 1;
  # to locate the testing files
  my $location = $0; $location =~ s/big_pi_e.t//i;
  if ($ENV{PERL_CORE})
    {
    # testing with the core distribution
    @@INC = qw(../lib);
    }
  unshift @@INC, '../lib';
  if (-d 't')
    {
    chdir 't';
    require File::Spec;
    unshift @@INC, File::Spec->catdir(File::Spec->updir, $location);
    }
  else
    {
    unshift @@INC, $location;
    }
  print "# INC = @@INC\n";

  plan tests => 8;
  }
