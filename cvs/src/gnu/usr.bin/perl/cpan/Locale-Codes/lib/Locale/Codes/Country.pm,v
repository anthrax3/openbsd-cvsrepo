#=======================================================================
#
# Old function for backward compatibility
#
#=======================================================================

sub alias_code {
   my($alias,$code,@@args) = @@_;
   my $success = rename_country_code($code,$alias,@@args);
   return 0  if (! $success);
   return $alias;
@


