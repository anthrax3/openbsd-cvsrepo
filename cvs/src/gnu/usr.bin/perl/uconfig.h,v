 *	the const type. There is no need to actually test for that symbol
 *	within your programs. The mere use of the "const" keyword will
 *	trigger the necessary tests.
d1024 18
a1041 4
/*#define HASCONST	/ **/
#ifndef HASCONST
#define const
#endif
