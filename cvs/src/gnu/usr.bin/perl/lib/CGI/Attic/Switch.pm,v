a2 34
package CGI::Switch;
use Carp;
use strict;
use vars qw($VERSION @@Pref);
$VERSION = '0.06';
@@Pref = qw(CGI::Apache CGI); #default

sub import {
    my($self,@@arg) = @@_;
    @@Pref = @@arg if @@arg;
}

sub new {
    shift;
    my($file,$pack);
    for $pack (@@Pref) {
	($file = $pack) =~ s|::|/|g;
	eval { require "$file.pm"; };
	if ($@@) {
#XXX	    warn $@@;
	    next;
	} else {
#XXX	    warn "Going to try $pack\->new\n";
	    my $obj;
	    eval {$obj = $pack->new(@@_)};
	    if ($@@) {
#XXX		warn $@@;
	    } else {
		return $obj;
	    }
	}
    }
    Carp::croak "Couldn't load+construct any of @@Pref\n";
}
