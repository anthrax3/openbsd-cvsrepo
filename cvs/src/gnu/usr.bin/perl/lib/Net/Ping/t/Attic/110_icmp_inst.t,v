@import perl 5.10.0 from CPAN
@
text
@d20 1
a20 1
        and !IsAdminUser())
a28 7
}

sub IsAdminUser {
  return unless $^O eq 'MSWin32';
  return unless eval { require Win32 };
  return unless defined &Win32::IsAdminUser;
  return Win32::IsAdminUser();
@


