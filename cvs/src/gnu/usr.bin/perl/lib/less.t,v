@d8 1
a8 1
use Test::More tests => 12;
d10 1
a10 8
BEGIN {
    use_ok( 'less' );

    package less::again;
    sub stash_name {'less'}
    @@ISA = 'less';
    $INC{'less/again.pm'} = 1;
}
a14 1
is_deeply([less::again->of], ['please'], 'less::again please');
a16 7
is_deeply([less::again->of], [], 'no less::again please');
use less::again;
is_deeply([less->of], ['please'],'less please');
is_deeply([less::again->of], ['please'], 'less::again please');
no less::again;
is_deeply([less->of],[],'more please');
