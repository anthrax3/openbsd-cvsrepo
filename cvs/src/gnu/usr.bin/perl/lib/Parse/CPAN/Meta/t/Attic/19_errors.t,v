@#!/usr/bin/perl

# Testing documents that should fail

BEGIN {
	if( $ENV{PERL_CORE} ) {
		chdir 't';
		@@INC = ('../lib', 'lib');
	}
	else {
		unshift @@INC, 't/lib/';
	}
}

use strict;
BEGIN {
	$|  = 1;
	$^W = 1;
}

use File::Spec::Functions ':ALL';
use Parse::CPAN::Meta::Test;
use Test::More tests => 1;





