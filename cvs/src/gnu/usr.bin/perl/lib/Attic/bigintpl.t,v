text
@d7 1
a7 12

{
    # Silence the deprecation warnings from bigint.pl for the purpose
    # of testing. These tests will be removed along with bigint.pl in
    # the next major release of perl.
    local $SIG{__WARN__} = sub {
        if ($_[0] !~ /deprecated/) {
            print(STDERR @@_);
        }
    };
    require "bigint.pl";
}
@

