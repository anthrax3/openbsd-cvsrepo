@#!/bin/sh
rm -f /tmp/*.dir /tmp/*.pag
awk -e '{
        printf "%s\t", $0
        for (i = 0; i < 40; i++)
                printf "%s.", $0
        printf "\n"
}' < /usr/dict/words | $1 build /tmp/$2

