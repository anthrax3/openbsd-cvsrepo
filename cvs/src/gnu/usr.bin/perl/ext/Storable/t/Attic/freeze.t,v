@d27 1
a27 1
print "1..20\n";
a138 13

if ($] >= 5.006) {
    eval '
        $a = []; $#$a = 2; $a->[1] = undef;
        $b = thaw freeze $a;
        @@a = map { ~~ exists $a->[$_] } 0 .. $#$a;
        @@b = map { ~~ exists $b->[$_] } 0 .. $#$b;
        ok 20, "@@a" eq "@@b";
    ';
}
else {
    print "ok 20 # skipped (no av_exists)\n";
}
@

