use strict;

my $out = 'apicheck.c';
my @@api = map { /^--api=(\w+)$/ ? ($1) : () } @@ARGV;
print "creating $out", (@@api ? " (@@api)" : ''), "\n";
system $^X, 'parts/apicheck.pl', @@api, $out
    and die "couldn't create $out\n";

@


