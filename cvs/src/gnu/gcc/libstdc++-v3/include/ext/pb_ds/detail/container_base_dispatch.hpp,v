  template<typename Key, typename Mapped, typename Policy_Tl, typename Alloc>
    struct container_base_dispatch<Key, Mapped, splay_tree_tag, 
				   Policy_Tl, Alloc>
    {
    private:
      typedef __gnu_cxx::typelist::at_index<Policy_Tl, 0>	at0;
