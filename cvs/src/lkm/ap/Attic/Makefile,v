1.2
log
@Don't install test aperture program
Install aperture postinstall script for use with modload
@
text
@d1 1
a1 1
#	$OpenBSD: Makefile,v 1.1 1996/03/05 11:25:24 mickey Exp $
a9 4

afterinstall:
	install -c -o ${LKMOWN} -g ${LKMGRP} -m 555 ${.CURDIR}/apinstall \
		${DESTDIR}${LKMDIR}
@


