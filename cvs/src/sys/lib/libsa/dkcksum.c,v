#include <sys/param.h>
#include <sys/disklabel.h>
#include "stand.h"

/*
 * Compute checksum for disk label.
 */
u_int
dkcksum(const struct disklabel *lp)
{
	u_short *start, *end;
	u_short sum = 0;

	start = (u_short *)lp;
	end = (u_short *)&lp->d_partitions[lp->d_npartitions];
	while (start < end)
		sum ^= *start++;
	return (sum);
}
@


