1.1
log
@Initial revision
@
text
@a40 22
fstat(fd, sb)
	int fd;
	struct stat *sb;
{
	register struct open_file *f = &files[fd];

	if ((unsigned)fd >= SOPEN_MAX || f->f_flags == 0) {
		errno = EBADF;
		return (-1);
	}

	/* operation not defined on raw devices */
	if (f->f_flags & F_RAW) {
		errno = EOPNOTSUPP;
		return (-1);
	}

	errno = (f->f_ops->stat)(f, sb);
	return (0);
}

int
