#include "stand.h"

int
stat(const char *str, struct stat *sb)
{
	int fd, rv;

#ifdef __INTERNAL_LIBSA_CREAD
	if ((fd = oopen(str, 0)) < 0)
#else
	if ((fd = open(str, 0)) < 0)
#endif
		return (-1);
	rv = fstat(fd, sb);
#ifdef __INTERNAL_LIBSA_CREAD
	(void)oclose(fd);
#else
	(void)close(fd);
#endif
	return (rv);
}
