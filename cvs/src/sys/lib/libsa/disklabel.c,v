text
@d40 1
a41 2
int dkcksum __P((struct disklabel *));
    
a65 17
}

/*
 * Compute checksum for disk label.
 */
int
dkcksum(lp)
	register struct disklabel *lp;
{
	register u_short *start, *end;
	register u_short sum = 0;

	start = (u_short *)lp;
	end = (u_short *)&lp->d_partitions[lp->d_npartitions];
	while (start < end)
		sum ^= *start++;
	return (sum);
@


