#include <sys/types.h>
#include "stand.h"

/*
 * This is designed to be small, not fast.
 */
void
bcopy(s1, s2, n)
	const void *s1;
	void *s2;
	size_t n;
{
	register const char *f = s1;
	register char *t = s2;

	if (f < t) {
		f += n;
		t += n;
		while (n-- > 0)
			*--t = *--f;
	} else
		while (n-- > 0)
			*t++ = *f++;
}
@


