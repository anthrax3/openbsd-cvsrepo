#include "stand.h"

off_t
#ifndef __INTERNAL_LIBSA_CREAD
lseek(int fd, off_t offset, int where)
#else
olseek(int fd, off_t offset, int where)
#endif
{
	struct open_file *f = &files[fd];

	if ((unsigned)fd >= SOPEN_MAX || f->f_flags == 0) {
		errno = EBADF;
		return (-1);
	}

	if (f->f_flags & F_RAW) {
		/*
		 * On RAW devices, update internal offset.
		 */
		switch (where) {
		case SEEK_SET:
			f->f_offset = offset;
			break;
		case SEEK_CUR:
			f->f_offset += offset;
			break;
		case SEEK_END:
		default:
			errno = EOFFSET;
			return (-1);
		}
		return (f->f_offset);
	}

	return (f->f_ops->seek)(f, offset, where);
}
