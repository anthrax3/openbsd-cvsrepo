#include "stand.h"
#include "saerrno.h"

int
#ifndef __INTERNAL_LIBSA_CREAD
close(int fd)
#else
oclose(int fd)
#endif
{
	struct open_file *f = &files[fd];
	int err1 = 0, err2 = 0;

	if ((unsigned)fd >= SOPEN_MAX || f->f_flags == 0) {
		errno = EBADF;
		return (-1);
	}
	if (!(f->f_flags & F_RAW) && f->f_ops)
		err1 = (f->f_ops->close)(f);
	if (!(f->f_flags & F_NODEV) && f->f_dev)
		err2 = (f->f_dev->dv_close)(f);
	f->f_flags = 0;
	if (err1) {
		errno = err1;
		return (-1);
	}
	if (err2) {
		errno = err2;
		return (-1);
	}
	return (0);
}
@


