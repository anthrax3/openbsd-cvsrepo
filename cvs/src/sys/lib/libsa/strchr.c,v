#ifndef _STANDALONE
#include <string.h>
#else
#include <lib/libsa/stand.h>
#endif

char *
strchr(const char *p, int ch)
{
	for (;; ++p) {
		if (*p == ch)
			return((char *)p);
		if (!*p)
			return((char *)NULL);
	}
	/* NOTREACHED */
}
@


