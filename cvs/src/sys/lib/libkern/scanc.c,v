#include <sys/types.h>
#include <lib/libkern/libkern.h>

int
scanc(u_int size, const u_char *cp, const u_char table[], int mask)
{
	const u_char *end = &cp[size];

	while (cp < end && (table[*cp] & (u_char)mask) == 0)
		cp++;
	return (end - cp);
}
@


