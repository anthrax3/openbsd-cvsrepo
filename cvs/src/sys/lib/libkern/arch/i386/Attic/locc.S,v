#include "DEFS.h"

ENTRY(locc)
	pushl	%edi
	movl	16(%esp),%edi
	movl	12(%esp),%ecx
	movl	8(%esp),%eax
	cld
	repne
	scasb
	jne	1f
	incl	%ecx
1:
	movl	%ecx,%eax
	popl	%edi
	ret
@


