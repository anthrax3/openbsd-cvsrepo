#include "DEFS.h"

ENTRY(scanc)
	movl	4(%esp),%ecx
	testl	%ecx,%ecx
	jz	3f
	pushl	%esi
	pushl	%edi
	movl	16(%esp),%esi
	movl	20(%esp),%edi
	movb	24(%esp),%dl
	xorl	%eax,%eax
1:
	lodsb
	testb	%dl,(%eax,%edi)
	jnz	2f
	decl	%ecx
	jnz	1b
2:
	popl	%edi
	popl	%esi
3:
	movl	%ecx,%eax
	ret
@


