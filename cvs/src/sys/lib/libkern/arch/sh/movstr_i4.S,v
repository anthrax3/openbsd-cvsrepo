#include <machine/asm.h>


NENTRY(__movstr_i4_odd)
	add	#-8,r4
	nop
odd_loop:
	mov.l	@@r5+,r0
	add	#8,r4
	mov.l	@@r5+,r1
	dt	r6
	mov.l	r0,@@(0,r4)
	bf/s	odd_loop
	 mov.l	r1,@@(4,r4)

	mov.l	@@r5+,r0
	mov.l	@@r5+,r1
	mov.l	@@r5+,r2
	mov.l	r0,@@(8,r4)
	mov.l	r1,@@(12,r4)
	rts
	 mov.l	r2,@@(16,r4)


NENTRY(__movstr_i4_even)
	add	#-8,r4
	nop
even_loop:
	mov.l	@@r5+,r0
	add	#8,r4
	mov.l	@@r5+,r1
	dt	r6
	mov.l	r0,@@(0,r4)
	bf/s	even_loop
	 mov.l	r1,@@(4,r4)

	mov.l	@@r5+,r0
	mov.l	@@r5+,r1
	mov.l	r0,@@(8,r4)
	rts
	 mov.l	r1,@@(12,r4)


/* gcc4 uses movmem, older versions use movstr */
STRONG_ALIAS(__movmem_i4_odd, __movstr_i4_odd)
STRONG_ALIAS(__movmem_i4_even, __movstr_i4_even)
@


