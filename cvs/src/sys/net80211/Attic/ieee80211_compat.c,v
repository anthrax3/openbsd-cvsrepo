#include <sys/param.h>
#include <sys/socket.h>
#include <sys/systm.h>
#include <net/if.h>
#include <net80211/ieee80211_compat.h>

void
if_printf(struct ifnet *ifp, const char *fmt, ...)
{
	va_list ap;
	va_start(ap, fmt);

	printf("%s: ", ifp->if_xname);
	vprintf(fmt, ap);

	va_end(ap);
	return;
}
