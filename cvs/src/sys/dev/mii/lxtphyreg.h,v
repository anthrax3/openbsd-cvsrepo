@/*	$OpenBSD: lxtphyreg.h,v 1.1 1998/11/11 19:34:47 jason Exp $	*/
/*	$NetBSD: lxtphyreg.h,v 1.1 1998/10/24 00:33:17 thorpej Exp $	*/

