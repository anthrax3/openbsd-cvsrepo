/* AIMS Lab Radiotrack FM Radio Card device driver */

/*
 * Sanyo LM7000 Direct PLL Frequency Synthesizer
 */

#ifndef _RTVAR_H_
#define _RTVAR_H_

struct rt_softc {
	struct device	sc_dev;

	int		sc_ct;	/* card type */
	int		sc_mute;
	u_int8_t	sc_vol;
	u_int32_t	sc_freq;
	u_int32_t	sc_rf;
	u_int32_t	sc_stereo;

	struct lm700x_t	lm;
};

void	rtattach(struct rt_softc *);

#endif /* _RTVAR_H_ */
@


