/*
 * This is glue to attach keyboard/mouse line disciplines to any serial
 * driver as a `tty' device (line discipline).
 */

struct tty;
struct consdev;

struct kbd_ms_tty_attach_args {
	char *kmta_name;	/* Either "keyboard" or "mouse" */
	dev_t kmta_dev;
	struct tty *kmta_tp;	/* tty to take over */
	struct consdev *kmta_consdev;	/* Tells us we're the console. */
	int kmta_baud;
	int kmta_cflag;
};

