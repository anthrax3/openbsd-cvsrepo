struct tigon_firmware {
	int		FwReleaseMajor;
	int		FwReleaseMinor;
	int		FwReleaseFix;
	u_int32_t	FwStartAddr;

	u_int32_t	FwTextAddr;
	int		FwTextLen;
	u_int32_t	FwRodataAddr;
	int		FwRodataLen;

	u_int32_t 	FwDataAddr;
	int		FwDataLen;
	u_int32_t	FwSbssAddr;
	int		FwSbssLen;

	u_int32_t	FwBssAddr;
	int		FwBssLen;

	int		FwTextOffset;
	int		FwRodataOffset;
	int		FwDataOffset;

	u_char		data[1];
};
@


