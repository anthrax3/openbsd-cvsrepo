a1 1
/*	$OpenBSD$	*/
d52 3
a54 3
void	plx9060_read_eeprom __P((struct plx9060_config *, int, int, void *));
void	plx9060_write_eeprom __P((struct plx9060_config *, int, int,
	    const void *));
@


