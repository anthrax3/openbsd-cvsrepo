text
@d1 1
a1 1
/*	$OpenBSD: cninit.c,v 1.10 2010/06/26 23:24:44 guenther Exp $	*/
a81 26
}

int
cnset(dev_t dev)
{
	struct consdev *cp;

	/*
	 * Look for the specified console device and use it.
	 */
	for (cp = constab; cp->cn_probe; cp++) {
		if (major(cp->cn_dev) == major(dev)) {
			/* Short-circuit noop. */
			if (cp == cn_tab && cp->cn_dev == dev)
				return (0);
			if (cp->cn_pri != CN_DEAD) {
				cn_tab = cp;
				cp->cn_dev = dev;
				/* Turn it on.  */
				(*cp->cn_init)(cp);
				return (0);
			}
			break;
		}
	}
	return (1);
@


