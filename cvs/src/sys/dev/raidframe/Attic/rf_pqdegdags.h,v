/*
 * rf_pqdegdags.c
 * Degraded mode dags for double fault cases.
 */

#ifndef	_RF__RF_PQDEGDAGS_H_
#define	_RF__RF_PQDEGDAGS_H_

#include "rf_dag.h"

RF_CREATE_DAG_FUNC_DECL(rf_PQ_DoubleDegRead);
