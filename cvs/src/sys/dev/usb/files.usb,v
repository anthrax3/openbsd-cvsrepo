1.133
log
@"urng" is needed on the file lines... or these end up in all kernels,
including ramdisks without usb support...
@
text
@#	$OpenBSD: files.usb,v 1.132 2017/08/28 19:31:57 jasper Exp $
#	$NetBSD: files.usb,v 1.16 2000/02/14 20:29:54 augustss Exp $
#
# Config file and device description for machine-independent USB code.
# Included by ports that need it.  Ports that use it must provide
# their own "major" declarations for the appropriate devices.

device	usb {}
attach	usb at usbus
file	dev/usb/usb.c			usb			needs-flag
file	dev/usb/usbdi.c			usb
file	dev/usb/usbdi_util.c		usb
file	dev/usb/usb_mem.c		usb
file	dev/usb/usb_subr.c		usb
file	dev/usb/usb_quirks.c		usb

# Hub driver
