d52 5
a56 1
#define	ABICALLS
d63 1
a63 1
 * Define how to access unaligned data word
d65 16
a80 4
#define LWLO    lwr
#define LWHI    lwl
#define	SWLO	swr
#define	SWHI	swl
d86 8
a93 7
#define	MCOUNT	.set noreorder; \
		.set noat; \
		move $1,$31; \
		jal _mcount; \
		subu sp,sp,8; \
		.set reorder; \
		.set at;
d103 6
a108 5
#define LEAF(x) \
	.globl x; \
	.ent x, 0; \
x: ; \
	.frame sp, 0, ra; \
d111 4
d120 6
a125 12
#define NLEAF(x) \
	.globl x; \
	.ent x, 0; \
x: ; \
	.frame sp, 0, ra

/*
 * ALEAF -- declare alternate entry to a leaf routine.
 */
#define	ALEAF(x)					\
	.globl	x;					\
x:
d133 3
a135 3
	.globl x; \
	.ent x, 0; \
x: ; \
d137 1
d147 5
a151 4
	.globl x; \
	.ent x, 0; \
x: ; \
	.frame sp, fsize, retpc
