#ifndef _M88K_ENDIAN_H_
#define _M88K_ENDIAN_H_

#define _BYTE_ORDER _BIG_ENDIAN
#define	__STRICT_ALIGNMENT

#ifndef __FROM_SYS__ENDIAN
#include <sys/endian.h>
#endif

#endif /* _M88K_ENDIAN_H_ */

@


