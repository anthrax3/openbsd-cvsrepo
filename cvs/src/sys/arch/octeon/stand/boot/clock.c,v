#include <sys/types.h>

#include "libsa.h"

/* Arbitrarily chosen value, needs talking to cp0 for a delay loop. */
#define DELAY_MULTIPLIER 10000

void
delay(int ms)
{
	volatile int i = ms * DELAY_MULTIPLIER;
	for (; --i;)
		;
}
@


