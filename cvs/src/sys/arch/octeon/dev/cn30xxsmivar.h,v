text
@d1 1
a1 1
/*	$OpenBSD$	*/
a30 6

#ifndef SET
#define	SET(t, f)	((t) |= (f))
#define	ISSET(t, f)	((t) & (f))
#define	CLR(t, f)	((t) &= ~(f))
#endif
@

