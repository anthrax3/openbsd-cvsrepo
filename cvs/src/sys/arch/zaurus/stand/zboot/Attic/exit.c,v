#include <sys/stdarg.h>

#include "libsa.h"

/* unixcons.c */
extern struct termios tioc;
extern int            tioc_valid;
extern __dead void uexit(int);

__dead void
panic(const char *fmt, ...)
{
	extern void closeall(void);
	va_list ap;
	static int paniced;

	if (!paniced) {
		paniced = 1;
		closeall();
	}

	va_start(ap, fmt);
	vprintf(fmt, ap);
	printf("\n");
	va_end(ap);
#if 0
	_rtt();
#else
	uexit(0);
#endif
	/*NOTREACHED*/
}

void
exit(void)
{
	if (tioc_valid)
		(void)tcsetattr(0, TCSADRAIN, &tioc);
	uexit(0);
}
@


