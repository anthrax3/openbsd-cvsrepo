#include <machine/asm.h>

	.text
ENTRY(ntohl)
ENTRY(htonl)
	movd	S_ARG0,r0
	rotw	8,r0
	rotd	16,r0
	rotw	8,r0
	ret	0


ENTRY(ntohs)
ENTRY(htons)
	movzwd	S_ARG0,r0
	rotw	8,r0
	ret	0
@


