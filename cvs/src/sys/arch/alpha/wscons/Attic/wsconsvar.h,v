@/*	$OpenBSD: wsconsvar.h,v 1.5 1997/07/31 13:40:06 kstailey Exp $	*/
/*	$NetBSD: wsconsvar.h,v 1.4 1996/11/19 05:17:00 cgd Exp $	*/

