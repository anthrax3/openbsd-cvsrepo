text
@d1 1
a1 1
/*	$NetBSD: support.c,v 1.1 1995/02/13 23:07:13 cgd Exp $	*/
a34 20

void
blkclr(void *b, size_t len)
{
	bzero(b, len);
}

void
ovbcopy(void *a, void *b, size_t len)
{
	if (b < a) {
		while (len--)
			*((char *)b)++ = *((char *)a)++;
	} else {
		((char *)a) += len;
		((char *)b) += len;
		while (len--)
			*--((char *)b) = *--((char *)a);
	}
}
@


