/*
 * Some C support functions that aren't (yet) in libkern or assembly.
 */
#include <sys/param.h>
#include <sys/systm.h>
#include <sys/errno.h>

struct qelem {
	struct qelem *q_forw;
	struct qelem *q_back;
};

void
_insque(entry, pred)
	void *entry;
	void *pred;
{
	struct qelem *e = (struct qelem *) entry;
	struct qelem *p = (struct qelem *) pred;

	e->q_forw = p->q_forw;
	e->q_back = p;
	p->q_forw->q_back = e;
	p->q_forw = e;
}

void
_remque(element)
	void *element;
{
	struct qelem *e = (struct qelem *) element;
	e->q_forw->q_back = e->q_back;
	e->q_back->q_forw = e->q_forw;
}
@


