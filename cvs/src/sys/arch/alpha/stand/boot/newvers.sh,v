r=`awk ' { print $3 ; exit } ' < $1`

echo "char bootprog_name[] = \"OpenBSD/alpha boot ${r}\";" > vers.c
#echo "char bootprog_rev[] = \"${r}\";" >> vers.c
#echo "char bootprog_date[] = \"${t}\";" >> vers.c
#echo "char bootprog_maker[] = \"${u}@@${h}\";" >> vers.c
