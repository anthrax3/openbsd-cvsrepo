	sed -e s,./,, | sort`
d8 20
a27 16
for dir in $dirlist; do
	echo === $dir
	echo ""
	echo "***"
	echo ""
	(cd $dir ; time make -k clean)
	echo ""
	echo "***"
	echo ""
	(cd $dir ; time make -k depend)
	echo ""
	echo "***"
	echo ""
	(cd $dir ; time make -k)
	echo ""
