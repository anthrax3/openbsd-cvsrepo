/*
 * Can't use _C_LABEL here.
 */

#define _MCOUNT_SYM "__mcount"
#define _MCOUNT_ENTRY "_mcount"

#ifdef __PIC__
/* Inline expansion of PICCY_SET() (see <machine/asm.h>). */
#define MCOUNT \
	__asm__(".global " _MCOUNT_ENTRY);\
	__asm__(".text");\
	__asm__(_MCOUNT_ENTRY ":");\
	__asm__("add %o7, 8, %o1");\
	__asm__("1: call 2f; nop; 2:");\
	__asm__("add %o7," _MCOUNT_SYM "-1b, %o2");\
	__asm__("ld [%o2], %o2");\
	__asm__("jmpl %o2, %g0");\
	__asm__("add %i7, 8, %o0");
#else
#define MCOUNT \
	__asm__(".global " _MCOUNT_ENTRY);\
	__asm__(".text");\
	__asm__(_MCOUNT_ENTRY ":");\
	__asm__("add %i7, 8, %o0");\
	__asm__("sethi %hi(" _MCOUNT_SYM "), %o2");\
	__asm__("jmpl %o2 + %lo(" _MCOUNT_SYM "), %g0");\
	__asm__("add %o7, 8, %o1");
#endif

#define	_MCOUNT_DECL	static void __mcount

#ifdef _KERNEL
/*
 * Block interrupts during mcount so that those interrupts can also be
 * counted (as soon as we get done with the current counting).  On the
 * SPARC, we just splhigh/splx as those do not recursively invoke mcount.
 */
#define	MCOUNT_ENTER	s = splhigh()
#define	MCOUNT_EXIT	splx(s)
#endif /* _KERNEL */
@


