#ifndef _M68K_EXEC_H_
#define _M68K_EXEC_H_

#define __LDPGSZ	8192

#define ARCH_ELFSIZE		32

#define ELF_TARG_CLASS          ELFCLASS32
#define ELF_TARG_DATA           ELFDATA2MSB
#define ELF_TARG_MACH           EM_68K

#define _NLIST_DO_ELF
#define _KERN_DO_ELF

#endif  /* _M68K_EXEC_H_ */
@


