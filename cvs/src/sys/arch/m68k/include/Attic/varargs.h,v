1.2
log
@varargs.h needs ... in va_dcl for gcc >= 2; from arnej@@imf.unit.no; netbsd pr#1695
@
text
@d1 1
a1 1
/*	$NetBSD: varargs.h,v 1.10 1995/03/26 17:08:41 briggs Exp $	*/
d46 1
a46 1
#include <machine/ansi.h>
d48 5
a52 1
typedef _BSD_VA_LIST_	va_list;
d54 2
a55 4
#define	va_dcl	int va_alist; ...

#define	__va_promote(type) \
	(((sizeof(type) + sizeof(int) - 1) / sizeof(int)) * sizeof(int))
d57 1
d59 1
a59 14
	ap = (char *)&va_alist

#ifdef _KERNEL
#define	va_arg(ap, type) \
	((type *)(ap += sizeof(type)))[-1]
#else
#define	va_arg(ap, type) \
	((type *)(ap += __va_promote(type),				\
		ap - (sizeof(type) < sizeof(int) &&			\
		    sizeof(type) != __va_promote(type) ?		\
		    sizeof(type) : __va_promote(type))))[0]
#endif

#define	va_end(ap)	((void) 0)
@


