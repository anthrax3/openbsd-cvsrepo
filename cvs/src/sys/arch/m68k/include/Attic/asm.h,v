/*	$OpenBSD: asm.h,v 1.8 2007/11/24 20:58:26 deraadt Exp $	*/
d45 2
a46 5
#ifdef	__ELF__
#define	_C_LABEL(name)		name
#else
#ifdef __STDC__
#define	_C_LABEL(name)		_ ## name
d48 1
a48 2
#define	_C_LABEL(name)		_/**/name
#endif /* __STDC__ */
d51 1
d63 1
a63 1
#define _PROF_PROLOG	link a6,#0; jbsr mcount; unlk a6
a96 1
#ifdef	__ELF__
a99 11
#else
#ifdef	__STDC__
#define	WEAK_ALIAS(alias,sym)				\
	.weak _##alias;				\
	_##alias = _##sym
#else
#define	WEAK_ALIAS(alias,sym)				\
	.weak _/**/alias;				\
	_/**/alias = _/**/sym
#endif
#endif
