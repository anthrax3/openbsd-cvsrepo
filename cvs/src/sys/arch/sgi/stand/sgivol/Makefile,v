1.3
log
@simple man page for sgivol
@
text
@#	$OpenBSD: Makefile,v 1.2 2004/08/25 18:19:35 pefo Exp $
#	$NetBSD: Makefile,v 1.5 2002/12/13 02:36:37 lukem Exp $

MAN=	sgivol.8
MANSUBDIR=sgi

.if ${MACHINE} == "sgi"
PROG=	sgivol
LDADD+=	-lutil
LDSTATIC=-static

.else
NOPROG=
.endif

.include <bsd.prog.mk>
@


