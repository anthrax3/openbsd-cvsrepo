/* variables per Upa */
struct upa_softc {
	struct	device uc_dev;		/* base device */
	bus_space_tag_t	uc_bustag;
	bus_dma_tag_t	uc_dmatag;
	int	uc_clockfreq;		/* clock frequency (in Hz) */
	struct	upadev *uc_sbdev;	/* list of all children */
};

