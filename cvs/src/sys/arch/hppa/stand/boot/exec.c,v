1.1
log
@use loadfile
@
text
@d1 1
a1 1
/*	$OpenBSD$	*/
d45 1
a45 1
exec(char *file, void *addr, int howto)
a46 8
	u_long marks[MARK_MAX];

	marks[MARK_START] = (u_long)addr;

	if (loadfile(file, marks, LOAD_KERNEL))
		return;

	marks[MARK_END] = ALIGN(marks[MARK_END] - (u_long)addr);
@


