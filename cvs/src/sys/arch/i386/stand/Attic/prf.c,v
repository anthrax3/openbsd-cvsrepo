#include <sys/types.h>

putchar(c)
char c;
{
        if (c == '\n')
		sput('\r');
	sput(c);
	return(0);
}

wait(n) {
	int v;

	while(n-- && (v = scankbd()) == 0);
	if (v) kbdreset();
}
