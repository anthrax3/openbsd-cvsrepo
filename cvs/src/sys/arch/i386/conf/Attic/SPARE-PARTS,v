text
@#
#	SPARE-PARTS: spare-parts.downsj.com
#
#	$OpenBSD: SPARE-PARTS,v 1.11 1997/03/03 04:09:16 downsj Exp $
#

machine		i386
option		I486_CPU

maxusers	8
