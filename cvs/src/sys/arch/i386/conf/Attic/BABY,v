#option		CCITT,LLC,HDLC	# X.25
option		IPFILTER	# IP packet filter for security
option		IPFILTER_LOG	# use /dev/ipl to log IPF

config		bsd	root on sd0 swap on sd0 and sd1 and sd2 and sd3 and sd4 dumps on sd2

mainbus0 at root

bios0	at mainbus0
apm0	at bios0

