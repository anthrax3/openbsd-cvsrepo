#ifndef __ARM32_FP_H
#define __ARM32_FP_H

typedef struct {
	u_int32_t fp_exp;
	u_int32_t fp_man_hi;
	u_int32_t fp_man_lo;
} fp_reg_t;

struct fp_state {
	unsigned int fp_flags;
	unsigned int fp_sr;
	unsigned int fp_cr;
	fp_reg_t fp_registers[16];
};

#endif

/* End of fp.h */
