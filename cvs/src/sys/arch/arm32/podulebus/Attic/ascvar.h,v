#define _ASCVAR_H_

#include <arm32/podulebus/sbicvar.h>
#include <arm32/podulebus/ascreg.h>

#define ASC_POLL 1

struct	asc_softc {
	struct sbic_softc	sc_softc;

	podule_t		*sc_podule;
	int			sc_podule_number;
	irqhandler_t		sc_ih;

	vu_int			sc_pagereg;
	vu_int			sc_intstat;
	};

#endif /* _ASCVAR_H_ */
