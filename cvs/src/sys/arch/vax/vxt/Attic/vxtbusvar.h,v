/*
 * VXT2000 Hardware addresses
 */
#define	SGECADDR_VXT	0x20008000
#define	QSCADDR		0x200a0000
#define	NISA_ROM_VXT	0x200c4000

#define	VXT_INTRVEC	0x200

void	vxtbus_intr_establish(const char *, int, int (*)(void *), void *);
@


