#define __va_promote(type) \
        (((sizeof(type) + sizeof(int) - 1) / sizeof(int)) * sizeof(int))
d49 1
a49 1
        (ap = ((char *)&(last) + __va_promote(last)))
d51 2
a52 8
#ifdef _KERNEL
#define va_arg(ap, type) \
        ((type *)(ap += sizeof(type)))[-1]
#else
#define va_arg(ap, type) \
        ((type *)(ap += sizeof(type) < sizeof(int) ? \
                (abort(), 0) : sizeof(type)))[-1]
#endif
d54 1
