#ifndef _MACHINE_REG_H_
#define _MACHINE_REG_H_

struct  reg {
	int	r0;     /* General registers saved upon trap/syscall */
	int	r1;
	int	r2;
	int	r3;
	int	r4;
	int	r5;
	int	r6;
	int	r7;
	int	r8;
	int	r9;
	int	r10;
	int	r11;
	int	ap;     /* Argument pointer on user stack */
	int	fp;     /* Stack frame pointer */
	int	sp;
	int	pc;     /* User pc */
	int	psl;    /* User psl */
};

#endif /* _MACHINE_REG_H_ */
