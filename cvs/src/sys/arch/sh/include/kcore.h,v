#ifndef	_SH_KCORE_H_
#define	_SH_KCORE_H_

/* this should be >= VM_PHYSSEG_MAX from <machine/vmparam.h> */
#define	NPHYS_RAM_SEGS	8

typedef struct cpu_kcore_hdr {
	paddr_t		kcore_kptp;
	unsigned int	kcore_nsegs;
	phys_ram_seg_t	kcore_segs[NPHYS_RAM_SEGS];
} cpu_kcore_hdr_t;

#endif	/* _SH_KCORE_H_ */
@
