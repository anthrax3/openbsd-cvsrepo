#include <sys/param.h>
#include <sys/reboot.h>

#include <machine/mon.h>

#include "stand.h"
#include "promboot.h"

/*
 * Boot device is derived from ROM provided information.
 */
#define LOADADDR	0x4000

extern char		*version;
char	defname[32] = "bsd";
char	line[80];


main()
{
	char *cp, *file;
	int	io;

	printf(">> OpenBSD netboot [%s]\n", version);
	prom_get_boot_info();
	file = defname;

	cp = prom_bootfile;
	if (cp && *cp)
		file = cp;

	for (;;) {
		if (prom_boothow & RB_ASKNAME) {
			printf("boot: ");
			gets(line);
			if (line[0])
				file = line;
		}
		exec_sun(file, LOADADDR);
		printf("boot: %s: %s\n", file, strerror(errno));
		prom_boothow |= RB_ASKNAME;
	}
}
@


