#include <sys/types.h>

#include "prom.h"

int
getchar(void)
{
	u_int ret;

	SCM_CALL(SCM_CHAR);
	__asm__ volatile ("or %0, %%r0, %%r2" : "=r" (ret));

	return (ret & 0xff);
}
@


