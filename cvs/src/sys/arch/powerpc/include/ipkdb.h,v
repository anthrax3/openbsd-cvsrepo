/* register array */
#define	FIX	0
#define	LR	32
#define	CR	33
#define	CTR	34
#define	XER	35
#define	PC	36
#define	MSR	37
#define	NREG	38

#ifndef _LOCORE
extern int ipkdbregs[NREG];

/* Doesn't handle overlapping regions */
__inline extern void
ipkdbcopy(s,d,n)
	void *s, *d;
	int n;
{
	char *sp = s, *dp = d;
	
	while (--n >= 0)
		*dp++ = *sp++;
}

__inline extern void
ipkdbzero(d,n)
	void *d;
	int n;
{
	char *dp = d;
	
	while (--n >= 0)
		*dp++ = 0;
}

__inline extern int
ipkdbcmp(s,d,n)
	void *s, *d;
{
	char *sp = s, *dp = d;
	
	while (--n >= 0)
		if (*sp++ != *dp++)
			return *--dp - *--sp;
	return 0;
}
#endif /* _LOCORE */
@


