#include <sys/param.h>
#include <sys/errno.h>
#include <sys/systm.h>

/*
 * Emulate copystr.
 */
int
copystr(const void *kfaddr, void *kdaddr, size_t len, size_t *done)
{
	u_char *kfp = (u_char *)kfaddr;
	u_char *kdp = kdaddr;
	size_t l;
	
	for (l = 0; len-- > 0; l++) {
		if (!(*kdp++ = *kfp++)) {
			if (done)
				*done = l + 1;
			return (0);
		}
	}
	if (done)
		*done = l;
	return (ENAMETOOLONG);
}
@


