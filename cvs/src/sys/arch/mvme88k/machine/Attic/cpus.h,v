/*
 *
 * HISTORY
 */
/* 
  Versions Idents for 88k family chips 
 */

#ifndef _M88K_CPUS_
#define _M88K_CPUS_

/*
 * cpu Processor Identification Register (PID).
 */
#ifndef ASSEMBLER
union cpupid {
    unsigned cpupid;
    struct {
	unsigned
	    /*empty*/:16,
	    arc:8,
	    version:7,
	    master:1;
    } m88100;
    struct {
	unsigned
	    id:8,
	    type:3,
	    version:5,
	    /*empty*/:16;
    } m88200;
};
#endif ASSEMBLER

#define	M88100		0
#define M88200		5
#define M88204		6

#endif  _M88K_CPUS_
@


