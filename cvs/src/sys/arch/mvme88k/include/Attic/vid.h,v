	unsigned char	cfg_ssc;
	unsigned char	cfg_runit;
	unsigned short	cfg_rsvc1;
	unsigned short	cfg_rsvc2;
	unsigned char	cfg_4[196];
};
#endif
#endif /* __MACHINE_VID_H__ */
