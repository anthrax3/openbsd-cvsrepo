/*	$OpenBSD: varargs.h,v 1.5 2001/11/16 01:17:21 miod Exp $	*/

#ifndef _M88K_STDARGS_H_
#define _M88K_STDARGS_H_

#include <machine/ansi.h>

#ifndef	_STDARG_H
