#include <stdarg.h>
#include <sys/types.h>

int
getchar()
{
	char c;

	__asm("movb #0,sp@@-; trap #15; .short 0x0; movb sp@@+, %0" : : "d" (c));
	return (c);
}

peekchar()
{
	int have = 0;

	__asm("trap #15; .short 0x1; beq 1f; movl #1, %0\n1:" : : "d" (have));
	return (have);
}

void
putchar(c)
	int c;
{
	if (c == '\n')
		putchar('\r');
	__asm("movb %0,sp@@-; trap #15; .short 0x20" : : "d" (c));
}

@


