		| start at 0x4000, load at 0xa000, stack at 0x9ff0.
		.text
		.globl start
start:		movb	#0,_reboot
		jra	Ldoit
restart:	movb	#1,_reboot		| fall through

Ldoit:		movl	#0x00006ff0,sp
		jsr	_main

		.globl	___main
___main:	rts
@


