/*	$OpenBSD: dev_hppa64.c,v 1.5 2010/12/06 22:51:45 jasper Exp $	*/
a190 16
}

int
getchar()
{
	int c = cngetc();

	if (c == '\r')
		c = '\n';

	if ((c < ' ' && c != '\n') || c == '\177')
		return(c);

	putchar(c);

	return(c);
