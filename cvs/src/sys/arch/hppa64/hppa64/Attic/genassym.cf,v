struct	hppa_fpstate
member	hfp_regs
member	hfp_cpu

struct	uvmexp
member	fpswtch

# system calls
export	SYSCALLGATE
export	SYS_exit
export	SYS_execve
export	SYS_sigreturn

# errno
export	EFAULT
export	ENAMETOOLONG
