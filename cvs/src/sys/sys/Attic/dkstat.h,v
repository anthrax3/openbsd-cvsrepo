#define	CP_USER		0
#define	CP_NICE		1
#define	CP_SYS		2
#define	CP_INTR		3
#define	CP_IDLE		4
#define	CPUSTATES	5

#ifdef _KERNEL
extern int64_t tk_cancc, tk_nin, tk_nout, tk_rawcc;
#endif
