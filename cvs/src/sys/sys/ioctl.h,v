#ifndef	_SYS_IOCTL_H_
#define	_SYS_IOCTL_H_

#include <sys/ttycom.h>
#include <sys/filio.h>
#include <sys/sockio.h>

#ifndef _KERNEL

#include <sys/cdefs.h>

