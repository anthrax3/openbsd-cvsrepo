d80 3
a82 3
.globl _normalize

_normalize:
d136 1
a136 1
	call	_arith_underflow
d142 1
a142 1
	call	_arith_overflow
d149 3
d153 3
a155 3
.globl _normalize_nuo

_normalize_nuo:
@


