#ifndef _NETINET_TCP_DEBUG_H_
#define _NETINET_TCP_DEBUG_H_

#include <netinet6/tcpipv6.h>

struct	tcp_debug {
	uint32_t td_time;
	short	td_act;
	short	td_ostate;
	caddr_t	td_tcb;
	struct	tcpiphdr td_ti;
	struct  tcpipv6hdr td_ti6;
	short	td_req;
	struct	tcpcb td_cb;
};

#define	TA_INPUT	0
#define	TA_OUTPUT	1
#define	TA_USER		2
#define	TA_RESPOND	3
#define	TA_DROP		4

#ifdef TANAMES
char	*tanames[] =
    { "input", "output", "user", "respond", "drop" };
#endif /* TANAMES */

#define	TCP_NDEBUG 100
extern struct	tcp_debug tcp_debug[];
extern int	tcp_debx;
#endif /* _NETINET_TCP_DEBUG_H_ */
@


