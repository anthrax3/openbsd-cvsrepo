#ifndef _NETINET_IN_SYSTM_H_
#define _NETINET_IN_SYSTM_H_

/* network byte order */
typedef u_int16_t n_short;
typedef u_int32_t n_long;
typedef u_int32_t n_time;		/* ms since 00:00 GMT */

#ifdef _KERNEL
u_int32_t iptime(void);
#endif /* _KERNEL */

#endif /* _NETINET_IN_SYSTM_H_ */
@


