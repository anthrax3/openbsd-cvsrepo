#ifndef	_IBCS2_TIMEB_H
#define	_IBCS2_TIMEB_H

struct xenix_timeb {
        ibcs2_time_t time;
	unsigned short millitm;
	short timezone;
	short dstflag;
} __packed;

#define xenix_timeb_len 10      /* packed struct */

#endif /* _IBCS2_TIMEB_H */
