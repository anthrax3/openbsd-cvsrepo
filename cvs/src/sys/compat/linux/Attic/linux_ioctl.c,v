			return error;
		SCARG(&ia, data) = (caddr_t) abts;
		return sys_ioctl(p, &ia, retval);
	case LINUX_TIOCSETD:
		if ((error = copyin(data, (caddr_t) &idat, sizeof idat)))
			return error;
		switch (idat) {
		case LINUX_N_TTY:
