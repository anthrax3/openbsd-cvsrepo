/*
 * Signal-related HPUX compatibility routines and data types
 */

typedef
struct hpux_sigaltstack {
	void	*ss_sp;
	int	ss_flags;
	size_t	ss_size;
} hpux_stack_t;
#define	HPUX_SS_DISABLE	0x0002

#ifdef _KERNEL
int	hpuxtobsdsig(int);
int	bsdtohpuxsig(int);
int	hpuxtobsdmask(int);
int	bsdtohpuxmask(int);
#endif /* _KERNEL */
@


