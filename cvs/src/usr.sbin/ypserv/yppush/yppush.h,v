#define _YPPUSH_H_RPCGEN

#include <rpc/rpc.h>

#define YPPUSH_XFRRESPPROG	((u_long)0x40000000)
#define YPPUSH_XFRRESPVERS	((u_long)1)
#define YPPUSHPROC_NULL		((u_long)0)
#define YPPUSHPROC_XFRRESP	((u_long)1)

__BEGIN_DECLS
