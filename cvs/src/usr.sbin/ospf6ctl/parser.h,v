#ifndef _PARSER_H_
#define _PARSER_H_

#include <sys/types.h>
#include <net/if.h>
#include <netinet/in.h>

enum actions {
	NONE,
	FIB,
	FIB_COUPLE,
	FIB_DECOUPLE,
	LOG_VERBOSE,
	LOG_BRIEF,
	SHOW,
	SHOW_SUM,
	SHOW_IFACE,
	SHOW_IFACE_DTAIL,
	SHOW_NBR,
	SHOW_NBR_DTAIL,
	SHOW_DB,
	SHOW_DBBYAREA,
	SHOW_DBEXT,
	SHOW_DBLINK,
	SHOW_DBNET,
	SHOW_DBRTR,
	SHOW_DBINTRA,
	SHOW_DBSELF,
	SHOW_DBSUM,
	SHOW_DBASBR,
	SHOW_RIB,
	SHOW_RIB_DTAIL,
	SHOW_FIB,
	RELOAD
};

struct parse_result {
	struct in6_addr	addr;
	char		ifname[IF_NAMESIZE];
	int		flags;
	enum actions	action;
	u_int8_t	prefixlen;
};

struct parse_result	*parse(int, char *[]);
int			 parse_addr(const char *, struct in6_addr *);
int			 parse_prefix(const char *, struct in6_addr *,
			     u_int8_t *);

#endif	/* _PARSER_H_ */
@


