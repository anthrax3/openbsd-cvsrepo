#ifndef	DEBUG_MACRO_H
#define	DEBUG_MACRO_H 1

#if ((__STDC_VERSION__ - 0) >= 199901L)
#define _FUNC_  __func__
#else
#define _FUNC_  ""
#endif

#ifndef ASSERT
#ifdef	DEBUG
#define ASSERT(x)	\
	if (!(x)) { \
	    fprintf(stderr, \
		"\nASSERT(%s) failed on %s() at %s:%d.\n" \
		, #x, _FUNC_, __FILE__, __LINE__); \
	    abort(); \
	}
#else
#define ASSERT(x)	((void)0);
#endif
#endif

#endif
@


