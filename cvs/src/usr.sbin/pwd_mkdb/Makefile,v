1.5
log
@make static for install purposes
@
text
@d1 1
a1 1
#	$OpenBSD: Makefile,v 1.4 1997/09/21 11:44:07 deraadt Exp $
a4 1
SRCS=	pwd_mkdb.c getpwent.c
a8 2
.PATH:	${.CURDIR}/../../lib/libc/gen

a9 3

getpwent.o: getpwent.c
	${CC} ${CFLAGS} ${CPPFLAGS} -UYP -c ${.IMPSRC}
@


