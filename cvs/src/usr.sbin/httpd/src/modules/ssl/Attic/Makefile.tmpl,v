nocons:
	@@$(MAKE) $(MFLAGS) $(MFLAGS_STATIC) \
	     SSL_CFLAGS="`echo $(SSL_CFLAGS) |\
	     sed -e 's;-DSSL_CONSERVATIVE;;'`" all

cons:
	@@$(MAKE) $(MFLAGS) $(MFLAGS_STATIC) \
	     SSL_CFLAGS="`echo $(SSL_CFLAGS) |\
	     sed -e 's;-DSSL_CONSERVATIVE;;' \
	         -e 's;^;-DSSL_CONSERVATIVE ;'`" all
