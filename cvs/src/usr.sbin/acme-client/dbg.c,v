#include <err.h>
#include <stdarg.h>
#include <stdlib.h>

#include "extern.h"

void
doddbg(const char *fmt, ...)
{
	va_list		 ap;

	if (verbose < 2)
		return;

	va_start(ap, fmt);
	vwarnx(fmt, ap);
	va_end(ap);
}

void
dodbg(const char *fmt, ...)
{
	va_list		 ap;

	if (!verbose)
		return;

	va_start(ap, fmt);
	vwarnx(fmt, ap);
	va_end(ap);
}
