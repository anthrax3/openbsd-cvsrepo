/* $arla: stds.h,v 1.10 2003/06/12 05:28:19 lha Exp $ */

#ifndef __STDS_H
#define __STDS_H 1

/* for compat resons with other kerberos distributions */

#ifndef ANAME_SZ
#define ANAME_SZ 40
#endif
#ifndef INST_SZ
#define INST_SZ 40
#endif
#ifndef REALM_SZ
#define REALM_SZ 40
#endif

#if defined(KERBEROS) && !defined(HAVE_KRB_PRINCIPAL)

typedef struct krb_principal{
    char name[ANAME_SZ];
    char instance[INST_SZ];
    char realm[REALM_SZ];
}krb_principal;
#endif

#endif /* __STDS_H */
@


