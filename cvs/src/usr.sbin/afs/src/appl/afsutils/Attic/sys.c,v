/* 
 * sys - a version of the sys command
 */

#ifdef HAVE_CONFIG_H
#include <config.h>
#endif

RCSID("$arla: sys.c,v 1.3 2000/10/03 00:06:30 lha Exp $");

#include "appl_locl.h"

int
main (int argc, char **argv)
{
    const char *sysname = arla_getsysname();
    char buf[2048];
    int ret;

    if (sysname == NULL)
	sysname = "unknown";

    if (!k_hasafs()) {
	printf ("%s\n", sysname);
	exit (0);
    }

    ret = fs_get_sysname (buf, sizeof(buf));
    if (ret) {
	printf ("%s\n", sysname);
    } else {
	printf ("%s\n", buf);
    }
    return 0;
}
