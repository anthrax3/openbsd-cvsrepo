#ifdef HAVE_CONFIG_H
#include <config.h>
#endif

#include <stdio.h>
#include <unistd.h>
#include <fcntl.h>
#include <sys/types.h>
#include <sys/stat.h>

#include <err.h>
#include <roken.h>

#ifdef RCSID
RCSID("$KTH: mkdir2.c,v 1.3 2000/10/03 00:34:47 lha Exp $");
#endif

int
main(int argc, char *argv[])
{
    int ret;
    struct stat dot_sb, sb;

    set_progname (argv[0]);

    ret = mkdir ("foo", 0777);
    if (ret < 0)
	err (1, "mkdir foo");
    ret = lstat (".", &dot_sb);
    if (ret < 0)
	err (1, "lstat .");
    ret = lstat ("foo", &sb);
    if (ret < 0)
	err (1, "lstat foo");
    if (sb.st_nlink != 2)
	errx (1, "sb.st_link != 2");
    ret = lstat ("foo/.", &sb);
    if (ret < 0)
	err (1, "lstat foo/.");
    if (sb.st_nlink != 2)
	errx (1, "sb.st_link != 2");
    ret = lstat ("foo/..", &sb);
    if (ret < 0)
	err (1, "lstat foo");
    if (sb.st_nlink != dot_sb.st_nlink)
	errx (1, "sb.st_link != dot_sb.st_nlink");
    ret = rmdir ("foo");
    if (ret < 0)
	err (1, "rmdir foo");
    return 0;
}
