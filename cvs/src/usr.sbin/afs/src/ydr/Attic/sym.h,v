/* $arla: sym.h,v 1.10 2002/04/15 14:53:19 lha Exp $ */

#ifndef _SYM_
#define _SYM_

#include <bool.h>
#include <list.h>

struct Type;

typedef struct Type Type;

typedef enum
{
     YDR_TUNDEFINED, YDR_TSTRUCT, YDR_TENUM, YDR_TCONST, YDR_TENUMVAL,
     YDR_TTYPEDEF, YDR_TPROC
} SymbolType;

enum { TSPLIT = 1, TSIMPLE = 2, TMULTI = 4};

typedef struct {
     SymbolType type;
     char *name;
     union {
 	  List *list;
	  int val;
	  Type *type;
	  struct {
	      unsigned id;
	      List *arguments;
	      unsigned flags;
	      char *package;
	  } proc;
     } u;
     List *attrs;
} Symbol;

void initsym (void);
Symbol* addsym (char *);
Symbol* findsym (char *);
void symiterate (Bool (*func)(void *, void *), void *arg);

#endif /* _SYM_ */
@


