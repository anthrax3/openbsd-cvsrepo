#ifdef HAVE_CONFIG_H
#include <config.h>
RCSID("$Id: estrdup.c,v 1.2 2000/09/11 14:41:00 art Exp $");
#endif

#include <stdlib.h>
#include <err.h>

#include <roken.h>

/*
 * Like strdup but never fails.
 */

char *
estrdup (const char *str)
{
    char *tmp = strdup (str);

    if (tmp == NULL)
	err (1, "strdup");
    return tmp;
}
