int acl_initialize(char *acl_file, int perm);
int acl_exact_match(char *acl, char *principal);
int acl_check(char *acl, char *principal);
int acl_add(char *acl, char *principal);
int acl_delete(char *acl, char *principal);

#endif /* __ACL_H */
