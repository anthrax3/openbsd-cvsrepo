/*
 * Boolean
 */

/* $arla: bool.h,v 1.5 2003/01/10 12:38:40 lha Exp $ */

#ifndef _ARLAUTIL_BOOL_H
#define _ARLAUTIL_BOOL_H 1

#ifndef FALSE

#ifndef Bool

typedef enum {
  FALSE = 0, TRUE = 1
} Bool;

#else /* Bool */

#define FALSE	0
#define TRUE	1

#endif /* Bool */

#else /* FALSE */

#ifndef Bool

typedef int Bool;

#endif /* Bool */

#endif /* FALSE */

#endif /* _ARLAUTIL_BOOL_H */
@


