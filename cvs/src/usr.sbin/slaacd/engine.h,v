	uint32_t			if_index;
	struct sockaddr_in6		addr;
	struct in6_addr			mask;
	struct sockaddr_in6		gateway;
	struct sockaddr_rtdns		rdns;
	struct sockaddr_rtsearch	dnssl;
};

void		 engine(int, int);
int		 engine_imsg_compose_frontend(int, pid_t, void *, uint16_t);
