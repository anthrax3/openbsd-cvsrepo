1.16
log
@Enable building wsfontload on Loongson.

OK visa@@, mpi@@,Â deraadt@@
@
text
@#	$OpenBSD: Makefile,v 1.15 2016/09/03 13:37:46 guenther Exp $

.if ${MACHINE} == "i386" || ${MACHINE} == "amd64" || \
    ${MACHINE} == "alpha" || ${MACHINE} == "hppa" || \
    ${MACHINE} == "loongson"

PROG=	wsfontload
SRCS=	wsfontload.c

.else

NOPROG=yes

.endif

MAN=	wsfontload.8

.include <bsd.prog.mk>
@


