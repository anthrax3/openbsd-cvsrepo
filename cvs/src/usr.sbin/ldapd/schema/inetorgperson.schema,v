@attributetype ( 2.16.840.1.113730.3.1.1 NAME 'carLicense'
	DESC 'vehicle license or registration plate'
	EQUALITY caseIgnoreMatch
	SUBSTR caseIgnoreSubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

