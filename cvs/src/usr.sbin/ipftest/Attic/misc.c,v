1.2
log
@IP filter 3.0.1
@
text
@d41 1
a41 1
static	char	sccsid[] = "@@(#)misc.c	1.2 1/12/96 (C) 1995 Darren Reed";
a68 20


#ifdef	NEED_INET_ATON
u_long	inet_aton(buf)
char *buf;
{
	u_long	n;
	char	*s = (char *)&n;
	int	a, b, c, d;

	if (sscanf(buf, "%d.%d.%d.%d", &a, &b, &c, &d) != 4)
		return -1;

	*s++ = (u_char)a;
	*s++ = (u_char)b;
	*s++ = (u_char)c;
	*s++ = (u_char)d;
	return n;
}
#endif
@


