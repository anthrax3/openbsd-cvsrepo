				then
					{ echo "$as_me:$LINENO: result: none" >&5
echo "${ECHO_T}none" >&6; }
					{ { echo "$as_me:$LINENO: error: \"could not find thread libraries\"" >&5
echo "$as_me: error: \"could not find thread libraries\"" >&2;}
   { (exit 1); exit 1; }; }
				fi

