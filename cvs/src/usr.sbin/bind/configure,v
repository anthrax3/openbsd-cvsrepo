# Look for stylesheets we need.
#


XSLT_DB2LATEX_STYLE=""
{ echo "$as_me:$LINENO: checking for db2latex/xsl/docbook.xsl" >&5
echo $ECHO_N "checking for db2latex/xsl/docbook.xsl... $ECHO_C" >&6; }
for d in $db2latex_xsl_trees
do
	f=$d/db2latex/xsl/docbook.xsl
