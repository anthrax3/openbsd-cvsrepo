static inline isc_boolean_t
checkowner_in_naptr(ARGS_CHECKOWNER) {

	REQUIRE(type == 35);
	REQUIRE(rdclass == 1);

	UNUSED(name);
	UNUSED(type);
	UNUSED(rdclass);
	UNUSED(wildcard);

	return (ISC_TRUE);
}

static inline isc_boolean_t
checknames_in_naptr(ARGS_CHECKNAMES) {

	REQUIRE(rdata->type == 35);
	REQUIRE(rdata->rdclass == 1);

	UNUSED(rdata);
	UNUSED(owner);
	UNUSED(bad);

	return (ISC_TRUE);
