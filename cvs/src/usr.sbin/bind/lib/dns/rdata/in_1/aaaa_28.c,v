static inline isc_boolean_t
checkowner_in_aaaa(ARGS_CHECKOWNER) {

	REQUIRE(type == 28);
	REQUIRE(rdclass == 1);

	UNUSED(type);
	UNUSED(rdclass);

	return (dns_name_ishostname(name, wildcard));
}

static inline isc_boolean_t
checknames_in_aaaa(ARGS_CHECKNAMES) {

	REQUIRE(rdata->type == 28);
	REQUIRE(rdata->rdclass == 1);

	UNUSED(rdata);
	UNUSED(owner);
	UNUSED(bad);

	return (ISC_TRUE);
