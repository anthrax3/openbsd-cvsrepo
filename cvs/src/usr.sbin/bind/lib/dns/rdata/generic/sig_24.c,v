static inline isc_boolean_t
checkowner_sig(ARGS_CHECKOWNER) {

	REQUIRE(type == 24);

	UNUSED(name);
	UNUSED(type);
	UNUSED(rdclass);
	UNUSED(wildcard);

	return (ISC_TRUE);
}

static inline isc_boolean_t
checknames_sig(ARGS_CHECKNAMES) {

	REQUIRE(rdata->type == 24);

	UNUSED(rdata);
	UNUSED(owner);
	UNUSED(bad);

	return (ISC_TRUE);
