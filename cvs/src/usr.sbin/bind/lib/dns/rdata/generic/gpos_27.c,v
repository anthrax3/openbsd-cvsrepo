checkowner_gpos(ARGS_CHECKOWNER) {

	REQUIRE(type == 27);

	UNUSED(name);
	UNUSED(type);
	UNUSED(rdclass);
	UNUSED(wildcard);

	return (ISC_TRUE);
}

