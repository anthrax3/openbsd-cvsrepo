checkowner_unspec(ARGS_CHECKOWNER) {

	REQUIRE(type == 103);

	UNUSED(name);
	UNUSED(type);
	UNUSED(rdclass);
	UNUSED(wildcard);

	return (ISC_TRUE);
}

