static inline isc_boolean_t
checkowner_null(ARGS_CHECKOWNER) {

	REQUIRE(type == 10);

	UNUSED(name);
	UNUSED(type);
	UNUSED(rdclass);
	UNUSED(wildcard);

	return (ISC_TRUE);
}

