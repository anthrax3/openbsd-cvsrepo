static inline isc_boolean_t
checkowner_txt(ARGS_CHECKOWNER) {

	REQUIRE(type == 16);

	UNUSED(name);
	UNUSED(type);
	UNUSED(rdclass);
	UNUSED(wildcard);

	return (ISC_TRUE);
}

