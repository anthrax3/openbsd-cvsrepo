TARGETS =

@@BIND9_MAKE_RULES@@

installdirs:
	$(SHELL) ${top_srcdir}/mkinstalldirs ${DESTDIR}${includedir}/dns

install:: installdirs
	for i in ${HEADERS}; do \
		${INSTALL_DATA} ${srcdir}/$$i ${DESTDIR}${includedir}/dns ; \
	done
	for i in ${GENHEADERS}; do \
		${INSTALL_DATA} $$i ${DESTDIR}${includedir}/dns ; \
	done
@


