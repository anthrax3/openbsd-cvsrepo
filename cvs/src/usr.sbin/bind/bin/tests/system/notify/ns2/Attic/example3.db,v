$ORIGIN .
$TTL 300	; 5 minutes
example			IN SOA	mname1. . (
				3          ; serial
				300        ; refresh (300 seconds)
				300        ; retry (300 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
example.		NS	ns2.example.
ns2.example.		A	10.53.0.2
example.		NS	ns3.example.
ns3.example.		A	10.53.0.3

$ORIGIN example.
a			A	10.0.0.3
$TTL 3600	; 1 hour
a01			A	0.0.0.0
