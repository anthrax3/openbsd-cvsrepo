// NS5

options {
	query-source address 10.53.0.5;
	notify-source 10.53.0.5;
	transfer-source 10.53.0.5;
	port 5300;
	pid-file "named.pid";
	listen-on { 10.53.0.5; };
	listen-on-v6 { none; };
	recursion yes;
};

zone "." {
	type hint;
	file "../../common/root.hint";
};

include "trusted.conf";
