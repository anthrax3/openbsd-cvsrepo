srcdir =	@@srcdir@@
VPATH =		@@srcdir@@
top_srcdir =	@@top_srcdir@@

@@BIND9_MAKE_RULES@@

PERL = @@PERL@@

MANOBJS = options

doc man:: ${MANOBJS}

docclean manclean maintainer-clean::
	rm -f options

options: 
	../../bin/tests/cfg_test --named --grammar | \
		${PERL} ${srcdir}/format-options.pl >options || \
		rm -f options
@


