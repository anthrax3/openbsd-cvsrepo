#define TCPDUMP_ALIGN

#include <machine/endian.h>

/* 32-bit data types */
/* N.B.: this doesn't address printf()'s %d vs. %ld formats */
typedef	int32_t		int32;		/* signed 32-bit integer */
#ifndef	AUTH_UNIX
typedef	u_int32_t	u_int32;	/* unsigned 32-bit integer */
#endif
