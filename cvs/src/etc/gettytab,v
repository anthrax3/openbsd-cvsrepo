@sync & label
@
text
@d1 1
a1 1
#	$OpenBSD$
d12 1
a12 1
	:ap:im=\r\n%s/%m (%h) (%t)\r\n\r\n:sp#1200:
d115 1
a115 1
	:np:ap:sp#9600:
@


