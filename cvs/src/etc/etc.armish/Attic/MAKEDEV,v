a82 1
#	ss*	SCSI scanners
a262 8
ss*)
	M ss$U c 29 $(($U*16)) 640 operator
	M nss$U c 29 $(($(($U*16))+1)) 640 operator
	M enss$U c 29 $(($(($U*16))+3)) 640 operator
	RMlist[${#RMlist[*]}]="scan$U"
	MKlist[${#MKlist[*]}]=";umask 77;ln -s ss$U scan$U"
	;;

