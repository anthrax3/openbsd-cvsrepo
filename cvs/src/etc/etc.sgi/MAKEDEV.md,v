__devitem(zs, tty[a-b]*, Zilog 8530 serial port,zs)dnl
_mkdev(zs, {-tty[a-b]-}, {-u=${i#tty*}
	case $u in
	a) n=0 ;;
	b) n=1 ;;
	*) echo unknown tty device $i ;;
	esac
	M tty$u c major_zs_c $n 660 dialer root
	M cua$u c major_zs_c Add($n, 128) 660 dialer root-})dnl
_TITLE(make)
_DEV(all)
_DEV(ramd)
