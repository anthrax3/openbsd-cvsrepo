@#!/bin/sh
#
# $OpenBSD$

daemon="/usr/sbin/switchd"

. /etc/rc.d/rc.subr

# child will not return a config loading error to the parent
rc_pre() {
	${daemon} -n ${daemon_flags}
}

rc_cmd $1
@
