#include <sys/types.h>
#include <sys/sysctl.h>
#include <stdio.h>		/* for NULL */

#include "util.h"

int
getrawpartition(void)
{
	int rawpart, mib[2];
	size_t varlen;

	mib[0] = CTL_KERN;
	mib[1] = KERN_RAWPARTITION;
	varlen = sizeof(rawpart);
	if (sysctl(mib, 2, &rawpart, &varlen, NULL, (size_t)0) < 0)
		return (-1);

	return (rawpart);
}
@


