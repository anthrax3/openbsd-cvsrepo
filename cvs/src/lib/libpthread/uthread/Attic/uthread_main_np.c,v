#ifdef _THREAD_SAFE
#include <pthread.h>
#include <pthread_np.h>
#include "pthread_private.h"

/*
 * Provide the equivalent to Solaris thr_main() function
 */
int
pthread_main_np()
{

	if (!_thread_initial)
		return (-1);
	else
		return (pthread_equal(pthread_self(), _thread_initial) ? 1 : 0);
}
#endif
@


