#include <stdlib.h>
#include <unistd.h>

#include "debug.h"

#ifdef DEBUG
/*
 * debug level, -1 means uninitialized
 */
int ifuse_debug = -1;

void
ifuse_debug_init(void)
{
	char *dbg;

	if (ifuse_debug < 0) {
		dbg = issetugid() ? NULL : getenv("FUSE_DEBUG");
		if (!dbg || sscanf(dbg, "%u", &ifuse_debug) != 1)
			ifuse_debug = 0;
	}
}
#endif
@
