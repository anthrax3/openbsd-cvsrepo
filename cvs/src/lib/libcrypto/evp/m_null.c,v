#include <stdio.h>

#include <openssl/evp.h>
#include <openssl/objects.h>
#include <openssl/x509.h>

static int
init(EVP_MD_CTX *ctx)
{
	return 1;
}

static int
update(EVP_MD_CTX *ctx, const void *data, size_t count)
{
	return 1;
}

static int
final(EVP_MD_CTX *ctx, unsigned char *md)
{
	return 1;
}

static const EVP_MD null_md = {
	.type = NID_undef,
	.pkey_type = NID_undef,
	.md_size = 0,
	.flags = 0,
	.init = init,
	.update = update,
	.final = final,
	.copy = NULL,
	.cleanup = NULL,
	.sign = NULL,
	.verify = NULL,
	.required_pkey_type = {
		0, 0, 0, 0,
	},
	.block_size = 0,
	.ctx_size = sizeof(EVP_MD *),
};

const EVP_MD *
EVP_md_null(void)
{
	return (&null_md);
}
