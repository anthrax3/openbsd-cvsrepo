#include <stdio.h>

#include <openssl/pkcs12.h>

PKCS8_PRIV_KEY_INFO *
PKCS8_decrypt(X509_SIG *p8, const char *pass, int passlen)
{
	return PKCS12_item_decrypt_d2i(p8->algor,
	    &PKCS8_PRIV_KEY_INFO_it, pass, passlen, p8->digest, 1);
}
@


