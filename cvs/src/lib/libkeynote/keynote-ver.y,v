%union {
	struct s {
		char   *string;
	} s;
};
%type <s.string> STRING VSTRING
%token STRING VSTRING EQ
%nonassoc EQ
%start program
%{

#include <sys/types.h>

#include <regex.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#include "header.h"
#include "keynote.h"
%}
%%

program: expr
       | STRING              { if (kn_add_authorizer(sessid, $1) != 0)
				 return keynote_errno;
                               free($1);
                             }

expr: VSTRING EQ STRING      { int i = kn_add_action(sessid, $1, $3, 0);

                               if (i != 0)
				 return i;
			       free($1);
			       free($3);
                             }
    | VSTRING EQ STRING      { int i = kn_add_action(sessid, $1, $3, 0);

                               if (i != 0)
				 return i;
			       free($1);
			       free($3);
                             } expr 
%%
void
kverror(char *s)
{}
@


