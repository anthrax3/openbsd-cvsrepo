1.14
log
@fairly simple unsigned char casts for ctype
ok krw
@
text
@/* OpenBSD S/Key (put.c)
 *
 * Authors:
 *          Neil M. Haller <nmh@@thumper.bellcore.com>
 *          Philip R. Karn <karn@@chicago.qualcomm.com>
 *          John S. Walden <jsw@@thumper.bellcore.com>
 *          Scott Chasin <chasin@@crimelab.com>
 *
 * Dictionary lookup and extraction.
 *
 * $OpenBSD: put.c,v 1.13 2003/04/03 17:48:50 millert Exp $
 */

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <assert.h>
#include <ctype.h>

#include "skey.h"

static unsigned int extract(char *, int, int);
static void standard(char *);
static void insert(char *, int, int, int);
static int wsrch(char *, int, int);

