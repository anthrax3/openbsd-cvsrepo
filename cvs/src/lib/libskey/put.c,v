a430 21
#ifdef	notdef
/* Encode 8 bytes in 'cp' as stream of ascii letters.
 * Provided as a possible alternative to btoe()
 */
char *
btoc(cp)
	char *cp;
{
	int i;
	static char out[31];

	/* code out put by characters 6 bits each added to 0x21 (!) */
	for (i = 0; i <= 10; i++) {
		/* last one is only 4 bits not 6 */
		out[i] = '!' + extract (cp, 6 * i, i >= 10 ? 4 : 6);
	}
	out[i] = '\0';
	return(out);
}
#endif

