#ifndef lint
static char rcsid[] = "$OpenBSD: tgoto.c,v 1.2 1996/06/16 08:56:56 tholo Exp $";
#endif

#include <stdlib.h>
#include <string.h>
#include "term.h"

char *
tgoto(CM, col, line)
     const char *CM;
     int col, line;
{
    return tparm(CM, line, col);
}
@


