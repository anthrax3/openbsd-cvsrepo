1.5
log
@Add a port of FreeBSD/alpha's libio as libalpha. This library enable
access to devices I/O ports from userland. Only BWX machines supported
for now. Added a man page.
Ok deraadt@@, miod@@.
@
text
@d2 1
a2 1
# $OpenBSD$
d4 12
a17 1

d19 5
a24 22
MAN=		libalpha.2

MLINKS+=	libalpha.2 dense_base.2 \
		libalpha.2 inb.2 \
		libalpha.2 inl.2 \
		libalpha.2 inw.2 \
		libalpha.2 ioperm.2 \
		libalpha.2 map_memory.2 \
		libalpha.2 outb.2 \
		libalpha.2 outl.2 \
		libalpha.2 outw.2 \
		libalpha.2 readb.2 \
		libalpha.2 readl.2 \
		libalpha.2 readw.2 \
		libalpha.2 unmap_memory.2 \
		libalpha.2 writeb.2 \
		libalpha.2 writel.2 \
		libalpha.2 writew.2

MANSUBDIR=alpha

.include <bsd.lib.mk>
