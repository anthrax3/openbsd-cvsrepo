1.4
log
@remove leftover NOLINT, WANTLINT, LINTFLAGS, LOBJ vars and lint targets.

ok guenther@@
@
text
@#	$OpenBSD: Makefile.inc,v 1.3 2005/11/24 20:49:14 deraadt Exp $
#	$NetBSD: Makefile.inc,v 1.1 1996/02/21 02:43:57 jtk Exp $
#
# This Makefile includes boiler-plate stuff included by each subdir's Makefile.

OBJS+= ${ASM}
POBJS+=	${ASM:.o=.po}
CLEANFILES+= ${ASM} ${POBJS}

LIB=    ${MACHINE_ARCH}
@


1.3
log
@use WANTLINT= (on all architectures)
@
text
@d1 1
a1 1
#	$OpenBSD: Makefile.inc,v 1.2 2004/02/26 00:10:47 deraadt Exp $
a10 1
WANTLINT=
@


1.2
log
@correct MACHINE vs MACHINE_ARCH issues
@
text
@d1 1
a1 1
#	$OpenBSD: Makefile.inc,v 1.1 1996/03/19 23:14:52 niklas Exp $
d11 1
@


1.1
log
@From NetBSD: merge of 960317
@
text
@d1 1
a1 1
#	$OpenBSD$
a5 6
.if exists(${.CURDIR}/../${MACHINE_ARCH}) && (${MACHINE} != ${MACHINE_ARCH})
.PATH:	${.CURDIR}/../${MACHINE_ARCH}
.include "${.CURDIR}/../${MACHINE_ARCH}/Makefile.inc"
AINC+= -I${.CURDIR}/../${MACHINE_ARCH}
.endif

d10 1
a10 1
LIB=    ${MACHINE}
@

