#include <sys/types.h>
#include <machine/signal.h>
#include <machine/segments.h>
#include <machine/sysarch.h>
#include <machine/vm86.h>

int
i386_vm86(struct vm86_struct *vmcp)
{

	return sysarch(I386_VM86, vmcp);
}

