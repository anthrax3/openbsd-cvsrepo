#include <sys/cdefs.h>
#include <sys/types.h>
#include <machine/segments.h>
#include <machine/sysarch.h>

int
amd64_set_mtrr(struct mtrr *mtrr, int *n)
{
	struct amd64_set_mtrr_args a;

	a.mtrrp = mtrr;
	a.n = n;
	return sysarch(AMD64_SET_MTRR, (void *)&a);
}
@


1.1
log
@mtrr calls
@
