#include <sys/types.h>

#include <machine/segments.h>
#include <machine/sysarch.h>

int
amd64_set_ioperm(iomap)
	u_long *iomap;
{
	struct amd64_set_ioperm_args p;

	p.iomap = iomap;

	/* LINTED pointer casts may be troublesome */
	return sysarch(AMD64_SET_IOPERM, (char *)&p);
}
