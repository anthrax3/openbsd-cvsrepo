#include <sys/types.h>

#include <machine/segments.h>
#include <machine/sysarch.h>

int
amd64_iopl(int iopl)
{
	struct amd64_iopl_args p;

	p.iopl = iopl;

	return sysarch(AMD64_IOPL, &p);
}
@


