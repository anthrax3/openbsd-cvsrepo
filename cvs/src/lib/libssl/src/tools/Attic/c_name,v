1.3
log
@Remove unused/never installed libssl tools and docs and references to them

Sure deraadt
@
text
@#!/bin/sh
#
# print the subject
#

for i in $*
do
	n=`openssl x509 -subject -noout -in $i`
	echo "$i	$n"
done
@


