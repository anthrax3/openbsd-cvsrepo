#include <stdio.h>

#include <openssl/asn1.h>
#include <openssl/err.h>
#include <openssl/evp.h>
#include <openssl/x509.h>

X509_INFO *
X509_INFO_new(void)
{
	X509_INFO *ret = NULL;

	ret = malloc(sizeof(X509_INFO));
	if (ret == NULL) {
		ASN1err(ASN1_F_X509_INFO_NEW, ERR_R_MALLOC_FAILURE);
		return (NULL);
	}

	ret->enc_cipher.cipher = NULL;
	ret->enc_len = 0;
	ret->enc_data = NULL;

	ret->references = 1;
	ret->x509 = NULL;
	ret->crl = NULL;
	ret->x_pkey = NULL;
	return (ret);
}

void
X509_INFO_free(X509_INFO *x)
{
	int i;

	if (x == NULL)
		return;

	i = CRYPTO_add(&x->references, -1, CRYPTO_LOCK_X509_INFO);
	if (i > 0)
		return;

	X509_free(x->x509);
	if (x->crl != NULL)
		X509_CRL_free(x->crl);
	if (x->x_pkey != NULL)
		X509_PKEY_free(x->x_pkey);
	free(x->enc_data);
	free(x);
}
