ASN1_SEQUENCE(PROXY_POLICY) = {
	ASN1_SIMPLE(PROXY_POLICY, policyLanguage, ASN1_OBJECT),
	ASN1_OPT(PROXY_POLICY, policy, ASN1_OCTET_STRING)
} ASN1_SEQUENCE_END(PROXY_POLICY)
d94 26
a119 5
ASN1_SEQUENCE(PROXY_CERT_INFO_EXTENSION) = {
