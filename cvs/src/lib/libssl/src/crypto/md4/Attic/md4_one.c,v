@a63 4
#ifdef CHARSET_EBCDIC
#include <openssl/ebcdic.h>
#endif

a71 1
#ifndef CHARSET_EBCDIC
a72 15
#else
	{
		char temp[1024];
		unsigned long chunk;

		while (n > 0)
		{
			chunk = (n > sizeof(temp)) ? sizeof(temp) : n;
			ebcdic2ascii(temp, d, chunk);
			MD4_Update(&c,temp,chunk);
			n -= chunk;
			d += chunk;
		}
	}
#endif
