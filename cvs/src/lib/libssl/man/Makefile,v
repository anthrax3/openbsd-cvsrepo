1.35
log
@after getting rid of the pod files, clean up the Makefiles; ok bcook@@
@
text
@d1 1
a1 1
# $OpenBSD: Makefile,v 1.34 2016/09/03 12:42:45 beck Exp $
d3 1
a3 1
.include <bsd.own.mk>		# for NOMAN
d5 1
a5 5
POD2MAN=pod2man --official --release="OpenBSD ${OSREV}" --center=OpenSSL

.ifndef NOMAN
MAN=	\
	BIO_f_ssl.3 \
d87 1
a87 1
	ssl.3 \
d89 1
a89 3
.include <bsd.man.mk>
.else
maninstall:
d91 1
a91 1
.endif
d93 1
a93 13
.PATH: ${.CURDIR}/../doc
.SUFFIXES: .pod .1 .3 .7
.for sect in 1 3 7
.pod.${sect}:
	@@echo '${POD2MAN} --section=${sect} --name=${*:U} $< > $@@'
	@@${POD2MAN} --section=${sect} --name=${*:U} $< > $@@.tmp && mv $@@.tmp $@@
.endfor

.include <bsd.obj.mk>
.include <bsd.subdir.mk>

clean cleandir:
	rm -f ${MAN}
@


