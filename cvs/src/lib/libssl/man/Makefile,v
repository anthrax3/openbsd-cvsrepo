BUF_MEM_new.cat3: buffer.pod
	( cp ${.ALLSRC} BUF_MEM_new.pm && pod2man --section=3 --official \
	  --center='OpenSSL' --release="OpenBSD `uname -r`" \
	  BUF_MEM_new.pm ) | nroff -Tascii -man > ${.TARGET}
.if MANPS
