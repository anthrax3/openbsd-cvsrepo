/*
 * PowerPC machine dependent routines for kvm.
 */

#include <sys/param.h>
#include <sys/exec.h>
#include <sys/proc.h>
#include <sys/stat.h>
#include <sys/core.h>
#include <sys/kcore.h>
#include <unistd.h>
#include <nlist.h>
#include <kvm.h>

#include <uvm/uvm_extern.h>

#include <stdlib.h>
#include <db.h>
#include <limits.h>

#include "kvm_private.h"

void
_kvm_freevtop(kvm_t *kd)
{
	free(kd->vmst);
	kd->vmst = NULL;
}

/*ARGSUSED*/
int
_kvm_initvtop(kvm_t *kd)
{

	return (0);
}

/*ARGSUSED*/
int
_kvm_kvatop(kvm_t *kd, u_long va, paddr_t *pa)
{
	_kvm_err(kd, 0, "vatop not yet implemented!");
	return (0);
}

/*ARGSUSED*/
off_t
_kvm_pa2off(kvm_t *kd, paddr_t pa)
{
	_kvm_err(kd, 0, "pa2off not yet implemented!");
	return (0);
}
