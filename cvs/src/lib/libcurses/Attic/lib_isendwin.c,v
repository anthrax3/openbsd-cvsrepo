/*
**	lib_endwin.c
**
**	The routine endwin().
**
*/

#include <curses.priv.h>

MODULE_ID("$From: lib_isendwin.c,v 1.4 1998/04/11 22:52:32 tom Exp $")

bool isendwin(void)
{
	if (SP == NULL)
		return FALSE;
	return SP->_endwin;
}
