/*
 *	nl.c
 *
 *	Routines:
 *		nl()
 *		nonl()
 *
 */

#include <curses.priv.h>

MODULE_ID("$Id: lib_nl.c,v 1.8 2000/12/10 02:43:27 tom Exp $")

#ifdef __EMX__
#include <io.h>
#endif

NCURSES_EXPORT(int)
nl(void)
{
    T((T_CALLED("nl()")));

    SP->_nl = TRUE;

#ifdef __EMX__
    _nc_flush();
    _fsetmode(NC_OUTPUT, "t");
#endif

    returnCode(OK);
}

NCURSES_EXPORT(int)
nonl(void)
{
    T((T_CALLED("nonl()")));

    SP->_nl = FALSE;

#ifdef __EMX__
    _nc_flush();
    _fsetmode(NC_OUTPUT, "b");
#endif

    returnCode(OK);
}
@


