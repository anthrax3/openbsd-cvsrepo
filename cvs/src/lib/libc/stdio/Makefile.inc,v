	getc.c getchar.c getw.c makebuf.c mktemp.c open_memstream.c \
	open_wmemstream.c perror.c printf.c putc.c putchar.c puts.c putw.c \
	refill.c remove.c rewind.c rget.c scanf.c setbuf.c setbuffer.c \
