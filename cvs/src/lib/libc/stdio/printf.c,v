1.3
log
@#if __STDC__ --> #ifdef __STDC__
@
text
@d38 1
a38 1
static char rcsid[] = "$OpenBSD: printf.c,v 1.2 1996/08/19 08:32:57 tholo Exp $";
a41 1
#ifdef __STDC__
a42 3
#else
#include <varargs.h>
#endif
a44 1
#ifdef __STDC__
a45 5
#else
printf(fmt, va_alist)
	char *fmt;
	va_dcl
#endif
a49 1
#ifdef __STDC__
a50 3
#else
	va_start(ap);
#endif
