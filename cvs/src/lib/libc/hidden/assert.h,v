/* outside the conditional for changes in NDEBUG */
#include_next <assert.h>

#ifndef _LIBC_ASSERT_H_
#define _LIBC_ASSERT_H_

PROTO_STD_DEPRECATED(__assert);
PROTO_NORMAL(__assert2);

#endif /* _LIBC_ASSERT_H_ */
