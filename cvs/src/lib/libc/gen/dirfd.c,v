1.3
log
@Wrap <dirent.h> so that internal calls go direct and they're all weak symbols
@
text
@/*
 * Written by Matthew Dempsky, 2011.
 * Public domain.
 */

#include <dirent.h>
#include "telldir.h"

int
dirfd(DIR *dirp)
{
	return (dirp->dd_fd);
}
DEF_WEAK(dirfd);
@


