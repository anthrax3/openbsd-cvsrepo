#if 0	/* XXX */ 
int
__isnanl(long double e)
{
	struct ieee_ext *p = (struct ieee_ext *)&e;

	p->ext_frach &= ~0x80000000;	/* clear sign bit */

	return (p->ext_exp == EXT_EXP_INFNAN &&
	    (p->ext_frach != 0 || p->ext_fracl != 0));
}
