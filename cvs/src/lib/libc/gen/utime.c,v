#include <sys/time.h>

#include <utime.h>

int
utime(const char *path, const struct utimbuf *times)
{
	struct timeval tv[2], *tvp;

	if (times == (struct utimbuf *) NULL)
		tvp = NULL;
	else {
		tv[0].tv_sec = times->actime;
		tv[1].tv_sec = times->modtime;
		tv[0].tv_usec = tv[1].tv_usec = 0;
		tvp = tv;
	}
	return (utimes(path, tvp));
}
@


