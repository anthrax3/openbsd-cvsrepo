#include <sys/types.h>
#include <sys/sysctl.h>
#include <unistd.h>

int
getdomainname(char *name, size_t namelen)
{
	int mib[2];
	size_t size;

	mib[0] = CTL_KERN;
	mib[1] = KERN_DOMAINNAME;
	size = namelen;
	if (sysctl(mib, 2, name, &size, NULL, 0) == -1)
		return (-1);
	return (0);
}
DEF_WEAK(getdomainname);
@


