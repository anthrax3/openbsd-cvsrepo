#include <sys/types.h>
#include <sys/sysctl.h>
#include <unistd.h>

int
getpagesize(void)
{
	if (_pagesize == 0) {
		int mib[2];
		size_t size;

		mib[0] = CTL_HW;
		mib[1] = HW_PAGESIZE;
		size = sizeof _pagesize;
		if (sysctl(mib, 2, &_pagesize, &size, NULL, 0) == -1)
			return (-1);
	}
	return (_pagesize);
}
DEF_WEAK(getpagesize);
