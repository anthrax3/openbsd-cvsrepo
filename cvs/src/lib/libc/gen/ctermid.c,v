#include <stdio.h>
#include <paths.h>
#include <string.h>

char *
ctermid(char *s)
{
	static char def[] = _PATH_TTY;

	if (s) {
		bcopy(def, s, sizeof(_PATH_TTY));
		return(s);
	}
	return(def);
}
DEF_WEAK(ctermid);
@


