#include <sys/time.h>
#include <time.h>

time_t
time(time_t *t)
{
	struct timeval tt;

	if (gettimeofday(&tt, NULL) < 0)
		return (-1);
	if (t)
		*t = (time_t)tt.tv_sec;
	return (tt.tv_sec);
}
DEF_STRONG(time);
@


