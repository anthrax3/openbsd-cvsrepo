#include <wctype.h>
#include "rune.h"
#include "rune_local.h"

_RuneType
___runetype_mb(wint_t c)
{
	rune_t c0;
	uint32_t x;
	_RuneRange *rr;
	_RuneEntry *base, *re;

	if (c == WEOF)
		return (0U);

	c0 = (rune_t)c; /* XXX assumes wint_t = int */
	rr = &_CurrentRuneLocale->rl_runetype_ext;
	base = rr->rr_rune_ranges;
	for (x = rr->rr_nranges; x; x >>= 1) {
		re = base + (x >> 1);
		if (re->re_min <= c0 && re->re_max >= c0) {
			if (re->re_rune_types)
				return (re->re_rune_types[c0 - re->re_min]);
			else
				return (re->re_map);
		} else if (c0 > re->re_max) {
			base = re + 1;
			x--;
		}
	}

	return (0U);
}
@


