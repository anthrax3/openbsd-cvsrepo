			void (*std_func)(void);
			void (*cxa_func)(void *);
		} fn_ptr;
@


1.7
log
@Add __cxa_atexit() support for gcc3.  This provides support for shared object destructors called at dlclose() time.  Inspired by similar changes in FreeBSD and NetBSD.
@
text
@d1 1
