#include <sys/param.h>
#include <netinet/in.h>
#include <arpa/nameser.h>
#include <netdb.h>
#include <resolv.h>

#include "thread_private.h"

void
sethostent(int stayopen)
{
	struct __res_state *_resp = _THREAD_PRIVATE(_res, _res, &_res);

	if (_res_init(0) == -1)
		return;
	if (stayopen)
		_resp->options |= RES_STAYOPEN | RES_USEVC;
}

void
endhostent(void)
{
	struct __res_state *_resp = _THREAD_PRIVATE(_res, _res, &_res);

	_resp->options &= ~(RES_STAYOPEN | RES_USEVC);
	res_close();
}
