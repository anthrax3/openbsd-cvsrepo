#include <sys/types.h>
#include <rpc/rpc.h>
#include <rpc/xdr.h>
#include <rpcsvc/yp.h>

bool_t
xdr_ypstat(XDR *xdrs, ypstat *objp)
{
	return xdr_enum(xdrs, (enum_t *)objp);
}
@


