@
text
@d1 1
a1 1
/*	$OpenBSD$	*/
a27 5

#if defined(LIBC_SCCS)
	.text
	.asciz "$OpenBSD: swab.S$"
#endif /* LIBC_SCCS */
@


1.3
log
@Fix RCS ids
Make sure everything uses {SYS,}LIBC_SCCS properly
@
text
@d1 26
d31 1
a31 1
	.asciz "$OpenBSD$"
d35 13
a47 5
	movl	sp@@(4),a0		| source
	movl	sp@@(8),a1		| destination
	movl	sp@@(12),d0		| count
	lsrl	#1,d0			| count is in bytes; we need words
	beq	swdone
a48 9
swloop:
	movw	a0@@+,d1
	rorw	#8,d1
	movw	d1,a1@@+
	subql	#1,d0
	bne	swloop

swdone:
	rts
@


