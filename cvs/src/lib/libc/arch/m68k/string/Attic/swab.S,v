@


1.5
log
@zap rcsid.
okay deraadt@@ (tested them all)
@
text
@d1 1
a1 1
/*	$OpenBSD: swab.S,v 1.4 2002/06/18 08:34:46 espie Exp $	*/
d30 2
a31 2
	movl	sp@@(12),d1	| #bytes
	lsrl	#1,d1		| -> #words
d33 3
a35 3
	subql	#1,d1		| for proper dbf behaviour
	movl	sp@@(4),a0	| src
	movl	sp@@(8),a1	| and dst
d37 4
a40 4
	movw	a0@@+,d0
	rorw	#8,d0
	movw	d0,a1@@+
	dbf	d1,swab_loop
@


