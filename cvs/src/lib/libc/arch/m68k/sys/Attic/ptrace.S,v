#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: ptrace.S,v 1.2 1996/08/19 08:15:38 tholo Exp $"
#endif /* SYSLIBC_SCCS */

#include "SYS.h"

	.globl	_errno

ENTRY(ptrace)
#ifdef	PIC
	movl	#__GLOBAL_OFFSET_TABLE_,a0
	lea	pc@@(0,a0:l),a0
	movl	a0@@(_errno:w),a0
	clrl	a0@@
#else
	clrl	_errno
#endif
	movl	#SYS_ptrace,d0
	trap	#0
	jcs	err
	rts
err:
	jra	cerror
@


