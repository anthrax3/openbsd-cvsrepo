#include "SYS.h"

	.globl	_end
	.globl	_C_LABEL(__minbrk)
	.globl	_C_LABEL(__curbrk)

	.data
_C_LABEL(__minbrk):
	.long	_end
	.text

ENTRY(brk)
#ifdef	__PIC__
	lea	%pc@@(_GLOBAL_OFFSET_TABLE_@@GOTPC),%a1
	movl	%a1@@(_C_LABEL(__minbrk)@@GOT:w),%a0
	movl	%a0@@,%d0
#else
	movl	_C_LABEL(__minbrk),%d0
#endif
	cmpl	%sp@@(4),%d0
	jls	ok
	movl	%d0,%sp@@(4)
ok:
	movl	#SYS_break,%d0
	trap	#0
	jcs	err
#ifdef	__PIC__
	movl	%a1@@(_C_LABEL(__curbrk)@@GOT:w),%a0
	movl	%sp@@(4),%a0@@
#else
	movl	%sp@@(4),_C_LABEL(__curbrk)
#endif
	clrl	%d0
	rts
err:
	jra	PIC_PLT(__cerror)
@


1.8
log
@Userland bits for m68k/ELF. Mostly addition of register prefixes to the
assembler instructions, and cope with the few changes in return values
location.
@
text
