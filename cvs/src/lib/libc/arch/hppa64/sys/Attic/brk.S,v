#include "SYS.h"

	.import	__curbrk, data
	.import	end, data

	.data
	.export	__minbrk, data
__minbrk
	.long	end

	.weak	brk

LEAF_ENTRY(brk)
#ifdef __PIC__
	addil	LT%__minbrk, %r19
	ldd	RT%__minbrk(%r1), arg4
	ldd	0(arg4), arg5
#else
	ldil	L%__minbrk, arg4
	ldd	R%__minbrk(arg4), arg5
#endif
	sub,<<	arg5, %arg0, %r0
	copy	arg5, %arg0
	std	%arg0, HPPA_FRAME_ARG(0)(%sp)

	SYSCALL(break)

	ldd	HPPA_FRAME_ARG(0)(%sp), %arg0
#ifdef __PIC__
	addil	LT%__curbrk, %r19
	ldd	RT%__curbrk(%r1), arg4
	bv	%r0(%rp)
	std     %arg0, 0(arg4)
#else
	ldil	L%__curbrk, arg4
	bv	%r0(%rp)
	std	%arg0, R%__curbrk(arg4)
#endif
EXIT(brk)

	.end

@


