#include <machine/asm.h>
/* $FreeBSD: pipe.S,v 1.4 2003/05/16 21:26:39 marcel Exp $ */

#include "SYS.h"

#ifdef WEAK_ALIAS
WEAK_ALIAS(pipe, _pipe)
#endif
/* XXX:	 Relook  param passing	*/
	
ENTRY(_pipe, 1)
	st8		[sp]=r32
	CALLSYS_ERROR(pipe)
	ld8		r14=[sp]
	;;
	st4		[r14]=ret0,4
	;;
	st4		[r14]=ret1
	mov		ret0=0
	br.ret.sptk.few rp
END(_pipe)
@


