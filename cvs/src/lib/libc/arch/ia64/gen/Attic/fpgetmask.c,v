/* __FBSDID("$FreeBSD: src/lib/libc/ia64/gen/fpgetmask.c,v 1.4 2003/10/22 09:00:07 marcel Exp $"); */

#include <sys/types.h>
#include <ieeefp.h>

/* XXX: Move to sysarchs() */
fp_except
fpgetmask(void)
{
	uint64_t fpsr;

	__asm volatile("mov %0=ar.fpsr" : "=r" (fpsr));
	return (~fpsr & 0x3d);
}
@


