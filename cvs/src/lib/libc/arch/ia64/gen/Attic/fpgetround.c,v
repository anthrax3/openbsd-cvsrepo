#include <sys/types.h>
#include <ieeefp.h>

fp_rnd
fpgetround(void)
{
	uint64_t fpsr;

	__asm volatile("mov %0=ar.fpsr" : "=r"(fpsr));
	return ((fp_rnd)((fpsr >> 10) & 3));
}
@


