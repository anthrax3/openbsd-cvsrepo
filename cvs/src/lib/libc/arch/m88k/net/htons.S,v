	.text
	/*.asciz "from: @@(#)htons.s	5.1 (Berkeley) 5/12/90"*/
	.asciz "$OpenBSD: htons.S,v 1.1 1998/12/15 07:10:32 smurph Exp $"
#endif /* LIBC_SCCS and not lint */
a48 2
#include "DEFS.h"

