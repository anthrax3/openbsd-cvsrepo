	ld	r5,r5,lo16(curbrk)
d65 7
a71 7
	br 	cerror
	/* Save old curbrk */
	or.u	r5,r0,hi16(curbrk)
	ld	r3,r5,lo16(curbrk)
	/* Update curbrk */
	st	r4,r5,lo16(curbrk)
	/* and return old curbrk */
