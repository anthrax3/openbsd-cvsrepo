#include "DEFS.h"

	.globl	_HIDDEN(__errno)

/*
 * long long __cerror(register_t errcode)
 * stores `errcode' into the current thread's errno variable, and returns
 * -1LL.
 */

ASENTRY(__cerror)
	subu	%r4,%r27,8
	subu	%r3,%r0,1
	st	%r2,%r4,%r0
	jmp.n	%r1
	 or	%r2, %r0, %r3
END(__cerror)

#if 0
/* ahh, if only */
ASENTRY(__errno)
	jmp.n	%r1
	 subu	%r2,%r27,8
END(__errno)
#endif
@


1.13
log
@Use a Thread Information Block in both single and multi-threaded programs.
This stores errno, the cancelation flags, and related bits for each thread
and is allocated by ld.so or libc.a.  This is an ABI break from 5.9-stable!

Make libpthread dlopen'able by moving the cancelation wrappers into libc
and doing locking and fork/errno handling via callbacks that libpthread
registers when it first initializes.  'errno' *must* be declared via
<errno.h> now!

Clean up libpthread's symbol exports like libc.

On powerpc, offset the TIB/TCB/TLS data from the register per the ELF spec.

Testing by various, particularly sthen@@ and patrick@@
ok kettenis@@
@
text
@d1 1
a1 1
/*	$OpenBSD: cerror.S,v 1.12 2013/01/11 21:23:24 miod Exp $	*/
d38 1
a38 1
	.globl	_C_LABEL(__errno)
d47 6
a52 2
	/* if %r26 has been initialized, don't bother setting up a frame */
	bcnd	ne0, %r26, has_r26
d54 6
a59 8
	subu	%r31,%r31,16
	st	%r25,%r0,%r31
	st	%r1,%r31,4

#ifdef __PIC__
	bsr.n	_C_LABEL(__errno)#plt
#else
	bsr.n	_C_LABEL(__errno)
a60 14
	 or	%r25,%r0,%r2
	or	%r26,%r0,%r2	/* save errno ptr */
	or	%r2,%r0,%r25	/* and restore errno value */

	ld	%r1,%r31,4
	ld	%r25,%r0,%r31
	addu	%r31,%r31,16

has_r26:
	st	%r2,%r0,%r26
	subu	%r2,%r0,1
	jmp.n	%r1
	 or	%r3, %r0, %r2
END(__cerror)
@


1.12
log
@Add END() directives to the various functions.
Make the code PIC-aware when necessary (i.e. invoke other functions through
the PLT, and access global data through the GOT). No change for non-PIC
compilation.
@
text
@d1 1
a1 1
/*	$OpenBSD: cerror.S,v 1.11 2013/01/08 23:06:43 miod Exp $	*/
d36 1
a36 1
#include "SYS.h"
d38 1
a38 1
WEAK_ALIAS(__cerror,___cerror)
d40 13
a52 6
	.globl	_C_LABEL(errno)
ENTRY(___cerror)
#ifdef __PIC__
	PIC_SAVE(%r9)
	PIC_SETUP
#endif
d55 1
a55 1
	PIC_STORE(%r2,_C_LABEL(errno))
d57 1
a57 6
	or.u	%r4,%r0,%hi16(_C_LABEL(errno))
	st	%r2,%r4,%lo16(_C_LABEL(errno))
#endif

#ifdef __PIC__
	PIC_RESTORE(%r9)
d59 11
a69 1
	sub	%r2,%r0,0x1
d71 2
a72 2
	 sub	%r3,%r0,0x1
END(___cerror)
@


