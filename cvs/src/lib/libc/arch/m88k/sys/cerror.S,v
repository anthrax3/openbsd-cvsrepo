ok miod
@
text
@d1 1
a1 1
/*	$OpenBSD: cerror.S,v 1.8 2011/04/04 12:42:39 guenther Exp $	*/
d38 1
a38 1
WEAK_ALIAS(_cerror,__cerror)
d40 1
a40 1
	.globl	_errno
d42 5
a46 5
	or.u	r4,r0,hi16(_errno)
	st	r2,r4,lo16(_errno)
	sub	r2,r0,0x1
	jmp.n	r1
	 sub	r3,r0,0x1
@


1.8
