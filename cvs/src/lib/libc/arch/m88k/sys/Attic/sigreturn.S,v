/*	$OpenBSD: sigreturn.S,v 1.4 2003/01/07 22:01:35 miod Exp $	*/
a34 5

#if defined(LIBC_SCCS)
	.data
	.string "$OpenBSD: sigreturn.S,v 1.4 2003/01/07 22:01:35 miod Exp $"
#endif /* LIBC_SCCS */
@


