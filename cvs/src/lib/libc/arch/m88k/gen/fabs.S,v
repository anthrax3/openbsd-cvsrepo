	subu	r31,r31,16
	st.d	r2,r31,0
	ld.bu	r4,r31,0
	mask	r4,r4,0x7f	/* set sign bit to 0 */
	st.b	r4,r31,0
	ld.d	r2,r31,0
	jmp.n	r1
	 addu	r31,r31,16
