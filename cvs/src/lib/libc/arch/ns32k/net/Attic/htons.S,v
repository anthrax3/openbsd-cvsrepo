#include <machine/asm.h>

#if defined(LIBC_SCCS)
	.text
	.asciz "$OpenBSD: htons.S,v 1.2 1996/08/19 08:16:56 tholo Exp $"
#endif /* LIBC_SCCS */

	.text
ENTRY(htons)
	movzwd	S_ARG0,r0
	rotw	8,r0
	ret	0
@


