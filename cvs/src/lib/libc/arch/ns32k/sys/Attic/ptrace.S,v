#include "SYS.h"

#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: ptrace.S,v 1.2 1996/08/19 08:17:10 tholo Exp $"
#endif /* SYSLIBC_SCCS */

	.globl	_errno
	.globl	cerror

ENTRY(ptrace)
	PIC_PROLOGUE
	movqd	0, PIC_GOT(_errno)
	PIC_EPILOGUE
	movd	SYS_ptrace, r0
	SVC
	bcs	cerror
	ret	0
@


1.2
log
@Fix RCS ids
Make sure everything uses {SYS,}LIBC_SCCS properly
@
text
@d21 1
a21 1
	.asciz "$OpenBSD$"
@


