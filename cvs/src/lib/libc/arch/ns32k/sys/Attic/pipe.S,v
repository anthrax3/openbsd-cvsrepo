#include <SYS.h>

#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: pipe.S,v 1.2 1996/08/19 08:17:09 tholo Exp $"
#endif /* SYSLIBC_SCCS */

SYSCALL(pipe)
	movd	S_ARG0, r2
	movd	r0, 0(r2)
	movd	r1, 4(r2)
	movqd	0, r0
	ret	0
@


1.2
log
@Fix RCS ids
Make sure everything uses {SYS,}LIBC_SCCS properly
@
text
@d21 1
a21 1
	.asciz "$OpenBSD$"
@


