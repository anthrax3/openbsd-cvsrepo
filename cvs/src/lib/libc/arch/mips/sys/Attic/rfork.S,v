#include "SYS.h"

#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: rfork.S,v 1.2 2003/06/02 20:18:32 millert Exp $"
#endif /* SYSLIBC_SCCS */

SYSLEAF(rfork)
	.set	reorder
	li	v0, SYS_rfork	# pid = fork()
	syscall
	bne	a3, zero, 2f
	beq	v1, zero, 1f	# v1 == 0 in parent, 1 in child
	move	v0, zero
1:
	j	ra
2:
	la	t9, _C_LABEL(cerror)
	jr	t9
SYSEND(rfork)
@


