#include "SYS.h"

#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: pipe.S,v 1.5 1999/01/19 01:38:53 d Exp $"
#endif /* SYSLIBC_SCCS */

SYSLEAF(pipe)
	.set	reorder
	li	v0, SYS_pipe	# pipe(fildes) int fildes[2];
	syscall
	bne	a3, zero, 1f
	sw	v0, 0(a0)	# store the two file descriptors
	sw	v1, 4(a0)
	move	v0, zero
	j	ra
1:
	la	t9, _C_LABEL(cerror)
	jr	t9
SYSEND(pipe)
@


