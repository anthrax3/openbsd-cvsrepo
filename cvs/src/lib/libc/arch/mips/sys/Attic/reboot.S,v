	ASMSTR("$Id: reboot.S,v 1.1.1.1 1995/10/18 08:41:35 deraadt Exp $")
a44 3
#ifdef ABICALLS
	.abicalls
#endif
a47 3
	.set	noreorder
	.cpload t9
	.set	reorder
d51 1
@


