#include "SYS.h"

#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: reboot.S,v 1.4 1996/08/19 08:16:24 tholo Exp $"
#endif /* SYSLIBC_SCCS */

LEAF(reboot)
#ifdef ABICALLS
	subu	sp, sp, 32
	.cprestore 16
#endif
	.set	reorder
	li	v0, SYS_reboot
	syscall
	bne	a3, zero, 1f
	jal	_C_LABEL(abort)
1:
	la	t9, _C_LABEL(cerror)
	jr	t9
END(reboot)
@


1.4
log
@Fix RCS ids
Make sure everything uses {SYS,}LIBC_SCCS properly
@
text
@d41 1
a41 1
	.asciz "$OpenBSD$"
@


1.3
log
@Cleanup of abi call and endian stuff. Also fixed some bugs. (AENT).
@
text
@a0 1
/*	$OpenBSD$	*/
d39 4
a42 5
#if defined(LIBC_SCCS) && !defined(lint)
	ASMSTR("from: @@(#)reboot.s	8.1 (Berkeley) 6/4/93")
	ASMSTR("$Id: reboot.S,v 1.2 1996/05/16 11:16:10 pefo Exp $")
#endif /* LIBC_SCCS and not lint */

@


