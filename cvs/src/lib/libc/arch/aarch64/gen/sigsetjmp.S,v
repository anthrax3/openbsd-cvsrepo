#include "DEFS.h"
#include <machine/setjmp.h>

ENTRY(sigsetjmp)
	cmp	x1, #0
	b.eq	_HIDDEN(_setjmp)
	b	_HIDDEN(setjmp)
END(sigsetjmp)

ENTRY(siglongjmp)
	/* Load the _setjmp magic */
	ldr	x2, .Lmagic
	ldr	x3, [x0]

	/* Check the magic */
	cmp	x2, x3
	b.eq	_HIDDEN(_longjmp)
	b	_HIDDEN(longjmp)
.Lmagic:
	.align	3
	.quad	_JB_MAGIC__SETJMP
END(siglongjmp)
@


