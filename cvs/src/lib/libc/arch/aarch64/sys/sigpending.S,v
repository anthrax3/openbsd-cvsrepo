#include "SYS.h"

SYSENTRY(sigpending)
	mov	x2, x0
	SYSTRAP(sigpending)
	bcs	CERROR
	str	w0, [x2]
	mov	x0, #0
	ret
SYSCALL_END(sigpending)
@


