/* int sigsuspend(int *mask) */

#include "SYS.h"

	.text
PREFIX_HIDDEN(sigsuspend)
	lwz 3, 0(3)		/* load the mask */
	sc
	PSEUDO_SUFFIX
SYSCALL_END_HIDDEN(sigsuspend)
@


