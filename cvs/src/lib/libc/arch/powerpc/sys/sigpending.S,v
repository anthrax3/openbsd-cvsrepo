/* sigpending(sigset_t *set); */

#include "SYS.h"

	.text
PREFIX(sigpending)
	mr	%r5, %r3
	sc
	stw	%r3, 0(%r5)
	li	%r3, 0
	blr
SYSCALL_END(sigpending)
@


