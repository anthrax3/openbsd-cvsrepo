#if defined(SYSLIBC_SCCS) 
	.text
	.asciz "$OpenBSD: pipe.S,v 1.3 1999/01/22 06:07:56 marc Exp $"
#endif /* SYSLIBC_SCCS */

#include "SYS.h"

SYSENTRY(pipe)
	mov	%o0, %o2	! save pointer
	mov	SYS_pipe, %g1
	t	ST_SYSCALL	! pipe()
	bcc,a	1f
	 st	%o0, [%o2]	! success, store fds
	ERROR()
1:
	st	%o1, [%o2 + 4]
	retl			! and return 0
	 clr	%o0
@


