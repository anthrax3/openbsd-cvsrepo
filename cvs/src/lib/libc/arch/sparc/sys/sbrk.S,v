#include "SYS.h"

	.globl	_end
	.globl	__curbrk
	.hidden	__curbrk

	.data
__curbrk:	.long	_end
	END(__curbrk)
	.type	__curbrk,@@object

	.text

	.weak	sbrk

ENTRY(sbrk)
#ifdef __PIC__
	PIC_PROLOGUE(%o5,%o4)
#if __PIC__ == 1
	ld	[%o5 + __curbrk], %o2
#else /* __PIC__ == 2 */
	sethi   %hi(__curbrk), %o2
	or      %o2, %lo(__curbrk), %o2
	ld	[%o5 + %o2], %o2
#endif
	ld	[%o2], %o3			! %o3 = old break
	add	%o3, %o0, %o4			! %o4 = new break
	mov	%o4, %o0			! copy for syscall
	mov	SYS_break, %g1
	t	ST_SYSCALL			! break(new_break)
	bcc,a	1f				! if success,
	 mov	%o3, %o0			!    set return value
	ERROR()
1:
	retl					! and update curbrk
	 st	%o4, [%o2]
#else
	sethi	%hi(__curbrk), %o2
	ld	[%o2 + %lo(__curbrk)], %o3	! %o3 = old break
	add	%o3, %o0, %o4			! %o4 = new break
	mov	%o4, %o0			! copy for syscall
	mov	SYS_break, %g1
	t	ST_SYSCALL			! break(new_break)
	bcc,a	1f				! if success,
	 mov	%o3, %o0			!    set return value
	ERROR()
1:
	retl					! and update curbrk
	 st	%o4, [%o2 + %lo(__curbrk)]
#endif
END(sbrk)
@


