/*	$OpenBSD: sigsetjmp.S,v 1.2 2001/03/29 01:43:51 mickey Exp $	*/
d7 1
a7 1
	.asciz "$OpenBSD: sigsetjmp.S,v 1.2 2001/03/29 01:43:51 mickey Exp $"
@


1.2
log
@fix fork, brk/sbrk; rcsid embedding
@
