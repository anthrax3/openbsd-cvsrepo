#include <machine/asm.h>

#include "SYS.h"

	.globl	__curbrk
	.globl	_C_LABEL(_end)

	.weak	brk

	.data
	.align	8
__minbrk:
	.xword	_C_LABEL(_end)		/* lower brk limit; also for gmon code */
	END(__minbrk)
	OTYPE(__minbrk)
	.text

ENTRY(brk)
#ifdef __PIC__
	PIC_PROLOGUE(%o5,%o4)
	set	__minbrk, %o4
	ldx	[%o5 + %o4], %o4
	ldx	[%o4], %o1		/* %o1 = minbrk */
	cmp	%o1, %o0		/* if (minbrk > %o0) */
	movgu	%xcc, %o1, %o0		/*	%o0 = minbrk */
	mov	%o0, %o2		/* save argument to syscall */
	mov	SYS_break, %g1
	t	ST_SYSCALL
	set	__curbrk, %o3
	bcc,a,pt	%icc, 1f
	 ldx	[%o5 + %o3], %o4
	ERROR()
1:
	retl				/* success, return 0 & record new break */
	 stx	%o2, [%o4]
#else
	sethi	%hi(__minbrk), %o1	/* %o1 = minbrk */
	ldx	[%o1 + %lo(__minbrk)], %o1
	cmp	%o1, %o0		/* if (minbrk > %o0) */
	movgu	%xcc, %o1, %o0		/*	%o0 = minbrk */
	mov	%o0, %o2		/* save argument to syscall */
	mov	SYS_break, %g1
	t	ST_SYSCALL
	bcc,a,pt	%icc,1f
	 sethi	%hi(__curbrk), %g1
	ERROR()
1:
	retl				/* success, return 0 & record new break */
	 stx	%o2, [%g1 + %lo(__curbrk)]
#endif
END(brk)
@


