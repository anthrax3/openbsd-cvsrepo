text
@d1 1
a1 1
/*	$OpenBSD$	*/
d36 1
a36 1
#include <machine/asm.h>
d51 2
a52 2
	beq	PIC_SYM(_C_LABEL(_setjmp), PLT)
	b	PIC_SYM(_C_LABEL(setjmp), PLT)
d61 2
a62 2
	beq	PIC_SYM(_C_LABEL(_longjmp), PLT)
	b	PIC_SYM(_C_LABEL(longjmp), PLT)
@


