#include <sys/cdefs.h>

#include "namespace.h"

#include <ieeefp.h>
#ifdef SOFTFLOAT_FOR_GCC
#include "softfloat-for-gcc.h"
#endif
#include "milieu.h"
#include "softfloat.h"

#ifdef __weak_alias
__weak_alias(_fpsetmask,fpsetmask);
#endif

fp_except
fpsetmask(fp_except mask)
{
	fp_except old;

	old = float_exception_mask;
	float_exception_mask = mask;
	return old;
}
