@
text
@d1 1
a1 1
/*	$OpenBSD: cerror.S,v 1.1 2004/01/28 01:44:45 mickey Exp $	*/
d50 2
a51 2
	movq    PIC_GOT(_C_LABEL(errno)), %rcx
	movq    %rax, (%rcx)
d53 1
a53 1
	movq    %rax, _C_LABEL(errno)(%rip)
@


