#include <machine/asm.h>

#include "SYS.h"

	.globl	_C_LABEL(errno)

_ENTRY(CERROR)
#ifdef PIC
	movq	PIC_GOT(_C_LABEL(errno)), %rcx
	movl	%eax, (%rcx)
#else
	movl	%eax, _C_LABEL(errno)(%rip)
#endif /* PIC */
	movq	$-1,%rax
	ret
@


