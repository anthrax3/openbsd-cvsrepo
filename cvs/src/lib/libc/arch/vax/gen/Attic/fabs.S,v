/* fabs - floating absolute value */

#include "DEFS.h"

STRONG_ALIAS(fabsl,fabs)
ENTRY(fabs, 0)
	movd	4(%ap),%r0
	bgeq	1f
	mnegd	%r0,%r0
1:
	ret
@


