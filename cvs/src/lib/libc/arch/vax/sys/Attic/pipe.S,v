#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: pipe.S,v 1.2 1996/08/19 08:19:01 tholo Exp $"
#endif /* SYSLIBC_SCCS */

#include "SYS.h"

SYSCALL(pipe)
	movl	4(ap),r2
	movl	r0,(r2)+
	movl	r1,(r2)
	clrl	r0
	ret
@


