#include "SYS.h"

	.data
__minbrk:
	.long	_end
	END(__minbrk)
	.type	__minbrk,@@object

	.text

	.globl	__curbrk

	.weak	brk

ENTRY(brk, 0)
	cmpl	__minbrk,4(%ap)
	blequ	1f
	movl	__minbrk,4(%ap)
1:
	chmk	$ SYS_break
	jcs	2f
	movl	4(%ap),__curbrk
	clrl	%r0
	ret
2:
	jmp	_C_LABEL(__cerror)
END(brk)
@


