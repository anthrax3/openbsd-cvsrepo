#include "SYS.h"

/*
 * We must preserve the state of the registers as the user has set them up.
 */
#ifdef PROF
#undef ENTRY
#define	ENTRY(x, y)							\
	_ENTRY(_C_LABEL(x), 0);						\
	pushr $0x3f;							\
	_PROF_PROLOGUE;							\
	popr $0x3f
#endif /* PROF */

SYSCALL(sigreturn)
	ret
SYSCALL_END(sigreturn)
@


