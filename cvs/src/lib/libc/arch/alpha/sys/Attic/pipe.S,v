#include "SYS.h"

#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: pipe.S,v 1.4 1999/02/04 06:22:35 d Exp $"
#endif /* SYSLIBC_SCCS */

SYSCALL(pipe)
	stl	v0, 0(a0)
	stl	a4, 4(a0)
	mov	zero, v0
	RET
SYSEND(pipe)
@


