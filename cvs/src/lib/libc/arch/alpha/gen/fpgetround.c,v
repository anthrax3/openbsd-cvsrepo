#include <sys/types.h>
#include <ieeefp.h>

fp_rnd
fpgetround(void)
{
	double fpcrval;
	u_int64_t old;

	__asm__("trapb");
	__asm__("mf_fpcr %0" : "=f" (fpcrval));
	__asm__("trapb");
	old = *(u_int64_t *)&fpcrval;

	return ((old >> 58) & 0x3);
}
DEF_WEAK(fpgetround);
@


