#include <math.h>

/* bytes for +Infinity on an Alpha (IEEE double format) */
char __infinity[] __attribute__((__aligned__(sizeof(double)))) =
					{ 0, 0, 0, 0, 0, 0, 0xf0, 0x7f };
@


