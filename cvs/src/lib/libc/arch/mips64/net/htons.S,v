#include <machine/asm.h>

/*
 * netorder = htons(hostorder)
 * hostorder = ntohs(netorder)
 */
ALEAF(ntohs)
NLEAF(htons, 0)
	.set	reorder
#ifdef __MIPSEL__
	srl	v0, a0, 8
	and	v0, v0, 0xff
	sll	v1, a0, 8
	and	v1, v1, 0xff00
	or	v0, v0, v1
#else
#ifdef __MIPSEB__
	move	v0, a0
#else
	ERROR
#endif
#endif
	j	ra
END(htons)
@


