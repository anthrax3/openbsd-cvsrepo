#include "SYS.h"

#if defined(SYSLIBC_SCCS)
	.text
	.asciz "$OpenBSD: pipe.S,v 1.3 1996/08/19 08:11:56 tholo Exp $"
#endif /* SYSLIB_SCCS */

	.text
	.align	0

ENTRY(pipe)
	mov	r2, r0
	swi	SYS_pipe
	bcs	cerror
	str	r0, [r2, #0x0000]
	str	r1, [r2, #0x0004]
	mov	r0, #0x00000000
	mov	r15, r14
@


1.3
log
@Fix RCS ids
Make sure everything uses {SYS,}LIBC_SCCS properly
@
text
@d38 1
a38 1
	.asciz "$OpenBSD$"
@


1.2
log
@sync to mark
@
text
@a0 2
/* $NetBSD: pipe.S,v 1.2 1996/05/12 19:55:16 mark Exp $ */

a31 3
 *
 *	from: @@(#)pipe.s	5.1 (Berkeley) 4/23/90
 *	$Id: pipe.S,v 1.1 1996/05/01 12:51:29 deraadt Exp $
d35 5
@


