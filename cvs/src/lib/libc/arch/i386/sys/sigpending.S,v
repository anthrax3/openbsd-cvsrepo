#include "SYS.h"

SYSCALL(sigpending)
	movl	4(%esp),%ecx		# fetch pointer to...
	movl	%eax,(%ecx)		# store old mask
	xorl	%eax,%eax
	ret
SYSCALL_END(sigpending)
@


