#include "SYS.h"

/*
 * We must preserve the state of the registers as the user has set them up.
 */
#ifdef PROF
#undef _PROF_PROLOGUE
#define _PROF_PROLOGUE \
	pusha; \
	.data; 1:; .long 0; .text; movl $1b,%eax; call mcount; popa ; nop ;
#endif /* PROF */

SYSCALL(sigreturn)
	ret
SYSCALL_END(sigreturn)
@


