#include <math.h>

/*
 * ldexp(value, exp): return value * (2 ** exp).
 */

double
ldexp(double value, int exp)
{
	double temp;
	__asm ("fscale"
		: "=t" (temp)
		: "0" (value), "u" ((double)exp));
	return (temp);
}
DEF_STRONG(ldexp);
@


