#include <machine/asm.h>
d45 11
a55 9
	PIC_PROLOGUE
	pushl	$0
#ifdef __PIC__
	call	PIC_PLT(_C_LABEL(_libc_sigblock))
#else
	call	_C_LABEL(_libc_sigblock)
#endif
	addl	$4,%esp
	PIC_EPILOGUE
a56 4
	movl	4(%esp),%ecx
	movl	%eax,24(%ecx)
1:	movl	0(%esp),%edx
	movl	%edx, 0(%ecx)
d58 6
