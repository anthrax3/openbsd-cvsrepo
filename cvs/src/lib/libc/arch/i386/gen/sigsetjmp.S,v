#else
	call	_C_LABEL(_libc_sigsetmask)
#endif
	addl	$4,%esp
	PIC_EPILOGUE

1:	movl	4(%esp),%edx
	movl	8(%esp),%eax
	movl	0(%edx),%ecx
	movl	4(%edx),%ebx
