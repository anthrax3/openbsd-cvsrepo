#include "SYS.h"

	.weak	brk

	.data
	.align	2
__minbrk:
	.long	_C_LABEL(_end)
	.size	__minbrk, . - __minbrk
	.type	__minbrk,@@object

	.text

ENTRY(brk)
#ifdef __PIC__
	mova	L_GOT, r0
	mov.l	L_GOT, r2
	add	r0, r2
	mov.l	Lminbrk, r0
	mov.l	@@(r0, r2), r0
	mov.l	@@r0, r0
#else
	mov.l	Lminbrk, r0
	mov.l	@@r0, r0
#endif
	cmp/hs	r4, r0
	bf	1f
	mov	r0, r4
1:
	mov.l	LSYS_break, r0
	trapa	#0x80
	bf	2f
#ifdef __PIC__
	mov.l	Lcurbrk, r0
	mov.l	@@(r0, r2), r1
	xor	r0, r0
#else
	xor	r0, r0
	mov.l	Lcurbrk, r1
#endif
	rts
	 mov.l	r4, @@r1
2:
	SET_ERRNO_AND_RETURN

	.align	2
LSYS_break:	.long	SYS_break
#ifdef __PIC__
L_GOT:		.long	_GLOBAL_OFFSET_TABLE_
Lminbrk:	.long	__minbrk@@GOT
Lcurbrk:	.long	__curbrk@@GOT
#else
Lminbrk:	.long	__minbrk
Lcurbrk:	.long	__curbrk
#endif
	SET_ENTRY_SIZE(brk)
@


