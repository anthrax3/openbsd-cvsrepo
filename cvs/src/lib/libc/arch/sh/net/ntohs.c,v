#include <sys/types.h>
#include <endian.h>

#undef ntohs

/* hostorder = ntohs(netorder) */
u_int16_t
ntohs(x)
	u_int16_t x;
{
#if BYTE_ORDER == LITTLE_ENDIAN
	__asm("swap.b %1, %0" : "=r" (x) :  "r" (x));
#endif
	return x;
}
@


