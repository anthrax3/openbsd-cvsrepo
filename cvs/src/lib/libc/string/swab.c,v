a16 1

d20 1
a20 1
swab(const void *from, void *to, size_t len)
d22 3
a24 3
	size_t n;
	char *fp, *tp;
	char temp;
d26 8
a33 17
	n = (len / 2) + 1;
	fp = (char *)from;
	tp = (char *)to;
#define	STEP do {	\
	temp = *fp++;	\
	*tp++ = *fp++;	\
	*tp++ = temp;	\
} while (0)
	/* round to multiple of 8 */
	while ((--n) & 07)
		STEP;
	n >>= 3;
	if (n == 0)
		return;
	while (n-- != 0) {
		STEP; STEP; STEP; STEP;
		STEP; STEP; STEP; STEP;
@


