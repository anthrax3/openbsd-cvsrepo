#include <wchar.h>
#include "locale/runetype.h"

/*
 * Compare strings.
 */
int
wcscmp(const wchar_t *s1, const wchar_t *s2)
{

	while (*s1 == *s2++)
		if (*s1++ == 0)
			return (0);
	/* XXX assumes wchar_t = int */
	return (*(const rune_t *)s1 - *(const rune_t *)--s2);
}
DEF_STRONG(wcscmp);
@


