#include <sys/types.h>
#include <signal.h>
#include <errno.h>

/*
 * Backwards-compatible killpg().
 */
int
killpg(pid_t pgid, int sig)
{
	if (pgid < 0 || pgid == 1) {
		errno = ESRCH;
		return (-1);
	}
	return (kill(-pgid, sig));
}
@


