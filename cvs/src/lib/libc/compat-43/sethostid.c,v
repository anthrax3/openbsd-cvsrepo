#include <sys/types.h>
#include <sys/sysctl.h>
#include <unistd.h>

int
sethostid(long hostid)
{
	int mib[2];

	mib[0] = CTL_KERN;
	mib[1] = KERN_HOSTID;
	if (sysctl(mib, 2, NULL, NULL, &hostid, sizeof hostid) == -1)
		return (-1);
	return (0);
}
@


