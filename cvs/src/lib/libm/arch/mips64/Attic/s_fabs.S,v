#include <machine/asm.h>

/*
 * fabs(x)
 *	double x;
 *
 * Return absolute value of x.
 */
LEAF(fabs, 0)
	.set	noreorder
	j	ra
	abs.d	$f0, $f12		# compute absolute value of x
END(fabs)
@


