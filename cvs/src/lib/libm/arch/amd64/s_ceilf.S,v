1.3
log
@Add missing $OpenBSD$ tags.
Zap wasteful RCSID("$NetBSD$")

okay deraadt@@
@
text
@d1 1
a1 1
/*	$OpenBSD$ */
a9 19
#ifdef __i386__
	pushl	%ebp
	movl	%esp,%ebp
	subl	$8,%esp

	fstcw	-4(%ebp)		/* store fpu control word */
	movw	-4(%ebp),%dx
	orw	$0x0800,%dx		/* round towards +oo */
	andw	$0xfbff,%dx
	movw	%dx,-8(%ebp)
	fldcw	-8(%ebp)		/* load modfied control word */

	flds	8(%ebp);		/* round */
	frndint

	fldcw	-4(%ebp)		/* restore original control word */

	leave
#else
a21 1
#endif
@


