#include <complex.h>
#include <math.h>

double complex
cproj(double complex z)
{
	double complex res;

	if (isinf(__real__ z) || isinf(__imag__ z)) {
		__real__ res = INFINITY;
		__imag__ res = copysign(0.0, __imag__ z);
	} else {
		res = z;
	}

	return res;
}
@


