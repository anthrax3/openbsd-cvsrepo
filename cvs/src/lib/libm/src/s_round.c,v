#include <float.h>
#include <math.h>

#include "math_private.h"

double
round(double x)
{
	double t;

	if (isinf(x) || isnan(x))
		return (x);

	if (x >= 0.0) {
		t = floor(x);
		if (t - x <= -0.5)
			t += 1.0;
		return (t);
	} else {
		t = floor(-x);
		if (t + x <= -0.5)
			t += 1.0;
		return (-t);
	}
}
DEF_STD(round);
LDBL_MAYBE_CLONE(round);
@


