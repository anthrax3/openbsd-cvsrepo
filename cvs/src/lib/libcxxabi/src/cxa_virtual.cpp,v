extern "C" {
_LIBCXXABI_FUNC_VIS LIBCXXABI_NORETURN
void __cxa_pure_virtual(void) {
  abort_message("Pure virtual function called!");
}

_LIBCXXABI_FUNC_VIS LIBCXXABI_NORETURN
void __cxa_deleted_virtual(void) {
  abort_message("Deleted virtual function called!");
}
} // extern "C"
} // abi
