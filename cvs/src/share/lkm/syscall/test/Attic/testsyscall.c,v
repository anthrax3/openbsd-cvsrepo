#include <stdio.h>

int
main(argc, argv)
	int argc;
	char* argv[];
{
	char buf[80];
	int err = 0;

	printf("Table offset as reported by modstat: ");
	if (fgets(buf, sizeof(buf), stdin) == NULL) {
		printf("[ABORT]\n");
		exit(1);
	}

	if(err = syscall(atoi(buf) /* no arguments*/))
		perror("syscall");

	exit(err);
}
@


