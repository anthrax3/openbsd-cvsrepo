FILES=		VPN-* policy singlehost-*
TARGETDIR=	/usr/share/ipsec/isakmpd

# The mkdir below is for installation on OpenBSD pre 2.7
install:
	@@-mkdir -p ${DESTDIR}${TARGETDIR}
	$(INSTALL) -c -m 0444 ${FILES} ${DESTDIR}${TARGETDIR}
@


