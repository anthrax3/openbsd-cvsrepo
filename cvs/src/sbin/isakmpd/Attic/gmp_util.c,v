#include <sys/param.h>

#include "sysdep.h"

#include "gmp_util.h"
#include "math_mp.h"

/* Various utility functions for gmp, used in more than one module */

u_int32_t
mpz_sizeinoctets(math_mp_t a)
{
	return BN_num_bytes(a);
}

void
mpz_getraw(u_int8_t *raw, math_mp_t v, u_int32_t len)
{
	math_mp_t       a;

	/* XXX bn2bin?  */
	a = BN_dup(v);

	while (len-- > 0)
	raw[len] = BN_div_word(a, 256);

	BN_clear_free(a);
}

void
mpz_setraw(math_mp_t d, u_int8_t *s, u_int32_t l)
{
	u_int32_t       i;

	/* XXX bin2bn?  */
	BN_set_word(d, 0);
	for (i = 0; i < l; i++) {
		BN_mul_word(d, 256);
		BN_add_word(d, s[i]);
	}
}
@


