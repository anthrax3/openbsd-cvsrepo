#ifndef _IKE_AUTH_H_
#define _IKE_AUTH_H_

#include <sys/types.h>

struct exchange;

struct ike_auth {
	u_int16_t       id;
	u_int8_t       *(*gen_skeyid) (struct exchange *, size_t *);
	int             (*decode_hash) (struct message *);
	int             (*encode_hash) (struct message *);
};

extern struct ike_auth *ike_auth_get(u_int16_t);

#endif				/* _IKE_AUTH_H_ */
@


