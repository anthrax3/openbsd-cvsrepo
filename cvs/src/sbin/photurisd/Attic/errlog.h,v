#ifndef _ERRLOG_H_
#define _ERRLOG_H_

#undef EXTERN
#ifdef _ERRLOG_C_
#define EXTERN
#else
#define EXTERN extern
#endif

#ifdef __STDC__
EXTERN void crit_error __P((int, char *, ...));
EXTERN void log_error __P((int, char *, ...));
#else
EXTERN void crit_error();
EXTERN void log_error();
#endif

#endif /* _ERRLOG_H_ */
