#include <sys/types.h>
#include <sys/disklabel.h>
#include <stdio.h>
#include "extern.h"

u_short
dkcksum(struct disklabel *lp)
{
	u_short *start, *end;
	u_short sum = 0;

	start = (u_short *)lp;
	end = (u_short *)&lp->d_partitions[lp->d_npartitions];
	while (start < end)
		sum ^= *start++;
	return (sum);
}
@


