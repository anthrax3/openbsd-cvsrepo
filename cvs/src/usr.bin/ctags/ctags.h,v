@/*	$OpenBSD: ctags.h,v 1.8 2014/12/10 19:44:21 tobias Exp $	*/
/*	$NetBSD: ctags.h,v 1.3 1995/03/26 20:14:07 glass Exp $	*/

/*
 * Copyright (c) 1987, 1993, 1994
 *	The Regents of the University of California.  All rights reserved.
