struct kinfo_proc;
void	fmt_puts(const char *, int *);
void	fmt_putc(int, int *);
void	pr_attime(time_t *, time_t *);
void	pr_idle(time_t);
int	proc_compare(const struct kinfo_proc *, const struct kinfo_proc *);

#define SECSPERHOUR	(60 * 60)
#define SECSPERDAY	(24 * 60 * 60)
