	OPT_NEVER_INTERACTIVE,
	OPT_NO_ALIGN,
        OPT_NO_ANSI_FUNC_DEFS,
        OPT_NO_ANSI_FUNC_PROTOS,
	OPT_NO_DEBUG,
	OPT_NO_DEFAULT,
	OPT_NO_ECS,
