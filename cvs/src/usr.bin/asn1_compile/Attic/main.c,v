#include "gen_locl.h"
#include <getarg.h>

/*
RCSID("$KTH: main.c,v 1.12 2005/03/31 00:37:42 lha Exp $");
*/

extern FILE *yyin;

int version_flag;
int help_flag;
struct getargs args[] = {
    { "version", 0, arg_flag, &version_flag },
    { "help", 0, arg_flag, &help_flag }
};
int num_args = sizeof(args) / sizeof(args[0]);

static void
usage(int code)
{
    arg_printusage(args, num_args, NULL, "[asn1-file [name]]");
    exit(code);
}

int
main(int argc, char **argv)
{
    int ret;
    const char *file;
    const char *name = NULL;
    int optind = 0;

    if(getarg(args, num_args, argc, argv, &optind))
	usage(1);
    if(help_flag)
	usage(0);
    if(version_flag) {
#if 0
	print_version(NULL);
#else
	printf("asn1_compile from heimdal-0.7\n");
#endif
	exit(0);
    }
    if (argc == optind) {
	file = "stdin";
	name = "stdin";
	yyin = stdin;
    } else {
	file = argv[optind];
	yyin = fopen (file, "r");
	if (yyin == NULL)
	    err (1, "open %s", file);
	name = argv[optind + 1];
    }

    init_generate (file, name);
    initsym ();
    ret = yyparse ();
    close_generate ();
    return ret;
}
