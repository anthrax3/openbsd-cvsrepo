text
@d1 1
a1 1
#	$OpenBSD$
d34 7
a40 2
# Sinclair QL libraries (ThMO)
0	beshort	=0xFB01		QL library archive data
d42 1
a42 47
# Sinclair QL executbables (ThMO)
6	beshort	=0x4AFB		QL executable,
>10	string	=\0		un-named
>8	beshort	=1
>>10	string	>\0		named: %.1s
>8	beshort	=2
>>10	string	>\0		named: %.2s
