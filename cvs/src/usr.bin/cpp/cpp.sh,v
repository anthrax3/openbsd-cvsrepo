#!/bin/sh
#	$OpenBSD: cpp.sh,v 1.7 2004/02/10 02:02:22 espie Exp $
d46 2
a47 3
DOLLAR="@@dollaropt@@"
OPTS=""
INCS="-nostdinc"
d74 8
d83 7
a89 1
		INCS="$INCS $A"
d93 1
a93 4
		;;
	-imacros|-include|-idirafter|-iprefix|-iwithprefix)
		INCS="$INCS '$A' '$1'"
		shift
d96 1
a96 1
		OPTS="$OPTS '$A'"
d100 2
a101 1
		eval $CPP $TRAD $DGNUC $DOLLAR $INCS $STDINC $OPTS $A || exit $?
d109 1
