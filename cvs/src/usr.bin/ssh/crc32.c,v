u_int32_t
ssh_crc32(const u_char *buf, u_int32_t size)
{
	u_int32_t i, crc;

	crc = 0;
	for (i = 0; i < size; i++)
		crc = crc32tab[(crc ^ buf[i]) & 0xff] ^ (crc >> 8);
	return crc;
}
