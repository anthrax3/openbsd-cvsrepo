a108 1
unsigned int crc32(const unsigned char *s, unsigned int len)
d110 8
a117 11
  unsigned int i;
  unsigned int crc32val;
  
  crc32val = 0;
  for (i = 0;  i < len;  i ++)
    {
      crc32val =
	crc32_tab[(crc32val ^ s[i]) & 0xff] ^
	  (crc32val >> 8);
    }
  return crc32val;
@


