	0x2d02ef8dL
d97 2
a98 4
/* Return a 32-bit CRC of the contents of the buffer. */

u_int
ssh_crc32(const u_char *s, u_int len)
d100 1
a100 2
	u_int i;
	u_int crc32val;
d102 4
a105 5
	crc32val = 0;
	for (i = 0;  i < len;  i ++) {
		crc32val = crc32_tab[(crc32val ^ s[i]) & 0xff] ^ (crc32val >> 8);
	}
	return crc32val;
@


