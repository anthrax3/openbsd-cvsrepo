/* Remote sftp filename globbing */

#ifndef _SFTP_GLOB_H
#define _SFTP_GLOB_H

#include "sftp-client.h"

int remote_glob(struct sftp_conn *, const char *, int,
    int (*)(const char *, int), glob_t *);

#endif
@


