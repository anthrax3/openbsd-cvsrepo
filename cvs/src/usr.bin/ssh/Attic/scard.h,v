#ifndef SCARD_H
#define SCARD_H

#define SCARD_ERROR_FAIL	-1
#define SCARD_ERROR_NOCARD	-2
#define SCARD_ERROR_APPLET	-3

Key	**sc_get_keys(const char *, const char *);
void	 sc_close(void);
int	 sc_put_key(Key *, const char *);
char	*sc_get_key_label(Key *);

#endif
@


