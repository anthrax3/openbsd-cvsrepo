a51 1
#ifdef __STDC__
a52 4
#else
quit (va_alist)
va_dcl
#endif
d55 1
a55 1
#ifdef __STDC__
a56 9
#else
	int status;
	char *fmt;

	va_start(args);
	status = va_arg(args, int);
	fmt = va_arg(args, char *);
#endif

