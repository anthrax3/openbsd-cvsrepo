#ifndef CVSZLIB_H
#define CVSZLIB_H

#include <sys/param.h>

#include <zlib.h>

#include "buf.h"

typedef struct cvs_zlib_ctx CVSZCTX;


CVSZCTX	*cvs_zlib_newctx(int);
void	 cvs_zlib_free(CVSZCTX *);
int	 cvs_zlib_deflate(CVSZCTX *, BUF *, u_char *, size_t);
int	 cvs_zlib_inflate(CVSZCTX *, BUF *, u_char *, size_t);

#endif	/* CVSZLIB_H */
