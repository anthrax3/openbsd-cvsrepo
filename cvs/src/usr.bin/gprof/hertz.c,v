#include <sys/time.h>
#include <sys/sysctl.h>

#include "gprof.h"

/*
 * Return the tick frequency on the machine or 0 if we can't find out.
 */

int
hertz(void)
{
	struct clockinfo cinfo;
	int mib[2];
	size_t len;

	mib[0] = CTL_KERN;
	mib[1] = KERN_CLOCKRATE;
	len = sizeof(cinfo);
	if (sysctl(mib, 2, &cinfo, &len, NULL, 0) == -1)
		return (0);

	return (cinfo.hz);
}
@


