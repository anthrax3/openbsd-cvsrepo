#ifndef lint
#if 0
static char sccsid[] = "@@(#)wwprintf.c	8.1 (Berkeley) 6/6/93";
#else
static char rcsid[] = "$OpenBSD: wwprintf.c,v 1.7 2003/08/01 22:01:37 david Exp $";
#endif
#endif /* not lint */

#include "ww.h"
#include <stdarg.h>
#include <stdio.h>

wwprintf(struct ww *w, char *fmt, ...)
{
	char buf[1024];
	va_list ap;

	va_start(ap, fmt);
	/* buffer can overflow */
	(void) wwwrite(w, buf, vsnprintf(buf, sizeof(buf), fmt, ap));
	va_end(ap);
}
@


