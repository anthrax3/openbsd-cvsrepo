#ifndef lint
#if 0
static char sccsid[] = "@@(#)wwalloc.c	8.1 (Berkeley) 6/6/93";
#else
static char rcsid[] = "$OpenBSD: wwalloc.c,v 1.6 2003/06/03 02:56:23 millert Exp $";
#endif
#endif /* not lint */

#include <stdlib.h>
#include "ww.h"

char **
wwalloc(row, col, nrow, ncol, size)
	int row, col, nrow, ncol;
	size_t size;
{
	char *p, **pp;
	int i;

	/* fast, call malloc only once */
	pp = (char **)
		malloc(sizeof (char **) * nrow + size * nrow * ncol);
	if (pp == 0) {
		wwerrno = WWE_NOMEM;
		return 0;
	}
	p = (char *)&pp[nrow];
	col *= size;
	size /= sizeof (char);		/* paranoid */
	size *= ncol;
	for (i = 0; i < nrow; i++) {
		pp[i] = p - col;
		p += size;
	}
	return pp - row;
}

wwfree(p, row)
char **p;
{
	free((char *)(p + row));
}
@


