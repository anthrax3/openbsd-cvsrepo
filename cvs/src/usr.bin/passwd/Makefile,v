more comfortable.
Reminded by brad@@
@
text
@d1 1
a1 1
#	$OpenBSD: Makefile,v 1.36 2013/08/06 19:05:57 miod Exp $
a15 10
.endif

.include <bsd.own.mk>	# For Kerberos

.if (${KERBEROS5:L} == "yes")
SRCS+= krb5_passwd.c
CFLAGS+= -DKRB5
DPADD+=	${LIBKRB5} ${LIBASN1} ${LIBCRYPTO}
LDADD+=	-lkrb5 -lasn1 -lcrypto
LDADD+= -lwind -lroken -lcom_err -lheimbase
@


