d6 1
a6 1
SRCS=	local_passwd.c yp_passwd.c passwd.c getpwent.c \
a13 3
.if (${YP:L} == "yes")
CFLAGS+=-DYP -I${.CURDIR}/../../lib/libc/yp
.endif
@


