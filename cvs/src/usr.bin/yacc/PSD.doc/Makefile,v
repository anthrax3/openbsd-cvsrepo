1.2
log
@add paper.txt target;
also, this needs -ms macros, not -me; (though paper not installed yet)
@
text
@#	$OpenBSD: Makefile,v 1.1 2002/12/03 21:36:35 mickey Exp $
#	@@(#)Makefile	8.1 (Berkeley) 8/14/93

DIR=	psd/15.yacc
SRCS=	ss.. ss0 ss1 ss2 ss3 ss4 ss5 ss6 ss7 ss8 ss9 ssA ssB ssa ssb ssc ssd
MACROS=	-ms
REFER=	refer -e # -p /usr/share/dict/papers/Ind

paper.ps: ${SRCS}
	${REFER} ${SRCS} | ${ROFF} > ${.TARGET}

paper.txt: ${SRCS}
	${REFER} ${SRCS} | ${ROFF} -Tascii > ${.TARGET}

.include <bsd.doc.mk>
@


