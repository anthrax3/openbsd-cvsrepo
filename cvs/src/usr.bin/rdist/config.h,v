a43 12

#include OS_H

/*
 * Include system pathname header file.  Usually this is <paths.h>.  
 * If your system doesn't have such a file, use "mypaths.h"
 */
#if	defined(PATHS_H)
#	include PATHS_H
#else
#	include "mypaths.h"
#endif
