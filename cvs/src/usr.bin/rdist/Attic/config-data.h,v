1.9
log
@Assume POSIX: <dirent.h>, struct dirent
@
text
@d1 1
a1 1
/*	$OpenBSD: config-data.h,v 1.8 2014/07/05 06:18:58 guenther Exp $	*/
a50 10

/*
 * Set default write(2) return and amount types.
 */
#if	!defined(WRITE_RETURN_T)
#define		WRITE_RETURN_T		ssize_t	/* What write() returns */
#endif	/* WRITE_RETURN_T */
#if	!defined(WRITE_AMT_T)
#define		WRITE_AMT_T		size_t	/* Amount to write */
#endif	/* WRITE_AMT_T */
@


1.8
log
@Assume C89: stdargs, const, and setvbuf()
Assume POSIX: O_NONBLOCK and S_IS{DIR,REG,LNK}
@
text
@d1 1
a1 1
/*	$OpenBSD: config-data.h,v 1.7 2011/04/10 15:47:28 krw Exp $	*/
a50 15

/*
 * Directory information
 */
#if	DIR_TYPE == DIR_DIRECT
#include 	<sys/dir.h>
typedef 	struct direct		DIRENTRY;
#define 	D_NAMLEN(p)		((p)->d_namlen)
#endif	/* DIR_DIRECT */

#if	DIR_TYPE == DIR_DIRENT
#include 	<dirent.h>
typedef 	struct dirent		DIRENTRY;
#define 	D_NAMLEN(p)		(strlen((p)->d_name))
#endif	/* DIR_DIRENT */
@


