/*
 * $From: config-data.h,v 1.1.1.2 1996/02/15 21:52:02 christos Exp $
 * @@(#)configdata.h
 */

#ifndef __configdata_h__
#define __configdata_h__

/*
 * Configuration data
 */

/*
 * Define the read and write values for the file descriptor array
 * used by pipe().
 */
#define PIPE_READ		0
#define PIPE_WRITE		1

#endif	/* __configdata_h__ */
@


