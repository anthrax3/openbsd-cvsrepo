a65 14

/*
 * Set a default buffering type.
 */
#if	!defined(SETBUF_TYPE)
#define 	SETBUF_TYPE		SETBUF_SETLINEBUF
#endif	/* SETBUF_TYPE */

/*
 * Set a default get socket pair type.
 */
#if	!defined(SOCKPAIR_TYPE)
#define 	SOCKPAIR_TYPE		SOCKPAIR_SOCKETPAIR
#endif	/* SOCKPAIR_TYPE */
