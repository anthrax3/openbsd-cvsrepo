#define HAVE_OPIE 1
_ACEOF

		{ echo "$as_me:$LINENO: checking whether to try NRL OPIE authentication" >&5
echo $ECHO_N "checking whether to try NRL OPIE authentication... $ECHO_C" >&6; }
		{ echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6; }
		AUTH_REG="$AUTH_REG NRL_OPIE"
		;;
esac
fi



