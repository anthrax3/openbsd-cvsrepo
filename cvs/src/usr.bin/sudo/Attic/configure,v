  withval=$with_incpath; case $with_incpath in
    yes)	{ { echo "$as_me:$LINENO: error: \"must give --with-incpath an argument.\"" >&5
echo "$as_me: error: \"must give --with-incpath an argument.\"" >&2;}
   { (exit 1); exit 1; }; }
		;;
