  withval=$with_otp_only; case $with_otp_only in
    yes)	with_passwd="no"
		{ echo "$as_me:$LINENO: --with-otp-only option deprecated, treating as --without-passwd" >&5
echo "$as_me: --with-otp-only option deprecated, treating as --without-passwd" >&6;}
		;;
esac
fi



