	O_KEYTIME,
	O_LEFTRIGHT,
	O_LINES,
	O_LISP,
	O_LIST,
	O_LOCKFILES,
	O_MAGIC,
	O_MATCHTIME,
	O_MESG,
	O_MODELINE,
	O_NOPRINT,
	O_NUMBER,
	O_OCTAL,
	O_OPEN,
	O_OPTIMIZE,
	O_PARAGRAPHS,
	O_PATH,
	O_PRINT,
