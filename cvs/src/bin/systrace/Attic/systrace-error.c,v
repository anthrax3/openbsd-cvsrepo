#include "intercept.h"
#include "systrace.h"
#include "systrace-errno.h"

struct systrace_error {
	char *name;
	int errno;
} systrace_errors[] = {
	{ "EPERM", SYSTRACE_EPERM },
	{ "ENOENT", SYSTRACE_ENOENT },
