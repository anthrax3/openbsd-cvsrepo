	{ NULL, 0}
};

int
systrace_error_translate(char *name)
{
	struct systrace_error *error = systrace_errors;

	while (error->name != NULL) {
		if (!strcasecmp(error->name, name))
			return (error->errno);
		error++;
	}

	return (-1);
}
