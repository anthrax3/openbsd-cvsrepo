@
text
@d1 1
a1 1
#	$OpenBSD: Makefile,v 1.12 2013/12/23 23:00:38 tedu Exp $
d9 2
a10 4
	${BINDIR}/md5 ${BINDIR}/cksum \
	${BINDIR}/md5 ${BINDIR}/sum
MLINKS= cksum.1 sum.1
MLINKS+= md5.1 sha1.1
@


1.12
