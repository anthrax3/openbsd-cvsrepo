@Initial revision
@
text
@d1 1
a1 1
/*	$OpenBSD$	*/
a4 3
#ifdef HAVE_VALUES_H
# include <values.h>
#endif /* HAVE_VALUES_H */
a10 8
#ifndef BITSPERBYTE
# ifdef CHAR_BIT
#  define BITSPERBYTE	CHAR_BIT
# else
#  define BITSPERBYTE	8	/* probably true.. */
# endif
#endif

d12 1
a12 1
# define BITS(t)	(BITSPERBYTE * sizeof(t))
