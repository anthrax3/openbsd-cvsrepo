/*	$OpenBSD: utmp.h,v 1.4 2001/01/31 17:42:25 deraadt Exp $	*/
a65 16
	time_t	ut_time;
};

/*
 * These should not be used for writing out new data, for reference only.
 */
struct old_lastlog {
	time_t	ll_time;
	char	ll_line[8];
	char	ll_host[16];
};

struct old_utmp {
	char	ut_line[8];
	char	ut_name[8];
	char	ut_host[16];
@


