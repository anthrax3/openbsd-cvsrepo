1.2
log
@No longer needs to be setuid root.
@
text
@#	$OpenBSD: Makefile,v 1.1 2000/12/12 02:35:18 millert Exp $

PROG=	login_skey
MAN=	login_skey.8
DPADD=	${LIBSKEY}
LDADD=	-lskey
CFLAGS+=-Wall

BINOWN=	root
BINGRP=	auth
BINMODE=2555
BINDIR=	/usr/libexec/auth

.include <bsd.prog.mk>
@


