Closes PR 1246
@
text
@d1 1
a1 1
#	$OpenBSD: Makefile,v 1.17 2000/07/19 19:28:02 mickey Exp $
a21 14

.if (${SKEY:L} == "yes")
CFLAGS+=-DSKEY
LDADD+= -lskey
DPADD+= ${LIBSKEY}
.endif

.if (${KERBEROS:L} == "yes")
SRCS+= klogin.c
.PATH:	${.CURDIR}/../../usr.bin/login
CFLAGS+= -DKERBEROS
LDADD+= -lkafs -lkrb -ldes
DPADD+= ${LIBKRB} ${LIBKRB} ${LIBKAFS}
.endif
@


