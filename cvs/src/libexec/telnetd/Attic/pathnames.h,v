#if BSD > 43

# include <paths.h>

# ifndef _PATH_LOGIN
#  define	_PATH_LOGIN	"/usr/bin/login"
# endif

#else
 
# define	_PATH_TTY	"/dev/tty"
# ifndef _PATH_LOGIN
#  define	_PATH_LOGIN	"/bin/login"
# endif

#endif

#ifdef BFTPDAEMON
#define		BFTPPATH	"/usr/ucb/bftp"
#endif  /* BFTPDAEMON */
