1.2
log
@runlist.sh & list2sh.awk unification
@
text
@#	$OpenBSD: runlist.sh,v 1.1 1997/05/07 12:46:50 niklas Exp $

if [ "X$1" = "X-d" ]; then
	SHELLCMD=cat
	shift
else
	SHELLCMD="sh"
fi

( while [ "X$1" != "X" ]; do
	cat $1
	shift
done ) | awk -f ${TOPDIR}/list2sh.awk | ${SHELLCMD}
@


