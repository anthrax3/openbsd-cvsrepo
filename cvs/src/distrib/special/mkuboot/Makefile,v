@#	$OpenBSD: Makefile,v 1.3 2013/10/28 09:00:06 patrick Exp $

.PATH: ${.CURDIR}/../../../usr.sbin/mkuboot

.if ${MACHINE} == "armv7"

BINDIR=	/usr/sbin
PROG=	mkuboot
SRCS=	mkuboot.c copy_elf32.c copy_elf64.c
DPADD=	${LIBZ}
LDADD=	-lz
CFLAGS=	-DMACHINE_ARCH=\"${MACHINE_ARCH}\"

.else

NOPROG=yes

.endif

.include <bsd.prog.mk>
