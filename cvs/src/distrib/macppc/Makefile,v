1.4
log
@correct unconfig: target; Jean-Philippe Ouellet
@
text
@#	$OpenBSD: Makefile,v 1.3 2007/10/18 18:16:20 deraadt Exp $

SUBDIR= ramdisk

.if	make(obj) || make(cleandir) || make(clean)
SUBDIR+= iso
.endif

unconfig:
	cd ramdisk; ${MAKE} unconfig

.include <bsd.subdir.mk>
@


