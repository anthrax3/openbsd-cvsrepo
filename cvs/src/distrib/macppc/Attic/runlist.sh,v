1.2
log
@runlist.sh & list2sh.awk unification
@
text
@#	$OpenBSD: runlist.sh,v 1.1 2001/09/01 16:47:03 drahn Exp $

if [ "X$1" = "X-d" ]; then
	SHELLCMD=cat
	shift
else
	SHELLCMD="sh -e"
fi

( while [ "X$1" != "X" ]; do
	cat $1
	shift
done ) | awk -f ${TOPDIR}/list2sh.awk | ${SHELLCMD}
@


