1.2
log
@pmax bye bye bye
@
text
@#	$OpenBSD: runlist.sh,v 1.1 1998/11/09 06:16:06 millert Exp $

if [ "X$1" = "X-d" ]; then
	SHELLCMD=cat
	shift
else
	SHELLCMD="sh"
fi

( while [ "X$1" != "X" ]; do
	cat $1
	shift
done ) | awk -f ${TOPDIR}/list2sh.awk | ${SHELLCMD}
@


