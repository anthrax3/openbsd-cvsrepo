1.2
log
@ARC ramdisk fiddling
@
text
@#	$Id: runlist.sh,v 1.1 1996/10/10 10:05:50 deraadt Exp $

if [ "X$1" = "X-d" ]; then
	SHELLCMD=cat
	shift
else
	SHELLCMD="sh -e"
fi

( while [ "X$1" != "X" ]; do
	cat $1
	shift
done ) | awk -f ${TOPDIR}/list2sh.awk | ${SHELLCMD}
@


