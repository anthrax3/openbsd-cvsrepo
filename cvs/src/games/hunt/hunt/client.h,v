1.2
log
@avoid denial of service attacks on server port; extra conf vars; fix otto
@
text
@d1 1
a1 1
/*	$OpenBSD: client.h,v 1.1 1999/01/29 07:30:33 d Exp $	*/
d10 3
a12 3
void	bad_con __P((void));
void	bad_ver __P((void));
void	intr __P((int));
d15 1
a15 1
void	do_connect __P((char *, u_int8_t, u_int32_t));
d18 3
a20 3
void	playit __P((void));
void	do_message __P((void));
int	quit __P((int));
d24 2
a25 2
int	otto __P((int, int, char, char *, size_t));
int	otto_quit __P((int));
@


