extern char *malloc();
extern char *realloc();
d93 1
a93 2
alloc(lth)
register unsigned lth;
d95 1
a95 1
	register char *ptr;
a99 12
}

long *
enlarge(ptr,lth)
register char *ptr;
register unsigned lth;
{
	register char *nptr;

	if(!(nptr = realloc(ptr,lth)))
		panic("Cannot reallocate %d bytes", lth);
	return((long *) nptr);
@


