#include <stdio.h>
#include <string.h>

#include "trek.h"

/*
**  Announce Device Out
*/

void
out(int dev)
{
	const struct device	*d;

	d = &Device[dev];
	printf("%s reports %s ", d->person, d->name);
	if (d->name[0] != '\0' && d->name[strlen(d->name) - 1] == 's')
		printf("are");
	else
		printf("is");
	printf(" damaged\n");
}
