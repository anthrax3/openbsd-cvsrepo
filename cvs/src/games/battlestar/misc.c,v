#include "extern.h"

/* for beenthere, injuries */
int
card(const char *array, int size)
{
	const char   *end = array + size;
	int     i = 0;

	while (array < end)
		if (*array++)
			i++;
	return (i);
}

int
ucard(const unsigned int *array)
{
	int     j = 0, n;

	for (n = 0; n < NUMOFOBJECTS; n++)
		if (TestBit(array, n))
			j++;
	return (j);
}
@


