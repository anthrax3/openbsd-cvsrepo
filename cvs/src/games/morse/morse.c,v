		case '(':	case ')':
			show("-.--.-.");
			break;
		case '`': case '"':
			show(".-..-.");
			break;
		case '\'':
			show(".----.");
			break;
		case '+':   /* AR */
			show(".-.-.");
			break;
		case '=':   /* BT */
			show("-...-");
			break;
		case '@@':   /* SK */
			show("...-.-");
			break;
d254 3
d258 2
@


